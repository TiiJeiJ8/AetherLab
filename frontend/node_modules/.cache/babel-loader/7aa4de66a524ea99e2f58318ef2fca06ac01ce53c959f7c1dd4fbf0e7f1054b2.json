{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'Card',\n  props: {\n    icon: {\n      type: String,\n      required: true\n    },\n    title: {\n      type: String,\n      required: true\n    },\n    description: {\n      type: String,\n      required: true\n    },\n    buttonText: {\n      type: String,\n      required: true\n    },\n    href: {\n      type: String,\n      required: true\n    },\n    isDeveloping: {\n      type: Boolean,\n      default: true\n    },\n    isExternal: {\n      type: Boolean,\n      default: false\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    const props = __props;\n    const router = useRouter();\n    const buttonColor = computed(() => props.isDeveloping ? 'btn-secondary' : 'btn-primary');\n    // 支持自定义动画类名，默认fadeIn\n    const animationClass = computed(() => props.animationClass || 'animate__animated animate__fadeIn');\n    function handleClick() {\n      if (props.isExternal) {\n        window.open(props.href, '_blank');\n      } else if (!props.isDeveloping) {\n        router.push(props.href);\n      } else {\n        router.push('/under-construction');\n      }\n    }\n    const __returned__ = {\n      props,\n      router,\n      buttonColor,\n      animationClass,\n      handleClick,\n      computed,\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useRouter","props","__props","router","buttonColor","isDeveloping","animationClass","handleClick","isExternal","window","open","href","push"],"sources":["D:/Learning Material/Git/Fuck-Charts/frontend/src/components/Common/Card.vue"],"sourcesContent":["<!--todo Card-->\n<template>\n  <div\n    :class=\"['Main-body', 'border-0', 'shadow-sm', animationClass, 'hover-card']\"\n    style=\"z-index: var(--z-index-cards);\"\n    tabindex=\"0\"\n    @keydown.enter=\"handleClick\"\n  >\n    <div class=\"card-body text-center\">\n      <div class=\"text-primary mb-3\" style=\"font-size: 3rem; user-select: none;\">\n        {{ icon }}\n      </div>\n      <h4 class=\"card-title mb-3\">{{ title }}</h4>\n      <p class=\"text-muted\">{{ description }}</p>\n      <button\n        :class=\"['home-btn', 'mt-3', 'px-4', 'py-2', 'btn-animated', 'hover-button', buttonColor]\"\n        @click=\"handleClick\"\n        @keydown.enter.stop.prevent=\"handleClick\"\n        aria-label=\"action\"\n      >\n        {{ buttonText }}\n      </button>\n    </div>\n  </div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { computed, defineProps } from 'vue'\nimport { useRouter } from 'vue-router'\n\nconst props = defineProps({\n  icon: { type: String, required: true },\n  title: { type: String, required: true },\n  description: { type: String, required: true },\n  buttonText: { type: String, required: true },\n  href: { type: String, required: true },\n  isDeveloping: { type: Boolean, default: true },\n  isExternal: { type: Boolean, default: false }\n})\n\nconst router = useRouter()\n\nconst buttonColor = computed(() => props.isDeveloping ? 'btn-secondary' : 'btn-primary')\n// 支持自定义动画类名，默认fadeIn\nconst animationClass = computed(() => props.animationClass || 'animate__animated animate__fadeIn')\n\nfunction handleClick () {\n  if (props.isExternal) {\n    window.open(props.href, '_blank')\n  } else if (!props.isDeveloping) {\n    router.push(props.href)\n  } else {\n    router.push('/under-construction')\n  }\n}\n</script>\n\n<style scoped>\n.Main-body {\n  margin-left: 4%;\n  margin-right: 4%;\n  margin-top: 2%;\n  border-radius: 7.8px;\n  height: 250px;\n}\n/* 卡片动画与交互增强 */\n.Main-body {\n  margin-left: 4%;\n  margin-right: 4%;\n  margin-top: 2%;\n  border-radius: 7.8px;\n  height: 250px;\n  transition: box-shadow 0.25s, transform 0.22s cubic-bezier(.4,0,.2,1);\n  box-shadow: 0 2px 8px rgba(0,0,0,0.08);\n  background: var(--card-bg, #fff);\n}\n.Main-body.hover-card:hover,\n.Main-body.hover-card:focus {\n  box-shadow: 0 8px 32px rgba(59,130,246,0.18), 0 2px 8px rgba(0,0,0,0.08);\n  transform: translateY(-2px) scale(1.025);\n  outline: none;\n}\n.Main-body.hover-card:active {\n  box-shadow: 0 2px 8px rgba(0,0,0,0.12);\n  transform: scale(0.98);\n}\n\n/* 按钮动画与交互增强 */\n.home-btn.btn-animated {\n  transition: background 0.18s, color 0.18s, box-shadow 0.18s, transform 0.18s;\n  box-shadow: 0 1px 4px rgba(59,130,246,0.08);\n}\n.home-btn.btn-animated:hover,\n.home-btn.btn-animated:focus {\n  background: #2563eb;\n  color: #fff;\n  box-shadow: 0 4px 16px rgba(59,130,246,0.18);\n  transform: scale(1.045);\n}\n.home-btn.btn-animated:active {\n  background: #1d4ed8;\n  color: #fff;\n  transform: scale(0.97);\n}\n</style>\n"],"mappings":";AA4BA,SAASA,QAAqB,QAAQ,KAAI;AAC1C,SAASC,SAAS,QAAQ,YAAW;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;IAFrC;IAIA,MAAMC,KAAK,GAAGC,OAAA;IAUd,MAAMC,MAAM,GAAGH,SAAS,CAAC;IAEzB,MAAMI,WAAW,GAAGL,QAAQ,CAAC,MAAME,KAAK,CAACI,YAAY,GAAG,eAAe,GAAG,aAAa;IACvF;IACA,MAAMC,cAAc,GAAGP,QAAQ,CAAC,MAAME,KAAK,CAACK,cAAc,IAAI,mCAAmC;IAEjG,SAASC,WAAWA,CAAA,EAAI;MACtB,IAAIN,KAAK,CAACO,UAAU,EAAE;QACpBC,MAAM,CAACC,IAAI,CAACT,KAAK,CAACU,IAAI,EAAE,QAAQ;MAClC,CAAC,MAAM,IAAI,CAACV,KAAK,CAACI,YAAY,EAAE;QAC9BF,MAAM,CAACS,IAAI,CAACX,KAAK,CAACU,IAAI;MACxB,CAAC,MAAM;QACLR,MAAM,CAACS,IAAI,CAAC,qBAAqB;MACnC;IACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}