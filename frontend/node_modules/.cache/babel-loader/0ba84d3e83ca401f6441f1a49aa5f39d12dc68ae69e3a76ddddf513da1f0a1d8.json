{"ast":null,"code":"import { createElementVNode as _createElementVNode, toDisplayString as _toDisplayString, createTextVNode as _createTextVNode, normalizeClass as _normalizeClass, openBlock as _openBlock, createElementBlock as _createElementBlock, createCommentVNode as _createCommentVNode, renderList as _renderList, Fragment as _Fragment, withModifiers as _withModifiers, normalizeStyle as _normalizeStyle } from \"vue\";\nconst _hoisted_1 = {\n  class: \"workspace-title\"\n};\nconst _hoisted_2 = {\n  class: \"file-counter\"\n};\nconst _hoisted_3 = {\n  key: 0,\n  class: \"workspace-content\"\n};\nconst _hoisted_4 = {\n  class: \"file-info\"\n};\nconst _hoisted_5 = {\n  class: \"file-name\"\n};\nconst _hoisted_6 = {\n  class: \"file-meta\"\n};\nconst _hoisted_7 = {\n  class: \"file-size\"\n};\nconst _hoisted_8 = {\n  class: \"file-type\"\n};\nconst _hoisted_9 = {\n  class: \"workspace-actions\"\n};\nconst _hoisted_10 = {\n  key: 1,\n  class: \"workspace-navigation\"\n};\nconst _hoisted_11 = {\n  class: \"nav-dots\"\n};\nconst _hoisted_12 = [\"onClick\"];\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  return $setup.workspaceFiles.length > 0 ? (_openBlock(), _createElementBlock(\"div\", {\n    key: 0,\n    class: _normalizeClass([\"file-workspace\", {\n      'is-focused': $setup.isFocused,\n      'dragging': $setup.isDragging\n    }]),\n    style: _normalizeStyle($setup.workspacePosition),\n    onWheel: _withModifiers($setup.handleWheel, [\"prevent\"]),\n    onMouseenter: $setup.handleMouseEnter,\n    onMouseleave: $setup.handleMouseLeave,\n    onMousedown: $setup.handleMouseDown,\n    ref: \"workspaceRef\"\n  }, [_createElementVNode(\"div\", null, [_cache[3] || (_cache[3] = _createTextVNode(\" class=\\\"workspace-header\\\" @mousedown=\\\"startDrag\\\" @mousemove=\\\"handleDrag\\\" @mouseup=\\\"stopDrag\\\"> \")), _createElementVNode(\"div\", _hoisted_1, [_cache[1] || (_cache[1] = _createElementVNode(\"span\", {\n    class: \"workspace-icon\"\n  }, \"üìÅ\", -1 /* HOISTED */)), _cache[2] || (_cache[2] = _createElementVNode(\"span\", null, \"Files\", -1 /* HOISTED */)), _createElementVNode(\"span\", _hoisted_2, _toDisplayString($setup.currentIndex + 1) + \"/\" + _toDisplayString($setup.workspaceFiles.length), 1 /* TEXT */)])]), $setup.currentFile ? (_openBlock(), _createElementBlock(\"div\", _hoisted_3, [_createElementVNode(\"div\", _hoisted_4, [_createElementVNode(\"div\", _hoisted_5, _toDisplayString($setup.currentFile.name), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_6, [_createElementVNode(\"span\", _hoisted_7, _toDisplayString($setup.formatFileSize($setup.currentFile.size)), 1 /* TEXT */), _createElementVNode(\"span\", _hoisted_8, _toDisplayString($setup.currentFile.name.split('.').pop().toUpperCase()), 1 /* TEXT */), _createElementVNode(\"span\", {\n    class: _normalizeClass([\"file-status\", $setup.currentFile.status])\n  }, _toDisplayString($setup.getStatusText($setup.currentFile.status)), 3 /* TEXT, CLASS */)])]), _createElementVNode(\"div\", _hoisted_9, [_createElementVNode(\"button\", {\n    class: \"workspace-action-btn remove-btn\",\n    onClick: _cache[0] || (_cache[0] = $event => $setup.removeFromWorkspace($setup.currentIndex)),\n    title: \"Remove from workspace\"\n  }, \" ‚úï \"), _createElementVNode(\"button\", {\n    class: \"workspace-action-btn preview-btn\",\n    onClick: $setup.previewCurrentFile,\n    title: \"Preview file\"\n  }, \" üëÅ \")])])) : _createCommentVNode(\"v-if\", true), $setup.workspaceFiles.length > 1 ? (_openBlock(), _createElementBlock(\"div\", _hoisted_10, [_createElementVNode(\"div\", _hoisted_11, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($setup.workspaceFiles, (file, index) => {\n    return _openBlock(), _createElementBlock(\"span\", {\n      key: file.id,\n      class: _normalizeClass([\"nav-dot\", {\n        active: index === $setup.currentIndex\n      }]),\n      onClick: $event => $setup.currentIndex = index\n    }, null, 10 /* CLASS, PROPS */, _hoisted_12);\n  }), 128 /* KEYED_FRAGMENT */))])])) : _createCommentVNode(\"v-if\", true)], 38 /* CLASS, STYLE, NEED_HYDRATION */)) : _createCommentVNode(\"v-if\", true);\n}","map":{"version":3,"names":["class","key","$setup","workspaceFiles","length","_createElementBlock","_normalizeClass","isFocused","isDragging","style","_normalizeStyle","workspacePosition","onWheel","_withModifiers","handleWheel","onMouseenter","handleMouseEnter","onMouseleave","handleMouseLeave","onMousedown","handleMouseDown","ref","_createElementVNode","_createTextVNode","_hoisted_1","_hoisted_2","_toDisplayString","currentIndex","currentFile","_hoisted_3","_hoisted_4","_hoisted_5","name","_hoisted_6","_hoisted_7","formatFileSize","size","_hoisted_8","split","pop","toUpperCase","status","getStatusText","_hoisted_9","onClick","_cache","$event","removeFromWorkspace","title","previewCurrentFile","_createCommentVNode","_hoisted_10","_hoisted_11","_Fragment","_renderList","file","index","id","active","_hoisted_12"],"sources":["D:\\Learning Material\\Git\\Fuck-Charts\\newArchitecture\\frontend_vue_design\\src\\components\\FileWorkspace.vue"],"sourcesContent":["<template>\n<div\nv-if=\"workspaceFiles.length > 0\"\nclass=\"file-workspace\"\n:class=\"{\n    'is-focused': isFocused,\n    'dragging': isDragging\n}\"\n:style=\"workspacePosition\"\n@wheel.prevent=\"handleWheel\"\n@mouseenter=\"handleMouseEnter\"\n@mouseleave=\"handleMouseLeave\"\n@mousedown=\"handleMouseDown\"\nref=\"workspaceRef\">\n<div>\n    class=\"workspace-header\"\n    @mousedown=\"startDrag\"\n    @mousemove=\"handleDrag\"\n    @mouseup=\"stopDrag\">\n    <div class=\"workspace-title\">\n    <span class=\"workspace-icon\">üìÅ</span>\n    <span>Files</span>\n    <span class=\"file-counter\">{{ currentIndex + 1 }}/{{ workspaceFiles.length }}</span>\n    </div>\n</div>\n\n<div class=\"workspace-content\" v-if=\"currentFile\">\n    <div class=\"file-info\">\n    <div class=\"file-name\">{{ currentFile.name }}</div>\n    <div class=\"file-meta\">\n        <span class=\"file-size\">{{ formatFileSize(currentFile.size) }}</span>\n        <span class=\"file-type\">{{ currentFile.name.split('.').pop().toUpperCase() }}</span>\n        <span class=\"file-status\" :class=\"currentFile.status\">\n        {{ getStatusText(currentFile.status) }}\n        </span>\n    </div>\n    </div>\n\n    <div class=\"workspace-actions\">\n    <button\n        class=\"workspace-action-btn remove-btn\"\n        @click=\"removeFromWorkspace(currentIndex)\"\n        title=\"Remove from workspace\">\n        ‚úï\n    </button>\n    <button\n        class=\"workspace-action-btn preview-btn\"\n        @click=\"previewCurrentFile\"\n        title=\"Preview file\">\n        üëÅ\n    </button>\n    </div>\n</div>\n\n<div class=\"workspace-navigation\" v-if=\"workspaceFiles.length > 1\">\n    <div class=\"nav-dots\">\n    <span\n        v-for=\"(file, index) in workspaceFiles\"\n        :key=\"file.id\"\n        class=\"nav-dot\"\n        :class=\"{ active: index === currentIndex }\"\n        @click=\"currentIndex = index\">\n    </span>\n    </div>\n</div>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, computed, watch, onMounted, onUnmounted } from 'vue'\n\n// Props\nconst props = defineProps({\n    files: {\n    type: Array,\n    default: () => []\n    }\n})\n\n// Emits\nconst emit = defineEmits(['remove', 'preview', 'clear'])\n\n// Â∑•‰ΩúÂå∫Êñá‰ª∂ÂàóË°®\nconst workspaceFiles = ref([])\nconst currentIndex = ref(0)\n\n// ÊãñÊãΩÁõ∏ÂÖ≥Áä∂ÊÄÅ\nconst isDragging = ref(false)\nconst isFocused = ref(true)\nconst dragOffset = ref({ x: 0, y: 0 })\nconst workspacePosition = ref({\n    position: 'fixed',\n    top: '10px',\n    left: '260px'\n})\n\n// DOM ÂºïÁî®\nconst workspaceRef = ref(null)\n\n// ÂΩìÂâçÊòæÁ§∫ÁöÑÊñá‰ª∂\nconst currentFile = computed(() => {\n    return workspaceFiles.value[currentIndex.value] || null\n})\n\n// ÁõëÂê¨‰º†ÂÖ•ÁöÑÊñá‰ª∂ÂàóË°®ÂèòÂåñ\nwatch(() => props.files, (newFiles) => {\n    workspaceFiles.value = [...newFiles]\n    if (currentIndex.value >= workspaceFiles.value.length) {\n        currentIndex.value = Math.max(0, workspaceFiles.value.length - 1)\n    }\n}, { immediate: true })\n\n// Èº†Ê†á‰∫ã‰ª∂Â§ÑÁêÜ\nfunction handleMouseEnter() {\n    isFocused.value = true\n}\n\nfunction handleMouseLeave() {\n    if (!isDragging.value) {\n        isFocused.value = false\n    }\n}\n\nfunction handleMouseDown() {\n    isFocused.value = true\n}\n\n// ÊãñÊãΩÂäüËÉΩ\nfunction startDrag(event) {\n    isDragging.value = true\n    isFocused.value = true\n    \n    const rect = workspaceRef.value.getBoundingClientRect()\n    dragOffset.value = {\n        x: event.clientX - rect.left,\n        y: event.clientY - rect.top\n    }\n    \n    document.addEventListener('mousemove', handleDrag)\n    document.addEventListener('mouseup', stopDrag)\n    event.preventDefault()\n}\n\nfunction handleDrag(event) {\n    if (!isDragging.value) return\n    \n    const newX = event.clientX - dragOffset.value.x\n    const newY = event.clientY - dragOffset.value.y\n    \n    // Á°Æ‰øùÁ™óÂè£‰∏ç‰ºöÊãñÊãΩÂà∞Â±èÂπïÂ§ñ\n    const maxX = window.innerWidth - 220\n    const maxY = window.innerHeight - 200\n    \n    const clampedX = Math.max(0, Math.min(newX, maxX))\n    const clampedY = Math.max(0, Math.min(newY, maxY))\n    \n    workspacePosition.value = {\n        position: 'fixed',\n        top: `${clampedY}px`,\n        left: `${clampedX}px`\n    }\n}\n\nfunction stopDrag() {\n    isDragging.value = false\n    document.removeEventListener('mousemove', handleDrag)\n    document.removeEventListener('mouseup', stopDrag)\n    \n    // Âª∂ËøüÊ£ÄÊü•ÁÑ¶ÁÇπÁä∂ÊÄÅ\n    setTimeout(() => {\n        if (!isDragging.value) {\n        isFocused.value = false\n        }\n    }, 100)\n}\n\n// ÁªÑ‰ª∂ÊåÇËΩΩÊó∂Ê∑ªÂä†ÂÖ®Â±ÄÁÇπÂáªÁõëÂê¨\nonMounted(() => {\n  document.addEventListener('click', handleGlobalClick)\n})\n\nonUnmounted(() => {\n  document.removeEventListener('click', handleGlobalClick)\n  document.removeEventListener('mousemove', handleDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n\n// ÂÖ®Â±ÄÁÇπÂáªÂ§ÑÁêÜ\nfunction handleGlobalClick(event) {\n  if (workspaceRef.value && !workspaceRef.value.contains(event.target)) {\n    isFocused.value = false\n  }\n}\n\n// Â§ÑÁêÜÊªöËΩÆ‰∫ã‰ª∂\nfunction handleWheel (event) {\n  if (workspaceFiles.value.length <= 1) return\n\n  const delta = event.deltaY\n  if (delta > 0) {\n    // Âêë‰∏ãÊªöÂä®ÔºåÂàáÊç¢Âà∞‰∏ã‰∏Ä‰∏™Êñá‰ª∂\n    currentIndex.value = (currentIndex.value + 1) % workspaceFiles.value.length\n  } else {\n    // Âêë‰∏äÊªöÂä®ÔºåÂàáÊç¢Âà∞‰∏ä‰∏Ä‰∏™Êñá‰ª∂\n    currentIndex.value = currentIndex.value === 0\n      ? workspaceFiles.value.length - 1\n      : currentIndex.value - 1\n  }\n}\n\n// ‰ªéÂ∑•‰ΩúÂå∫ÁßªÈô§Êñá‰ª∂\nfunction removeFromWorkspace (index) {\n  const removedFile = workspaceFiles.value[index]\n  workspaceFiles.value.splice(index, 1)\n\n  // Ë∞ÉÊï¥ÂΩìÂâçÁ¥¢Âºï\n  if (currentIndex.value >= workspaceFiles.value.length) {\n    currentIndex.value = Math.max(0, workspaceFiles.value.length - 1)\n  }\n\n  emit('remove', removedFile)\n}\n\n// Ê∏ÖÁ©∫Â∑•‰ΩúÂå∫\nfunction clearWorkspace () {\n  workspaceFiles.value = []\n  currentIndex.value = 0\n  emit('clear')\n}\n\n// È¢ÑËßàÂΩìÂâçÊñá‰ª∂\nfunction previewCurrentFile () {\n  if (currentFile.value) {\n    emit('preview', currentFile.value)\n  }\n}\n\n// Ê†ºÂºèÂåñÊñá‰ª∂Â§ßÂ∞è\nfunction formatFileSize (bytes) {\n  if (bytes === 0) return '0 Bytes'\n  const k = 1024\n  const sizes = ['Bytes', 'KB', 'MB', 'GB']\n  const i = Math.floor(Math.log(bytes) / Math.log(k))\n  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\n}\n\n// Ëé∑ÂèñÁä∂ÊÄÅÊñáÊú¨\nfunction getStatusText (status) {\n  const statusMap = {\n    local: 'Local',\n    uploading: 'Uploading',\n    uploaded: 'Uploaded',\n    operating: 'Operating',\n    error: 'Error'\n  }\n  return statusMap[status] || 'Unknown'\n}\n\n// Êö¥Èú≤ÊñπÊ≥ïÁªôÁà∂ÁªÑ‰ª∂\ndefineExpose({\n  addFile: (file) => {\n    // Ê£ÄÊü•Êñá‰ª∂ÊòØÂê¶Â∑≤ÁªèÂú®Â∑•‰ΩúÂå∫\n    const exists = workspaceFiles.value.find(f => f.id === file.id)\n    if (!exists) {\n      workspaceFiles.value.push(file)\n    }\n  },\n  removeFile: (fileId) => {\n    const index = workspaceFiles.value.findIndex(f => f.id === fileId)\n    if (index !== -1) {\n      removeFromWorkspace(index)\n    }\n  },\n  clearAll: () => {\n    clearWorkspace()\n  }\n})\n</script>\n\n<style scoped>\n.file-workspace {\n  position: fixed;\n  top: 10px;\n  left: 260px;\n  width: 220px;\n  background: rgba(255, 255, 255, 0.4);\n  backdrop-filter: blur(20px);\n  border-radius: 8px;\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n  z-index: 1000;\n  padding: 10px;\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n  cursor: default;\n  user-select: none;\n}\n\n.file-workspace.is-focused {\n  background: rgba(255, 255, 255, 0.85);\n  transform: translateY(-1px);\n  box-shadow: 0 6px 24px rgba(0, 0, 0, 0.15);\n}\n\n.file-workspace:not(.is-focused) {\n  background: rgba(255, 255, 255, 0.3);\n  opacity: 0.6;\n}\n\n/* ÊöóÈªëÊ®°ÂºèÈÄÇÈÖç */\n[data-theme=\"dark\"] .file-workspace {\n  background: rgba(17, 24, 39, 0.4);\n  border: 1px solid rgba(55, 65, 81, 0.4);\n}\n\n[data-theme=\"dark\"] .file-workspace.is-focused {\n  background: rgba(17, 24, 39, 0.85);\n}\n\n[data-theme=\"dark\"] .file-workspace:not(.is-focused) {\n  background: rgba(17, 24, 39, 0.25);\n  opacity: 0.6;\n}\n\n.workspace-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 8px;\n  padding-bottom: 6px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.08);\n  cursor: move;\n  padding: 4px 0 6px 0;\n}\n\n.workspace-header:hover {\n  background: rgba(59, 130, 246, 0.05);\n  border-radius: 4px;\n  margin: -4px 0 8px 0;\n  padding: 8px 4px 6px 4px;\n}\n\n[data-theme=\"dark\"] .workspace-header {\n  border-bottom: 1px solid rgba(255, 255, 255, 0.08);\n}\n\n[data-theme=\"dark\"] .workspace-header:hover {\n  background: rgba(96, 165, 250, 0.1);\n}\n\n.workspace-title {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 12px;\n  font-weight: 600;\n  color: var(--text-color);\n  pointer-events: none;\n  flex: 1;\n}\n\n.workspace-title::after {\n  content: '‚ãÆ‚ãÆ';\n  margin-left: auto;\n  color: var(--text-secondary);\n  font-size: 14px;\n  opacity: 0.5;\n  pointer-events: none;\n  transform: rotate(90deg);\n}\n\n.workspace-header:hover .workspace-title::after {\n  opacity: 0.8;\n  color: #3b82f6;\n}\n\n.workspace-icon {\n  font-size: 14px;\n}\n\n.file-counter {\n  font-size: 10px;\n  color: var(--text-secondary);\n  background: rgba(59, 130, 246, 0.1);\n  padding: 1px 4px;\n  border-radius: 3px;\n  pointer-events: none;\n}\n\n.workspace-content {\n  margin-bottom: 8px;\n}\n\n.file-info {\n  margin-bottom: 6px;\n}\n\n.file-name {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--text-color);\n  margin-bottom: 3px;\n  word-break: break-word;\n  line-height: 1.2;\n}\n\n.file-meta {\n  display: flex;\n  gap: 4px;\n  font-size: 10px;\n  color: var(--text-secondary);\n  flex-wrap: wrap;\n}\n\n.file-size, .file-type {\n  padding: 1px 4px;\n  background: rgba(107, 114, 128, 0.1);\n  border-radius: 3px;\n}\n\n.file-status {\n  padding: 1px 4px;\n  border-radius: 3px;\n  font-weight: 500;\n  font-size: 10px;\n}\n\n.file-status.local {\n  background: rgba(251, 191, 36, 0.2);\n  color: #92400e;\n}\n\n.file-status.uploading {\n  background: rgba(59, 130, 246, 0.2);\n  color: #1e40af;\n}\n\n.file-status.uploaded {\n  background: rgba(16, 185, 129, 0.2);\n  color: #065f46;\n}\n\n.file-status.operating {\n  background: rgba(139, 92, 246, 0.2);\n  color: #6b46c1;\n}\n\n.file-status.error {\n  background: rgba(239, 68, 68, 0.2);\n  color: #b91c1c;\n}\n\n/* ÊöóÈªëÊ®°Âºè‰∏ãÁöÑÁä∂ÊÄÅÈ¢úËâ≤ */\n[data-theme=\"dark\"] .file-status.local {\n  background: rgba(251, 191, 36, 0.3);\n  color: #fcd34d;\n}\n\n[data-theme=\"dark\"] .file-status.uploading {\n  background: rgba(59, 130, 246, 0.3);\n  color: #93c5fd;\n}\n\n[data-theme=\"dark\"] .file-status.uploaded {\n  background: rgba(16, 185, 129, 0.3);\n  color: #6ee7b7;\n}\n\n[data-theme=\"dark\"] .file-status.operating {\n  background: rgba(139, 92, 246, 0.3);\n  color: #c4b5fd;\n}\n\n[data-theme=\"dark\"] .file-status.error {\n  background: rgba(239, 68, 68, 0.3);\n  color: #fca5a5;\n}\n\n.workspace-actions {\n  display: flex;\n  gap: 4px;\n}\n\n.workspace-action-btn {\n  background: none;\n  border: 1px solid rgba(0, 0, 0, 0.08);\n  padding: 4px 8px;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 10px;\n  transition: all 0.2s;\n  display: flex;\n  align-items: center;\n  gap: 2px;\n}\n\n[data-theme=\"dark\"] .workspace-action-btn {\n  border: 1px solid rgba(255, 255, 255, 0.08);\n}\n\n.workspace-action-btn:hover {\n  transform: translateY(-1px);\n}\n\n.remove-btn:hover {\n  background: rgba(239, 68, 68, 0.1);\n  border-color: #ef4444;\n}\n\n.preview-btn:hover {\n  background: rgba(59, 130, 246, 0.1);\n  border-color: #3b82f6;\n}\n\n.workspace-navigation {\n  display: flex;\n  justify-content: center;\n  padding-top: 6px;\n  border-top: 1px solid rgba(0, 0, 0, 0.08);\n}\n\n[data-theme=\"dark\"] .workspace-navigation {\n  border-top: 1px solid rgba(255, 255, 255, 0.08);\n}\n\n.nav-dots {\n  display: flex;\n  gap: 4px;\n}\n\n.nav-dot {\n  width: 6px;\n  height: 6px;\n  border-radius: 50%;\n  background: rgba(0, 0, 0, 0.2);\n  cursor: pointer;\n  transition: all 0.2s;\n}\n\n[data-theme=\"dark\"] .nav-dot {\n  background: rgba(255, 255, 255, 0.2);\n}\n\n.nav-dot.active {\n  background: #3b82f6;\n  transform: scale(1.3);\n}\n\n.nav-dot:hover {\n  transform: scale(1.2);\n}\n\n/* ÂìçÂ∫îÂºèÈÄÇÈÖç */\n@media (max-width: 768px) {\n  .file-workspace {\n    width: 180px;\n    top: 8px;\n    left: 220px;\n  }\n\n  .workspace-title {\n    font-size: 10px;\n  }\n\n  .file-name {\n    font-size: 10px;\n  }\n\n  .file-meta {\n