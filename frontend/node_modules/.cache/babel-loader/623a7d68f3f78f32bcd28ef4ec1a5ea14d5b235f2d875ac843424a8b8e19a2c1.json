{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nimport { ref, computed, onMounted, onUnmounted } from 'vue';\nexport default {\n  __name: 'FileStructureModal',\n  props: {\n    show: {\n      type: Boolean,\n      default: false\n    },\n    fileInfo: {\n      type: Object,\n      required: true\n    },\n    previewData: {\n      type: Array,\n      default: () => []\n    }\n  },\n  emits: ['close', 'column-drag'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    const props = __props;\n    const emit = __emit;\n\n    // ËßÜÂõæÊ®°Âºè\n    const viewMode = ref('grid');\n\n    // ÊãñÊãΩÁõ∏ÂÖ≥\n    const isDragging = ref(false);\n    const draggedColumn = ref(null);\n    const dragOffset = ref({\n      x: 0,\n      y: 0\n    });\n    const modalPosition = ref({\n      position: 'fixed',\n      top: '50px',\n      left: '300px',\n      zIndex: 10000\n    });\n    const modalRef = ref(null);\n\n    // ËÆ°ÁÆóÂàóÁªìÊûÑ‰ø°ÊÅØ\n    const columns = computed(() => {\n      if (!props.previewData || props.previewData.length === 0) return [];\n      const columnNames = Object.keys(props.previewData[0] || {});\n      return columnNames.map(name => {\n        const values = props.previewData.map(row => row[name]).filter(val => val !== null && val !== undefined && val !== '');\n        const uniqueValues = [...new Set(values)];\n\n        // Ê£ÄÊµãÊï∞ÊçÆÁ±ªÂûã\n        const type = detectColumnType(values);\n\n        // Ëé∑ÂèñÊ†∑Êú¨Êï∞ÊçÆ\n        const samples = uniqueValues.slice(0, 3).map(val => String(val).length > 20 ? String(val).substring(0, 20) + '...' : String(val));\n        return {\n          name,\n          type,\n          nonNull: values.length,\n          unique: uniqueValues.length,\n          samples\n        };\n      });\n    });\n\n    // Ê£ÄÊµãÂàóÊï∞ÊçÆÁ±ªÂûã\n    function detectColumnType(values) {\n      if (values.length === 0) return 'unknown';\n      const sample = values.slice(0, 10);\n      let numericCount = 0;\n      let dateCount = 0;\n      for (const value of sample) {\n        if (!isNaN(Number(value)) && !isNaN(parseFloat(value))) {\n          numericCount++;\n        }\n        if (isValidDate(value)) {\n          dateCount++;\n        }\n      }\n      if (numericCount / sample.length > 0.8) return 'numeric';\n      if (dateCount / sample.length > 0.6) return 'date';\n      return 'text';\n    }\n\n    // Ê£ÄÊµãÊòØÂê¶‰∏∫ÊúâÊïàÊó•Êúü\n    function isValidDate(value) {\n      const date = new Date(value);\n      return !isNaN(date.getTime()) && String(value).length > 6;\n    }\n\n    // Ëé∑ÂèñÊñá‰ª∂ÂõæÊ†á\n    function getFileIcon(fileName) {\n      const ext = fileName.split('.').pop().toLowerCase();\n      const iconMap = {\n        'xlsx': 'üìä',\n        'csv': 'üìã',\n        'json': 'üìÑ',\n        'txt': 'üìù'\n      };\n      return iconMap[ext] || 'üìÅ';\n    }\n\n    // Ê†ºÂºèÂåñÊñá‰ª∂Â§ßÂ∞è\n    function formatFileSize(bytes) {\n      if (bytes === 0) return '0 Bytes';\n      const k = 1024;\n      const sizes = ['Bytes', 'KB', 'MB', 'GB'];\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\n    }\n\n    // Ê®°ÊÄÅÊ°ÜÊãñÊãΩ\n    function startDrag(event) {\n      if (event.target.closest('.close-btn') || event.target.closest('.column-item')) return;\n      isDragging.value = true;\n      const rect = modalRef.value.getBoundingClientRect();\n      dragOffset.value = {\n        x: event.clientX - rect.left,\n        y: event.clientY - rect.top\n      };\n      document.addEventListener('mousemove', handleDrag);\n      document.addEventListener('mouseup', stopDrag);\n      event.preventDefault();\n    }\n    function handleDrag(event) {\n      if (!isDragging.value) return;\n      const newX = event.clientX - dragOffset.value.x;\n      const newY = event.clientY - dragOffset.value.y;\n      const maxX = window.innerWidth - 400;\n      const maxY = window.innerHeight - 300;\n      modalPosition.value = {\n        position: 'fixed',\n        top: `${Math.max(0, Math.min(newY, maxY))}px`,\n        left: `${Math.max(0, Math.min(newX, maxX))}px`,\n        zIndex: 10000\n      };\n    }\n    function stopDrag() {\n      isDragging.value = false;\n      document.removeEventListener('mousemove', handleDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    }\n\n    // ÂàóÊãñÊãΩ\n    function startColumnDrag(event, column) {\n      draggedColumn.value = column.name;\n\n      // ËÆæÁΩÆÊãñÊãΩÊï∞ÊçÆ\n      event.dataTransfer.setData('text/plain', JSON.stringify({\n        type: 'column',\n        fileName: props.fileInfo.name,\n        fileId: props.fileInfo.id,\n        columnName: column.name,\n        columnType: column.type,\n        columnData: column\n      }));\n      event.dataTransfer.effectAllowed = 'copy';\n      emit('column-drag', {\n        column,\n        fileInfo: props.fileInfo\n      });\n    }\n    function endColumnDrag() {\n      draggedColumn.value = null;\n    }\n\n    // Ê∏ÖÁêÜ‰∫ã‰ª∂ÁõëÂê¨\n    onUnmounted(() => {\n      document.removeEventListener('mousemove', handleDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    const __returned__ = {\n      props,\n      emit,\n      viewMode,\n      isDragging,\n      draggedColumn,\n      dragOffset,\n      modalPosition,\n      modalRef,\n      columns,\n      detectColumnType,\n      isValidDate,\n      getFileIcon,\n      formatFileSize,\n      startDrag,\n      handleDrag,\n      stopDrag,\n      startColumnDrag,\n      endColumnDrag,\n      ref,\n      computed,\n      onMounted,\n      onUnmounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","onUnmounted","props","__props","emit","__emit","viewMode","isDragging","draggedColumn","dragOffset","x","y","modalPosition","position","top","left","zIndex","modalRef","columns","previewData","length","columnNames","Object","keys","map","name","values","row","filter","val","undefined","uniqueValues","Set","type","detectColumnType","samples","slice","String","substring","nonNull","unique","sample","numericCount","dateCount","value","isNaN","Number","parseFloat","isValidDate","date","Date","getTime","getFileIcon","fileName","ext","split","pop","toLowerCase","iconMap","formatFileSize","bytes","k","sizes","i","Math","floor","log","pow","toFixed","startDrag","event","target","closest","rect","getBoundingClientRect","clientX","clientY","document","addEventListener","handleDrag","stopDrag","preventDefault","newX","newY","maxX","window","innerWidth","maxY","innerHeight","max","min","removeEventListener","startColumnDrag","column","dataTransfer","setData","JSON","stringify","fileInfo","fileId","id","columnName","columnType","columnData","effectAllowed","endColumnDrag"],"sources":["D:/Learning Material/Git/Fuck-Charts/newArchitecture/frontend_vue_design/src/components/FileStructureModal.vue"],"sourcesContent":["<template>\n<div\n  v-if=\"show\"\n  class=\"file-structure-modal\"\n  :style=\"modalPosition\"\n  @mousedown=\"startDrag\"\n  ref=\"modalRef\"\n>\n  <div class=\"modal-header\">\n    <div class=\"modal-title\">\n      <span class=\"file-icon\" v-html=\"getFileIcon(fileInfo.name)\"></span>\n      <span class=\"file-name\">{{ fileInfo.name }}</span>\n      <span class=\"column-count\">{{ columns.length }} Âàó</span>\n    </div>\n    <button class=\"close-btn\" @click=\"$emit('close')\">√ó</button>\n  </div>\n  <div class=\"modal-content\">\n    <div class=\"structure-info\">\n      <div class=\"info-row\">\n        <span class=\"info-label\">Êñá‰ª∂Â§ßÂ∞è:</span>\n        <span class=\"info-value\">{{ formatFileSize(fileInfo.size) }}</span>\n      </div>\n      <div class=\"info-row\">\n        <span class=\"info-label\">Êï∞ÊçÆË°åÊï∞:</span>\n        <span class=\"info-value\">{{ previewData.length }}</span>\n      </div>\n      <div class=\"info-row\">\n        <span class=\"info-label\">Êñá‰ª∂Á±ªÂûã:</span>\n        <span class=\"info-value\">{{ fileInfo.name.split('.').pop().toUpperCase() }}</span>\n      </div>\n    </div>\n    \n    <div class=\"columns-section\">\n      <div class=\"section-header\">\n        <h4>ÂàóÁªìÊûÑ</h4>\n        <div class=\"view-toggle\">\n          <button \n            :class=\"{ active: viewMode === 'grid' }\"\n            @click=\"viewMode = 'grid'\"\n            title=\"ÁΩëÊ†ºËßÜÂõæ\"\n          >\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"/>\n              <rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"/>\n              <rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"/>\n              <rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"/>\n            </svg>\n          </button>\n          <button \n            :class=\"{ active: viewMode === 'list' }\"\n            @click=\"viewMode = 'list'\"\n            title=\"ÂàóË°®ËßÜÂõæ\"\n          >\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <line x1=\"8\" y1=\"6\" x2=\"21\" y2=\"6\"/>\n              <line x1=\"8\" y1=\"12\" x2=\"21\" y2=\"12\"/>\n              <line x1=\"8\" y1=\"18\" x2=\"21\" y2=\"18\"/>\n              <line x1=\"3\" y1=\"6\" x2=\"3.01\" y2=\"6\"/>\n              <line x1=\"3\" y1=\"12\" x2=\"3.01\" y2=\"12\"/>\n              <line x1=\"3\" y1=\"18\" x2=\"3.01\" y2=\"18\"/>\n            </svg>\n          </button>\n        </div>\n      </div>\n      \n      <div class=\"columns-container\" :class=\"viewMode\">\n        <div \n          v-for=\"(column, index) in columns\" \n          :key=\"index\"\n          class=\"column-item\"\n          :class=\"{ 'dragging': draggedColumn === column.name }\"\n          draggable=\"true\"\n          @dragstart=\"startColumnDrag($event, column)\"\n          @dragend=\"endColumnDrag\"\n        >\n          <div class=\"column-header\">\n            <div class=\"column-name\">\n              <span class=\"drag-handle\">‚ãÆ‚ãÆ</span>\n              {{ column.name }}\n            </div>\n            <div class=\"column-type\" :class=\"column.type\">\n              {{ column.type }}\n            </div>\n          </div>\n          <div class=\"column-stats\">\n            <div class=\"stat-item\">\n              <span class=\"stat-label\">ÈùûÁ©∫:</span>\n              <span class=\"stat-value\">{{ column.nonNull }}/{{ previewData.length }}</span>\n            </div>\n            <div class=\"stat-item\">\n              <span class=\"stat-label\">ÂîØ‰∏Ä:</span>\n              <span class=\"stat-value\">{{ column.unique }}</span>\n            </div>\n          </div>\n          <div class=\"column-samples\">\n            <div class=\"sample-header\">Ê†∑Êú¨Êï∞ÊçÆ:</div>\n            <div class=\"sample-values\">\n              <span \n                v-for=\"(sample, i) in column.samples\" \n                :key=\"i\"\n                class=\"sample-value\"\n              >\n                {{ sample }}\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, computed, onMounted, onUnmounted } from 'vue'\n\nconst props = defineProps({\n  show: {\n    type: Boolean,\n    default: false\n  },\n  fileInfo: {\n    type: Object,\n    required: true\n  },\n  previewData: {\n    type: Array,\n    default: () => []\n  }\n})\n\nconst emit = defineEmits(['close', 'column-drag'])\n\n// ËßÜÂõæÊ®°Âºè\nconst viewMode = ref('grid')\n\n// ÊãñÊãΩÁõ∏ÂÖ≥\nconst isDragging = ref(false)\nconst draggedColumn = ref(null)\nconst dragOffset = ref({ x: 0, y: 0 })\nconst modalPosition = ref({\n  position: 'fixed',\n  top: '50px',\n  left: '300px',\n  zIndex: 10000\n})\n\nconst modalRef = ref(null)\n\n// ËÆ°ÁÆóÂàóÁªìÊûÑ‰ø°ÊÅØ\nconst columns = computed(() => {\n  if (!props.previewData || props.previewData.length === 0) return []\n  \n  const columnNames = Object.keys(props.previewData[0] || {})\n  \n  return columnNames.map(name => {\n    const values = props.previewData.map(row => row[name]).filter(val => val !== null && val !== undefined && val !== '')\n    const uniqueValues = [...new Set(values)]\n    \n    // Ê£ÄÊµãÊï∞ÊçÆÁ±ªÂûã\n    const type = detectColumnType(values)\n    \n    // Ëé∑ÂèñÊ†∑Êú¨Êï∞ÊçÆ\n    const samples = uniqueValues.slice(0, 3).map(val => \n      String(val).length > 20 ? String(val).substring(0, 20) + '...' : String(val)\n    )\n    \n    return {\n      name,\n      type,\n      nonNull: values.length,\n      unique: uniqueValues.length,\n      samples\n    }\n  })\n})\n\n// Ê£ÄÊµãÂàóÊï∞ÊçÆÁ±ªÂûã\nfunction detectColumnType(values) {\n  if (values.length === 0) return 'unknown'\n  \n  const sample = values.slice(0, 10)\n  let numericCount = 0\n  let dateCount = 0\n  \n  for (const value of sample) {\n    if (!isNaN(Number(value)) && !isNaN(parseFloat(value))) {\n      numericCount++\n    }\n    if (isValidDate(value)) {\n      dateCount++\n    }\n  }\n  \n  if (numericCount / sample.length > 0.8) return 'numeric'\n  if (dateCount / sample.length > 0.6) return 'date'\n  return 'text'\n}\n\n// Ê£ÄÊµãÊòØÂê¶‰∏∫ÊúâÊïàÊó•Êúü\nfunction isValidDate(value) {\n  const date = new Date(value)\n  return !isNaN(date.getTime()) && String(value).length > 6\n}\n\n// Ëé∑ÂèñÊñá‰ª∂ÂõæÊ†á\nfunction getFileIcon(fileName) {\n  const ext = fileName.split('.').pop().toLowerCase()\n  const iconMap = {\n    'xlsx': 'üìä',\n    'csv': 'üìã',\n    'json': 'üìÑ',\n    'txt': 'üìù'\n  }\n  return iconMap[ext] || 'üìÅ'\n}\n\n// Ê†ºÂºèÂåñÊñá‰ª∂Â§ßÂ∞è\nfunction formatFileSize(bytes) {\n  if (bytes === 0) return '0 Bytes'\n  const k = 1024\n  const sizes = ['Bytes', 'KB', 'MB', 'GB']\n  const i = Math.floor(Math.log(bytes) / Math.log(k))\n  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\n}\n\n// Ê®°ÊÄÅÊ°ÜÊãñÊãΩ\nfunction startDrag(event) {\n  if (event.target.closest('.close-btn') || event.target.closest('.column-item')) return\n  \n  isDragging.value = true\n  const rect = modalRef.value.getBoundingClientRect()\n  dragOffset.value = {\n    x: event.clientX - rect.left,\n    y: event.clientY - rect.top\n  }\n  \n  document.addEventListener('mousemove', handleDrag)\n  document.addEventListener('mouseup', stopDrag)\n  event.preventDefault()\n}\n\nfunction handleDrag(event) {\n  if (!isDragging.value) return\n  \n  const newX = event.clientX - dragOffset.value.x\n  const newY = event.clientY - dragOffset.value.y\n  \n  const maxX = window.innerWidth - 400\n  const maxY = window.innerHeight - 300\n  \n  modalPosition.value = {\n    position: 'fixed',\n    top: `${Math.max(0, Math.min(newY, maxY))}px`,\n    left: `${Math.max(0, Math.min(newX, maxX))}px`,\n    zIndex: 10000\n  }\n}\n\nfunction stopDrag() {\n  isDragging.value = false\n  document.removeEventListener('mousemove', handleDrag)\n  document.removeEventListener('mouseup', stopDrag)\n}\n\n// ÂàóÊãñÊãΩ\nfunction startColumnDrag(event, column) {\n  draggedColumn.value = column.name\n  \n  // ËÆæÁΩÆÊãñÊãΩÊï∞ÊçÆ\n  event.dataTransfer.setData('text/plain', JSON.stringify({\n    type: 'column',\n    fileName: props.fileInfo.name,\n    fileId: props.fileInfo.id,\n    columnName: column.name,\n    columnType: column.type,\n    columnData: column\n  }))\n  \n  event.dataTransfer.effectAllowed = 'copy'\n  \n  emit('column-drag', {\n    column,\n    fileInfo: props.fileInfo\n  })\n}\n\nfunction endColumnDrag() {\n  draggedColumn.value = null\n}\n\n// Ê∏ÖÁêÜ‰∫ã‰ª∂ÁõëÂê¨\nonUnmounted(() => {\n  document.removeEventListener('mousemove', handleDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n</script>\n\n<style scoped>\n.file-structure-modal {\n  position: fixed;\n  top: 50px;\n  left: 300px;\n  width: 400px;\n  max-height: 600px;\n  background: rgba(255, 255, 255, 0.95);\n  backdrop-filter: blur(20px);\n  border-radius: 12px;\n  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.15);\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  z-index: 10000;\n  overflow: hidden;\n  cursor: move;\n}\n\n[data-theme=\"dark\"] .file-structure-modal {\n  background: rgba(17, 24, 39, 0.95);\n  border: 1px solid rgba(55, 65, 81, 0.4);\n}\n\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 12px 16px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.08);\n  background: rgba(255, 255, 255, 0.5);\n}\n\n[data-theme=\"dark\"] .modal-header {\n  border-bottom: 1px solid rgba(255, 255, 255, 0.08);\n  background: rgba(17, 24, 39, 0.5);\n}\n\n.modal-title {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--text-color);\n}\n\n.file-icon {\n  font-size: 16px;\n}\n\n.file-name {\n  max-width: 200px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.column-count {\n  font-size: 12px;\n  color: var(--text-secondary);\n  background: rgba(59, 130, 246, 0.1);\n  padding: 2px 6px;\n  border-radius: 4px;\n}\n\n.close-btn {\n  background: none;\n  border: none;\n  font-size: 20px;\n  cursor: pointer;\n  color: var(--text-secondary);\n  padding: 4px;\n  border-radius: 4px;\n  transition: all 0.2s;\n}\n\n.close-btn:hover {\n 