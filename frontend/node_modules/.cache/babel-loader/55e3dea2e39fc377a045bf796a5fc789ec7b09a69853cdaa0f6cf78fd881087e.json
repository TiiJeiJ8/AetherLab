{"ast":null,"code":"import ChartThumbnail from './ChartThumbnail.vue';\nimport { ref, onMounted, onBeforeUnmount } from 'vue';\n// 拖拽浮窗相关\n\nexport default {\n  __name: 'ChartHistoryModal',\n  props: {\n    show: Boolean,\n    chartHistory: Array\n  },\n  emits: ['close', 'preview', 'delete'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n\n    // 点击浮窗空白关闭\n    function onMaskClick(e) {\n      // 只在点击最外层（即不是内容区）时关闭\n      if (e.target === e.currentTarget) {\n        onClose();\n      }\n    }\n    /* eslint-disable */\n    const modalStyle = ref({\n      left: '50vw',\n      top: '12vh'\n    });\n    const dragOffset = {\n      x: 0,\n      y: 0\n    };\n    let dragging = false;\n    function startDrag(e) {\n      if (e.button !== 0) return;\n      dragging = true;\n      const modal = document.querySelector('.history-modal-draggable');\n      const rect = modal.getBoundingClientRect();\n      dragOffset.x = e.clientX - rect.left;\n      dragOffset.y = e.clientY - rect.top;\n      document.addEventListener('mousemove', onDrag);\n      document.addEventListener('mouseup', stopDrag);\n    }\n    function onDrag(e) {\n      if (!dragging) return;\n      let left = e.clientX - dragOffset.x;\n      let top = e.clientY - dragOffset.y;\n      // 限制在窗口内\n      const minLeft = 20;\n      const minTop = 20;\n      const maxLeft = window.innerWidth - 480 - 20;\n      const maxTop = window.innerHeight - 420 - 20;\n      left = Math.max(minLeft, Math.min(left, maxLeft));\n      top = Math.max(minTop, Math.min(top, maxTop));\n      modalStyle.value = {\n        left: left + 'px',\n        top: top + 'px'\n      };\n    }\n    function stopDrag() {\n      dragging = false;\n      document.removeEventListener('mousemove', onDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    }\n    onMounted(() => {\n      // 初始居中\n      const w = 480;\n      const h = 420;\n      modalStyle.value = {\n        left: (window.innerWidth - w)