me=\"fade-slide\">\n    <div v-if=\"show\" class=\"history-modal\" @click.self=\"onClose\">\n    <div class=\"history-content\">\n        <div class=\"history-header\">\n        <span>Charts Storage</span>\n        <button class=\"close-btn\" @click=\"onClose\">×</button>\n        </div>\n        <div v-if=\"!chartHistory?.length\" class=\"empty-tip\">\n        No saved charts available.\n        </div>\n        <div class=\"history-list\">\n        <div\n            v-for=\"(item, idx) in validChartHistory\"\n            :key=\"item.title + '-' + idx\"\n            class=\"chart-history-item\"\n        >\n            <div class=\"thumb-title-row\">\n                <ChartThumbnail :option=\"item.option\" @click=\"onPreview(item)\" />\n                <input\n                  class=\"chart-history-title-input\"\n                  v-if=\"editIndex === idx\"\n                  v-model=\"editTitle\"\n                  @blur=\"saveTitle(idx)\"\n                  @keyup.enter=\"saveTitle(idx)\"\n                  @keyup.esc=\"cancelEditTitle\"\n                  @click.stop\n                  :ref=\"el => { if (editIndex === idx && el) editInputRef.value = el; else if (editIndex === idx && !el) editInputRef.value = null }\"\n                  maxlength=\"30\"\n                  style=\"text-align:center; width:90%; font-size:inherit; font-weight:500; border-radius:4px; border:1px solid #bbb; padding:2px 6px; margin:0 auto;\"\n                />\n                <div\n                  class=\"chart-history-title\"\n                  v-else\n                  @dblclick=\"startEditTitle(idx, item.title)\"\n                  :title=\"item.title\"\n                  style=\"cursor:pointer;\"\n                >\n                  {{ item.title }}\n                </div>\n                <button class=\"delete-btn\" @click.stop=\"confirmDelete(idx)\" title=\"Delete\">❌</button>\n            </div>\n        </div>\n        </div>\n    </div>\n    </div>\n</transition>\n<transition name=\"fade\">\n    <div v-if=\"showDeleteConfirm\" class=\"confirm-modal\" @click.self=\"cancelDelete\">\n    <div class=\"confirm-content\">\n        <h3>Confirmation</h3>\n        <p>Are you sure you want to delete this chart? This operation is irreversible.</p>\n        <div name=\"confirm-buttons\">\n        <button class=\"cancel-btn\" @click=\"cancelDelete\">Cancel</button>\n        <button class=\"confirm-btn\" @click=\"handleDelete\">Confirm</button>\n        </div>\n    </div>\n    </div>\n</transition>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport ChartThumbnail from './ChartThumbnail.vue'\nimport { ref, nextTick } from 'vue'\n// ...existing code...\nimport { computed } from 'vue'\n// ...existing code...\n\n// 过滤无效项，避免 v-for/v-if 混用\nconst validChartHistory = computed(() =>\n  Array.isArray(props.chartHistory)\n    ? props.chartHistory.filter(item => item && item.option && item.title)\n    : []\n)\n// 删除确认\nconst showDeleteConfirm = ref(false)\nlet deleteIndex = ref(null)\n\nfunction confirmDelete(idx) {\n    deleteIndex.value = idx\n    showDeleteConfirm.value = true\n}\n\nfunction cancelDelete() {\n    showDeleteConfirm.value = false\n    deleteIndex.value = null\n}\n\nfunction handleDelete() {\n    if (deleteIndex.value !== null) {\n        emit('delete', deleteIndex.value)\n        showDeleteConfirm.value = false\n        deleteIndex.value = null\n    }\n}\n\n// props\nconst props = defineProps({\n    show: Boolean,\n    chartHistory: Array\n})\nconst emit = defineEmits(['close', 'preview', 'delete'])\nfunction onClose() { emit('close') }\nfunction onPreview(item) { emit('preview', item) }\nfunction onDelete(idx) { emit('delete', idx) }\n\n// 重命名相关\nconst editIndex = ref(null)\nconst editTitle = ref('')\nconst editInputRef = ref(null)\n\nfunction startEditTitle(idx, title) {\n    editIndex.value = idx\n    editTitle.value = title\n    nextTick(() => {\n        // 防止弹窗已关闭或input已销毁\n        if (editInputRef.value && props.show) {\n            try {\n                editInputRef.value.focus()\n                editInputRef.value.select()\n            } catch (e) {}\n        }\n    })\n}\nfunction saveTitle(idx) {\n    if (editIndex.value !== null && editTitle.value.trim()) {\n        emit('rename', { idx, title: editTitle.value.trim() })\n    }\n    editIndex.value = null\n    editTitle.value = ''\n    editInputRef.value = null\n}\nfunction cancelEditTitle() {\n    editIndex.value = null\n    editTitle.value = ''\n    editInputRef.value = null\n}\n</script>\n\n<style scoped>\n.history-modal {\n    position: fixed;\n    top: 0; left: 0; right: 0; bottom: 0;\n    background: rgba(0,0,0,0.18);\n    z-index: 9999;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n.history-content {\n    width: min(90vw, 480px);\n    max-height: min(90vh, 600px);\n    background: var(--bg-color, #fff);\n    border-radius: 12px;\n    box-shadow: 0 8px 32px rgba(0,0,0,0.18);\n    padding: clamp(12px, 2vw, 18px);\n    overflow-y: auto;\n    animation: pop-in 0.4s;\n}\n.history-header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    font-weight: bold;\n    font-size: 1.1em;\n    margin-bottom: 10px;\n}\n.close-btn {\n    background: none;\n    border: none;\n    font-size: 1.5em;\n    cursor: pointer;\n    color: var(--text-color, #333);\n    line-height: 1;\n}\n.history-list {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));\n    gap: clamp(8px, 1.5vw, 16px);\n    padding: 4px;\n}\n.chart-history-item {\n    background: var(--bg-color);\n    border-radius: 8px;\n    padding: clamp(6px, 1vw, 10px);\n    box-shadow: 0 1px 4px rgba(0,0,0,0.04);\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    position: relative;\n    transition: transform 0.2s, box-shadow 0.2s;\n}\n.chart-history-item:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 1px 1px rgba(58, 57, 57, 0.88);\n}\n.thumb-title-row {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    gap: 8px;\n    width: 100%;\n}\n.chart-history-title {\n    font-size: clamp(0.8em, 1.5vw, 1em);\n    font-weight: 500;\n    color: var(--text-color);\n    text-align: center;\n    width: 100%;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n}\n.delete-btn {\n    position: absolute;\n    top: 4px;\n    right: 4px;\n    background: var(--text-color);\n    border: none;\n    border-radius: 50%;\n    width: 24px;\n    height: 24px;\n    font-size: 12px;\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    opacity: 0;\n    transition: opacity 0.2s;\n}\n.chart-history-item:hover .delete-btn {\n    opacity: 1;\n}\n@media (max-width: 480px) {\n    .history-header {\n        font-size: 1em;\n        padding: 8px 0;\n    }\n\n    .delete-btn {\n        opacity: 1; /* 在移动端始终显示删除按钮 */\n    }\n}\n\n.fade-slide-enter-active, .fade-slide-leave-active {\n    transition: opacity 0.3s, transform 0.3s;\n}\n.fade-slide-enter-from, .fade-slide-leave-to {\n    opacity: 0;\n    transform: translateY(-30px);\n}\n@keyframes pop-in {\n    from { opacity: 0; transform: scale(0.95);}\n    to { opacity: 1; transform: scale(1);}\n}\n\n/* 确认弹窗样式 */\n.confirm-modal {\n    position: fixed;\n    top: 0; left: 0; right: 0; bottom: 0;\n    background: rgba(0,0,0,0.5);\n    z-index: 10000;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.confirm-content {\n    background: var(--bg-color, #fff);\n    border-radius: 8px;\n    padding: 20px;\n    width: min(90vw, 300px);\n    text-align: center;\n    box-shadow: 0 4px 16px rgba(0,0,0,0.2);\n}\n\n.confirm-content h3 {\n    margin: 0 0 12px;\n    color: #333;\n    font-size: 1.2em;\n}\n\n.confirm-content p {\n    margin: 0 0 20px;\n    color: #666;\n    font-size: 0.95em;\n}\n\n.confirm-buttons {\n    display: flex;\n    gap: 12px;\n    justify-content: center;\n}\n\n.confirm-btn, .cancel-btn {\n    padding: 8px 16px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    font-size: 0.9em;\n    transition: opacity 0.2s;\n}\n\n.cancel-btn {\n    background: #f0f0f0;\n    color: #666;\n}\n\n.confirm-btn {\n    background: #dc3545;\n    color: white;\n}\n\n.confirm-btn:hover, .cancel-btn:hover {\n    opacity: 0.8;\n}\n\n/* 淡入淡出动画 */\n.fade-enter-active, .fade-leave-active {\n    transition: opacity 0.2s;\n}\n\n.fade-enter-from, .fade-leave-to {\n    opacity: 0;\n}\n\n.empty-tip {\n    text-align: center;\n    color: #999;\n    padding: 40px 20px;\n    font-size: 1.1em;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    min-height: 200px;\n    background: #f8f9fa;\n    border-radius: 8px;\n    margin: 10px 0;\n    user-select: none;\n}\n\n@media (max-width: 480px) {\n    .empty-tip {\n        min-height: 150px;\n        padding: 20px;\n        font-size: 1em;\n    }\n}\n</style>"],"mappings":";;EAISA,KAAK,EAAC;AAAiB;;EAJhCC,GAAA;EAS0CD,KAAK,EAAC;;;EAGnCA,KAAK,EAAC;AAAc;;EAMhBA,KAAK,EAAC;AAAiB;mBAlBxC;mBAAA;mBAAA;;uBACAE,mBAAA,CA2DM,cA1DNC,YAAA,CA6CaC,WAAA;IA7CDC,IAAI,EAAC;EAAY;IAF7BC,OAAA,EAAAC,QAAA,CAGI,MA2CM,CA3CKC,MAAA,CAAAC,IAAI,I,cAAfP,mBAAA,CA2CM;MA9CVD,GAAA;MAGqBD,KAAK,EAAC,eAAe;MAAEU,OAAK,EAHjDC,cAAA,CAGwDC,MAAA,CAAAC,OAAO;QAC3DC,mBAAA,CAyCM,OAzCNC,UAyCM,GAxCFD,mBAAA,CAGM;MAHDd,KAAK,EAAC;IAAgB,I,0BAC3Bc,mBAAA,CAA2B,cAArB,gBAAc,sBACpBA,mBAAA,CAAqD;MAA7Cd,KAAK,EAAC,WAAW;MAAEU,OAAK,EAAEE,MAAA,CAAAC;OAAS,GAAC,E,IAEhCL,MAAA,CAAAQ,YAAY,EAAEC,MAAM,I,cAAhCf,mBAAA,CAEM,OAFNgB,UAEM,EAF8C,8BAEpD,KAXRC,mBAAA,gBAYQL,mBAAA,CAgCM,OAhCNM,UAgCM,I,kBA/BNlB,mBAAA,CA8BMmB,SAAA,QA3CdC,WAAA,CAckCV,MAAA,CAAAW,iBAAiB,EAdnD,CAcoBC,IAAI,EAAEC,GAAG;2BADrBvB,mBAAA,CA8BM;QA5BDD,GAAG,EAAEuB,IAAI,CAACE,KAAK,SAASD,GAAG;QAC5BzB,KAAK,EAAC;UAENc,mBAAA,CAwBM,OAxBNa,UAwBM,GAvBFxB,YAAA,CAAiES,MAAA;QAAhDgB,MAAM,EAAEJ,IAAI,CAACI,MAAM;QAAGlB,OAAK,EAAAmB,MAAA,IAAEjB,MAAA,CAAAkB,SAAS,CAACN,IAAI;sD