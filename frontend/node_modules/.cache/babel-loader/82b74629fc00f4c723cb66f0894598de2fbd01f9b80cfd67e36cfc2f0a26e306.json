 onClear: $setup.handleWorkspaceClear,\n    onShowStructure: $setup.handleShowStructure\n  }, null, 8 /* PROPS */, [\"files\"]), _createCommentVNode(\" 文件结构面板 \"), $setup.showFileStructure ? (_openBlock(), _createElementBlock(\"div\", {\n    key: 2,\n    class: \"file-structure-overlay\",\n    onClick: _cache[5] || (_cache[5] = $event => $setup.showFileStructure = false)\n  }, [_createVNode($setup[\"FileStructurePanel\"], {\n    files: $setup.workspaceFiles,\n    visible: $setup.showFileStructure,\n    onClose: _cache[3] || (_cache[3] = $event => $setup.showFileStructure = false),\n    onPreviewFile: $setup.handleFilePreview,\n    onColumnDragStart: $setup.handleColumnDragStart,\n    onColumnDragEnd: $setup.handleColumnDragEnd,\n    onClick: _cache[4] || (_cache[4] = _withModifiers(() => {}, [\"stop\"]))\n  }, null, 8 /* PROPS */, [\"files\", \"visible\"])])) : _createCommentVNode(\"v-if\", true), _createCommentVNode(\" 主题切换按钮 \"), _createVNode($setup[\"ThemeButton\"])], 64 /* STABLE_FRAGMENT */);\n}","map":{"version":3,"names":["class","_createElementBlock","_Fragment","_createElementVNode","_hoisted_1","_createVNode","$setup","actions","UploadFiles","DataPreview","openHistory","toggleFileStructure","_createCommentVNode","position","collapsedWidth","expandedWidth","default","_withCtx","onSelect","onChartTypeSelect","_","selectedChartType","workspaceFiles","onChartUpdated","handleChartUpdate","onMappingChanged","handleMappingChanged","_hoisted_2","_hoisted_3","chartOption","_createBlock","key","option","show","showHistory","chartHistory","onClose","_cache","$event","onPreview","previewChart","onDelete","deleteChart","showFileUpload","onWorkspaceUpdated","handleWorkspaceUpdate","showDataPreview","currentDataFile","previewData","files","onRemove","handleWorkspaceRemove","handleWorkspacePreview","onClear","handleWorkspaceClear","onShowStructure","handleShowStructure","showFileStructure","onClick","visible","onPreviewFile","handleFilePreview","onColumnDragStart","handleColumnDragStart","onColumnDragEnd","handleColumnDragEnd","_withModifiers"],"sources":["D:\\Learning Material\\Git\\Fuck-Charts\\newArchitecture\\frontend_vue_design\\src\\views\\ChartVisualizations.vue"],"sourcesContent":["<template>\n<div class=\"py-5\">\n  <TopBar :actions=\"[\n    { type: 'button', label: 'Files', onClick: UploadFiles },\n    { type: 'button', label: 'Data Preview', onClick: DataPreview},\n    { type: 'button', label: 'Chart History', onClick: openHistory},\n    { type: 'button', label: 'File Structure', onClick: toggleFileStructure},\n    { type: 'button', label: 'Dashboard', },\n    { type: 'button', label: 'Instruction', },\n    { type: 'button', label: 'Back2Home', to: '/'},\n  ]\"/>\n</div>\n\n<!-- 左侧边栏 -->\n<SideBar\nposition=\"left\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"180\">\n  <ChartTypeSelector @select=\"onChartTypeSelect\" />\n</SideBar>\n\n<!-- 右侧边栏 -->\n<SideBar\nposition=\"right\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"400\">\n  <DataMappingPanel\n    :selected-chart-type=\"selectedChartType\"\n    :workspace-files=\"workspaceFiles\"\n    @chart-updated=\"handleChartUpdate\"\n    @mapping-changed=\"handleMappingChanged\"\n  />\n</SideBar>\n\n<!-- 绘制区 -->\n<div class=\"main-content\">\n  <section class=\"chart-workspace\">\n    <ChartDisplay v-if=\"chartOption\" :option=\"chartOption\" />\n  </section>\n</div>\n\n<!-- 图表存储区 -->\n<ChartHistoryModal\n  :show=\"showHistory\"\n  :chartHistory=\"chartHistory\"\n  @close=\"showHistory = false\"\n  @preview=\"previewChart\"\n  @delete=\"deleteChart\"\n/>\n\n<!-- 文件上传弹窗 -->\n<FileUploadModal\n  v-if=\"showFileUpload\"\n  :show=\"showFileUpload\"\n  :workspaceFiles=\"workspaceFiles\"\n  @close=\"showFileUpload = false\"\n  @workspace-updated=\"handleWorkspaceUpdate\"\n/>\n\n<!-- 数据预览弹窗 -->\n<DataPreviewModal\n  v-if=\"showDataPreview\"\n  :show=\"showDataPreview\"\n  :current-file=\"currentDataFile\"\n  :preview-data=\"previewData\"\n  @close=\"showDataPreview = false\"\n/>\n\n<!-- 文件工作区 -->\n<FileWorkspace\n  :files=\"workspaceFiles\"\n  @remove=\"handleWorkspaceRemove\"\n  @preview=\"handleWorkspacePreview\"\n  @clear=\"handleWorkspaceClear\"\n  @show-structure=\"handleShowStructure\"\n/>\n\n<!-- 文件结构面板 -->\n<div v-if=\"showFileStructure\" class=\"file-structure-overlay\" @click=\"showFileStructure = false\">\n  <FileStructurePanel\n    :files=\"workspaceFiles\"\n    :visible=\"showFileStructure\"\n    @close=\"showFileStructure = false\"\n    @preview-file=\"handleFilePreview\"\n    @column-drag-start=\"handleColumnDragStart\"\n    @column-drag-end=\"handleColumnDragEnd\"\n    @click.stop\n  />\n</div>\n\n<!-- 主题切换按钮 -->\n<ThemeButton />\n</template>\n\n<script setup>\n/* eslint-disable */\nimport TopBar from '../components/TopBar.vue'\nimport SideBar from '../components/SideBar.vue'\nimport ChartTypeSelector from '../components/chartIcons.vue'\nimport ChartDisplay from '../components/ChartDisplay.vue'\nimport ChartHistoryModal from '../components/ChartHistoryModal.vue'\nimport FileUploadModal from '../components/FileUploadModal.vue'\nimport DataPreviewModal from '../components/DataPreviewModal.vue'\nimport FileWorkspace from '../components/FileWorkspace.vue'\nimport DataMappingPanel from '../components/DataMappingPanel.vue'\nimport FileStructurePanel from '../components/FileStructurePanel.vue'\nimport ThemeButton from '../components/ThemeButton.vue'\nimport { getFilePreview } from '../services/FileServices.js'\nimport { useTheme } from '../composables/useTheme.js'\nimport { useFileStructure } from '../composables/useFileStructure.js'\nimport { useDataFiltering } from '../composables/useDataFiltering.js'\nimport { ref, nextTick, onMounted } from 'vue'\n\n// 主题管理\nconst { isDarkMode, toggleTheme, getThemeIcon, initTheme } = useTheme()\n\n// 文件结构管理\nconst { analyzeFileStructure, mergeFilesData } = useFileStructure()\n\n// 数据过滤\nconst { setRawData } = useDataFiltering()\n\n// 组件状态\nconst selectedChartType = ref('line')\nconst showFileStructure = ref(false)\nconst currentMappingData = ref({})\nconst isDragging = ref(false)\n\n// 文件上传相关\nconst showFileUpload = ref(false)\nfunction UploadFiles() {\n  showFileUpload.value = true\n}\n\n// 数据预览相关\nconst showDataPreview = ref(false)\nconst currentDataFile = ref(null)\nconst previewData = ref([])\n\n// 工作区相关\nconst workspaceFiles = ref([])\n\nfunction DataPreview() {\n  // 这里可以根据当前图表获取对应的数据文件信息\n  currentDataFile.value = {\n    name: 'currentFile.csv'\n  }\n  // 这里可以获取当前图表的数据用于预览\n  previewData.value = [\n    { column1: 'A', column2: 100 },\n    { column1: 'B', column2: 200 },\n    { column1: 'C', column2: 300 }\n  ]\n  showDataPreview.value = true\n}\n\n// 测试图表绘制\nconst chartOption = ref({\n  title: { text: '示例图表' },\n  tooltip: {},\n  xAxis: { data: ['A', 'B', 'C', 'D'] },\n  yAxis: {},\n  series: [{ type: 'line', data: [23, 45, 12, 37] }]\n})\n\n// 图表存储区（可从后端获取）\nconst chartHistory = ref([\n  {\n    title: '销售柱状图',\n    option: {\n      title: { text: '销售柱状图' },\n      tooltip: {},\n      xAxis: { data: ['一季度', '二季度', '三季度', '四季度'] },\n      yAxis: {},\n      series: [{ type: 'bar', data: [120, 200, 150, 80] }]\n    }\n  },\n  {\n    title: '折线趋势',\n    option: {\n      title: { text: '折线趋势' },\n      tooltip: {},\n      xAxis: { data: ['1月', '2月', '3月', '4月'] },\n      yAxis: {},\n      series: [{ type: 'line', data: [30, 50, 80, 65] }]\n    }\n  },\n  {\n    title: '饼图示例',\n    option: {\n      title: { text: '饼图示例' },\n      tooltip: {},\n      series: [\n        {\n          type: 'pie',\n          radius: '60%',\n          data: [\n            { value: 40, name: 'A类' },\n            { value: 32, name: 'B类' },\n            { value: 28, name: 'C类' }\n          ]\n        }\n      ]\n    }\n  }\n])\n\nconst showHistory = ref(false)\nfunction openHistory() {\n  showHistory.value = true\n}\nfunction previewChart(item) {\n  chartOption.value = null\n  nextTick(() => {\n    chartOption.value = JSON.parse(JSON.stringify(item.option))\n    showHistory.value = false\n  })\n}\nfunction deleteChart(idx) {\n  chartHistory.value.splice(idx, 1)\n}\n\n// 图表选择\nfunction onChartTypeSelect(type) {\n  console.log(`Selected chart type: ${type}`)\n  selectedChartType.value = type\n  \n  // 根据选择的图表类型更新示例图表\n  updateExampleChart(type)\n}\n\n// 更新示例图表\nfunction updateExampleChart(type) {\n  const exampleData = {\n    line: {\n      title: { text: '折线图示例' },\n      tooltip: {},\n      xAxis: { data: ['1月', '2月', '3月', '4月', '5月', '6月'] },\n      yAxis: {},\n      series: [{ type: 'line', data: [23, 45, 56, 78, 32, 46], smooth: true }]\n    },\n    bar: {\n      title: { text: '柱状图示例' },\n      tooltip: {},\n      xAxis: { data: ['A', 'B', 'C', 'D', 'E'] },\n      yAxis: {},\n      series: [{ type: 'bar', data: [120, 200, 150, 80, 70] }]\n    },\n    pie: {\n      title: { text: '饼图示例' },\n      tooltip: { trigger: 'item' },\n      series: [{\n        type: 'pie',\n        radius: '60%',\n        data: [\n          { value: 40, name: 'A类' },\n          { value: 32, name: 'B类' },\n          { value: 28, name: 'C类' },\n          { value: 20, name: 'D类' }\n        ]\n      }]\n    },\n    scatter: {\n      title: { text: '散点图示例' },\n      tooltip: {},\n      xAxis: { scale: true },\n      yAxis: { scale: true },\n      series: [{\n        type: 'scatter',\n        data: [[10, 20], [15, 25], [20, 30], [25, 35], [30, 40]]\n      }]\n    },\n    radar: {\n      title: { text: '雷达图示例' },\n      tooltip: {},\n      radar: {\n        indicator: [\n          { name: '销售', max: 100 },\n          { name: '管理', max: 100 },\n          { name: '技术', max: 100 },\n          { name: '客服', max: 100 },\n          { name: '研发', max: 100 }\n        ]\n      },\n      series: [{\n        type: 'radar',\n        data: [{ value: [80, 90, 70, 85, 75], name: '预算分配' }]\n      }]\n    }\n  }\n  \n  chartOption.value = exampleData[type] || exampleData.line\n}\n\n// 文件结构面板\nfunction toggleFileStructure() {\n  showFileStructure.value = !showFileStructure.value\n}\n\n// 工作区相关方法\nasync function handleWorkspaceUpdate(files) {\n  workspaceFiles.value = [...files]\n  console.log('Workspace updated:', workspaceFiles.value.length, 'files')\n  \n  // 自动分析文件结构\n  for (const file of workspaceFiles.value) {\n    if (!file.columns) {\n      try {\n        await analyzeFileStructure(file)\n      } catch (error) {\n        console.error('Failed to analyze file:', file.name, error)\n      }\n    }\n  }\n  \n  // 更新合并数据\n  updateMergedData()\n}\n\nfunction handleWorkspaceRemove(file) {\n  console.log('File removed from workspace:', file.name)\n  // 从工作区数组中移除文件\n  const index = workspaceFiles.value.findIndex(f => f.id === file.id)\n  if (index !== -1) {\n    workspaceFiles.value.splice(index, 1)\n  }\n  updateMergedData()\n}\n\nfunction handleWorkspacePreview(file) {\n  // 预览工作区中的文件\n  console.log('Preview workspace file:', file.name)\n  currentDataFile.value = file\n  \n  // 使用与 FileUploadModal 相同的预览逻辑\n  if (file.previewData) {\n    // 如果文件已经有预览数据，直接使用\n    previewData.value = file.previewData\n    showDataPreview.value = true\n  } else {\n    // 否则读取文件内容\n    loadFilePreview(file)\n  }\n}\n\n// 加载文件预览数据\nasync function loadFilePreview(file) {\n  try {\n    const previewResult = await getFilePreview(file.id)\n    previewData.value = previewResult.data\n    showDataPreview.value = true\n  } catch (error) {\n    console.error('Preview failed:', error)\n    alert('Preview failed: ' + error.message)\n  }\n}\n\nfunction handleWorkspaceClear() {\n  console.log('Workspace cleared')\n  workspaceFiles.value = []\n  updateMergedData()\n}\n\n// 处理显示文件结构\nfunction handleShowStructure(files) {\n  showFileStructure.value = true\n}\n\n// 文件预览处理\nfunction handleFilePreview(file) {\n  handleWorkspacePreview(file)\n}\n\n// 拖拽处理\nfunction handleColumnDragStart(columnData) {\n  isDragging.value = true\n  console.log('Column drag started:', columnData)\n}\n\nfunction handleColumnDragEnd() {\n  isDragging.value = false\n  console.log('Column drag ended')\n}\n\n// 图表更新处理\nfunction handleChartUpdate(option) {\n  chartOption.value = null\n  nextTick(() => {\n    chartOption.value = option\n  })\n}\n\n// 映射变化处理\nfunction handleMappingChanged(mappingInfo) {\n  currentMappingData.value = mappingInfo\n  console.log('Mapping changed:', mappingInfo)\n}\n\n// 更新合并数据\nfunction updateMergedData() {\n  if (workspaceFiles.value.length === 0) {\n    setRawData([])\n    return\n  }\n  \n  try {\n    const mergedData = mergeFilesData(workspaceFiles.value, currentMappingData.value.mappedData || {})\n    setRawData(mergedData)\n  } catch (error) {\n    console.error('Failed to merge data:', error)\n    setRawData([])\n  }\n}\n\n// 初始化\nonMounted(() => {\n  initTheme()\n})\n</script>\n\n<style scoped>\n.sideBar-btn {\nbackground: transparent;\nborder: none;\nfont-size: 1.1rem;\nfont-weight: 600;\ncursor: pointer;\ncolor: var(--text-color);\npadding: 6px 14px;\nborder-radius: 8px;\ntransition:\n    background-color 0.3s cubic-bezier(.4,2,.6,1),\n    transform 0.2s cubic-bezier(.4,2,.6,1),\n    box-shadow 0.2s;\ntext-align: center;\ntext-decoration: none;\n}\n.sideBar-btn:hover {\nbackground-color: rgba(136, 133, 133, 0.1);\ntransform: translateY(-2px) scale(1.02);\nbox-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n/* 图表绘制区 */\n.main-content {\ndisplay: flex;\ngap: 32px;\nmargin: 40px 60px 0 220px;\nmin-height: 500px;\n}\n@media (max-width: 767px) {\n  .main-content {\n    margin: 20px 30px 0 80px;\n    gap: 16px;\n    min-height: auto;\n  }\n}\n.chart-workspace {\nflex: 1;\nmin-width: 0;\ndisplay: flex;\nalign-items: center;\njustify-content: center;\n}\n\n/* 图表缓存区 */\n.chart-history {\nwidth: 220px;\nbackground: var(--bg-color, #fff);\nborder-radius: 10px;\nbox-shadow: 0 0px 1px var(--text-color);\npadding: 16px;\nflex-shrink: 0;\n}\n.chart-history-item {\npadding: 8px 0;\ncursor: pointer;\nborder-bottom: 1px solid #747272;\ntransition: background 0.2s;\n}\n.chart-history-item:hover {\nbackground: rgba(136, 133, 133, 0.1);\n}\n\n/* 主题切换按钮 */\n.theme-toggle-btn {\nposition: fixed;\ntop: 20px;\nright: 20px;\nwidth: 40px;\nheight: 40px;\nborder: none;\nborder-radius: 50%;\nbackground: var(--bg-secondary);\ncolor: var(--text-color);\ncursor: pointer;\ndisplay: flex;\nalign-items: center;\njustify-content: center;\nbox-shadow: var(--shadow);\ntransition: all 0.3s ease;\nz-index: 1000;\n}\n\n.theme-toggle-btn:hover {\nbackground: var(--bg-hover);\ntransform: scale(1.05);\n}\n\n/* 拖拽状态样式 */\n.dragging {\ncursor: grabbing;\n}\n\n/* 响应式适配 */\n@media (max-width: 1024px) {\n  .main-content {\n    margin: 20px 20px 0 60px;\n  }\n}\n\n@media (max-width: 768px) {\n  .theme-toggle-btn {\n    top: 10px;\n    right: 10px;\n    width: 36px;\n    height: 36px;\n  }\n  \n  .main-content {\n    margin: 15px 15px 0 50px;\n  }\n}\n\n/* 暗黑模式特定样式 */\n:global(.theme-dark) {\n  background: var(--bg-color);\n  color: var(--text-color);\n}\n\n:global(.theme-dark) .chart-workspace {\n  background: var(--bg-color);\n}\n\n/* 动画效果 */\n@keyframes fadeIn {\n  from {\n    opacity: 0;\n    transform: translateY(20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n.chart-workspace {\n  animation: fadeIn 0.3s ease-out;\n}\n\n/* 拖拽反馈 */\n.drag-feedback {\n  position: fixed;\n  pointer-events: none;\n  z-index: 1001;\n  background: var(--primary-color);\n  color: white;\n  padding: 4px 8px;\n  border-radius: 4px;\n  font-size: 0.8rem;\n  opacity: 0.8;\n}\n\n/* 高对比度模式支持 */\n@media (prefers-contrast: high) {\n  .sideBar-btn {\n    border: 1px solid var(--border-color);\n  }\n  \n  .theme-toggle-btn {\n    border: 2px solid var(--border-color);\n  }\n}\n\n/* 动画优化 */\n@media (prefers-reduced-motion: reduce) {\n  * {\n    animation-duration: 0.01ms !important;\n    animation-iteration-count: 1 !important;\n    transition-duration: 0.01ms !important;\n  }\n}\n</style>\n"],"mappings":";;EACKA,KAAK,EAAC;AAAM;;EAkCZA,KAAK,EAAC;AAAc;;EACdA,KAAK,EAAC;AAAiB;;uBApClCC,mBAAA,CAAAC,SAAA,SACAC,mBAAA,CAUM,OAVNC,UAUM,GATJC,YAAA,CAQIC,MAAA;IARKC,O