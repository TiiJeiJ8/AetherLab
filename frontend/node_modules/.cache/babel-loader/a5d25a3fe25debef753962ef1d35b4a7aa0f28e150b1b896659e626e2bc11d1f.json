id=\"sunburstCenter\" cx=\"50%\" cy=\"50%\" r=\"50%\">\n                        <stop offset=\"0%\" stop-color=\"#fffbe6\"/>\n                        <stop offset=\"100%\" stop-color=\"#FFD700\"/>\n                    </radialGradient>\n                    <linearGradient id=\"arc1\" x1=\"0\" y1=\"0\" x2=\"1\" y2=\"1\">\n                        <stop offset=\"0%\" stop-color=\"#4F8EF7\"/>\n                        <stop offset=\"100%\" stop-color=\"#A0C4FF\"/>\n                    </linearGradient>\n                    <linearGradient id=\"arc2\" x1=\"0\" y1=\"0\" x2=\"1\" y2=\"1\">\n                        <stop offset=\"0%\" stop-color=\"#FFB300\"/>\n                        <stop offset=\"100%\" stop-color=\"#FFD700\"/>\n                    </linearGradient>\n                    <linearGradient id=\"arc3\" x1=\"0\" y1=\"0\" x2=\"1\" y2=\"1\">\n                        <stop offset=\"0%\" stop-color=\"#A0C4FF\"/>\n                        <stop offset=\"100%\" stop-color=\"#4F8EF7\"/>\n                    </linearGradient>\n                </defs>\n                <!-- 外层旭日环（3个扇形，动画旋转） -->\n                <path class=\"arc arc1\" d=\"M45 10 A35 35 0 0 1 80 45\" stroke=\"url(#arc1)\" stroke-width=\"12\" fill=\"none\"/>\n                <path class=\"arc arc2\" d=\"M80 45 A35 35 0 0 1 45 80\" stroke=\"url(#arc2)\" stroke-width=\"12\" fill=\"none\"/>\n                <path class=\"arc arc3\" d=\"M45 80 A35 35 0 0 1 10 45\" stroke=\"url(#arc3)\" stroke-width=\"12\" fill=\"none\"/>\n                <path class=\"arc arc4\" d=\"M10 45 A35 35 0 0 1 45 10\" stroke=\"#FFB300\" stroke-width=\"12\" fill=\"none\"/>\n                <!-- 内层旭日环（静态） -->\n                <circle cx=\"45\" cy=\"45\" r=\"20\" stroke=\"#A0C4FF\" stroke-width=\"6\" fill=\"none\" opacity=\"0.5\"/>\n                <!-- 中心圆（跳动） -->\n                <circle class=\"sunburst-center\" cx=\"45\" cy=\"45\" r=\"10\" fill=\"url(#sunburstCenter)\" />\n            </svg>\n        </div>\n        <div class=\"empty-title\">No Chart Data</div>\n        <div class=\"empty-desc\">Select a chart type on the left and configure your data to <span class=\"faststart\">get started fast</span> with visualization!</div>\n    </div>\n    <div v-else ref=\"chartRef\" class=\"chart-container\"></div>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, watch, onMounted, onBeforeUnmount, computed } from 'vue'\nimport * as echarts from 'echarts'\n\nconst props = defineProps({\n    option: { type: Object, required: true }\n})\n\nconst chartRef = ref(null)\nlet chartInstance = null\n\nconst hasSeries = computed(() => {\n    return props.option && Array.isArray(props.option.series) && props.option.series.length > 0\n})\n\nfunction renderChart() {\n    if (!hasSeries.value) return\n    if (!chartInstance && chartRef.value) {\n        chartInstance = echarts.init(chartRef.value)\n    }\n    if (chartInstance && props.option) {\n        chartInstance.setOption(props.option, true)\n    }\n}\n\nwatch(() => props.option, renderChart, { deep: true })\nonMounted(renderChart)\nonBeforeUnmount(() => {\n    if (chartInstance) chartInstance.dispose()\n})\n\n// 启动动画\nconst sunburstStarted = ref(false)\nonMounted(() => {\n    setTimeout(() => {\n        sunburstStarted.value = true\n    }, 1200)\n})\n</script>\n\n<style scoped>\n.chart-display {\n    width: 100%;\n    height: 100%;\n    min-height: clamp(300px, 60vh, 600px);\n    background: var(--bg-color, #fff);\n    border-radius: clamp(8px, 2vw, 12px);\n    box-shadow: 0 0 1px 1px var(--text-color);\n    position: relative;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    padding: clamp(10px, 2vw, 20px);\n}\n\n.chart-container {\n    width: 100% !important;\n    height: 100% !important;\n    min-height: inherit;\n}\n\n.empty-tip {\n    color: #888;\n    text-align: center;\n    letter-spacing: 1px;\n    padding: 24px 10px 10px 10px;\n    user-select: none;\n    background: none;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n}\n.empty-svg {\n    margin-bottom: 12px;\n    opacity: 0.96;\n    animation: floatX 3.2s ease-in-out infinite;\n    will-change: transform;\n}\n.sunburst-svg .arc {\n    stroke-linecap: round;\n    opacity: 0.85;\n    transform-origin: 45px 45px;\n    animation: arc-rotate 2.2s cubic-bezier(.4,1.6,.6,1) infinite;\n}\n.sunburst-svg .arc1 { animation-delay: 0s; }\n.sunburst-svg .arc2 { animation-delay: 0.3s; }\n.sunburst-svg .arc3 { animation-delay: 0.6s; }\n.sunburst-svg .arc4 { animation-delay: 0.9s; stroke: #FFD700; opacity: 0.7; }\n.sunburst-svg .sunburst-center {\n    animation: sunburst-pulse 1.6s cubic-bezier(.4,1.6,.6,1) infinite;\n    filter: drop-shadow(0 0 6px #FFD70088);\n}\n@keyframes arc-rotate {\n    0% { transform: rotate(0deg); opacity: 0.5; }\n    40% { opacity: 1; }\n    60% { opacity: 1; }\n    100% { transform: rotate(360deg); opacity: 0.5; }\n}\n@keyframes sunburst-pulse {\n    0% { r: 10; opacity: 1; }\n    50% { r: 13; opacity: 0.8; }\n    100% { r: 10; opacity: 1; }\n}\n@keyframes floatX {\n    0% { transform: translateY(0) translateX(0); }\n    25% { transform: translateY(-8px) translateX(4px); }\n    50% { transform: translateY(-12px) translateX(-4px); }\n    75% { transform: translateY(-8px) translateX(4px); }\n    100% { transform: translateY(0) translateX(0); }\n}\n.empty-title {\n    font-size: 1.25em;\n    font-weight: bold;\n    color: #4F8EF7;\n    margin-bottom: 6px;\n}\n.empty-desc {\n    font-size: 1em;\n    color: #666;\n    margin-top: 2px;\n}\n.faststart {\n    color: #FFB300;\n    font-weight: bold;\n}\n\n/* 小屏幕适配 */\n@media (max-width: 767px) {\n    .chart-display {\n        height: 100%;\n        padding: 8px;\n        border-radius: 8px;\n    }\n    .empty-tip {\n        font-size: 0.95em;\n        padding: 10px 2px 6px 2px;\n    }\n    .empty-title {\n        font-size: 1.05em;\n    }\n    .empty-desc {\n        font-size: 0.95em;\n    }\n    .empty-svg svg {\n        width: 44px;\n        height: 44px;\n    }\n}\n</style>\n"],"mappings":";;EACKA,KAAK,EAAC;AAAe;;EAD1BC,GAAA;EAE2BD,KAAK,EAAC;;;EAFjCC,GAAA;EAqCgBC,GAAG,EAAC,UAAU;EAACF,KAAK,EAAC;;;uBApCrCG,mBAAA,CAqCM,OArCNC,UAqCM,G,CApCUC,MAAA,CAAAC,SAAS,I,cAArBH,mBAAA,CAkCM,OAlCNI,UAkCM,EAAAC,MAAA,QAAAA,MAAA,OApCVC,kBAAA,gtE,qBAqCIN,mBAAA,CAAyD,OAAzDO,UAAyD,+B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}