{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, computed, watch } from 'vue';\nimport { getChartConfig, CHART_TYPES } from '../configs/chartConfigs.js';\nimport FieldMapping from './FieldMapping.vue';\nimport CustomConfigItem from './CustomConfigItem.vue';\n\n// Props\n\nexport default {\n  __name: 'ChartConfigPanel',\n  props: {\n    selectedChartType: {\n      type: String,\n      default: CHART_TYPES.BAR\n    },\n    currentFile: {\n      type: Object,\n      default: null\n    }\n  },\n  emits: ['config-change', 'generate-chart'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    const props = __props;\n\n    // Emits\n    const emit = __emit;\n\n    // 响应式数据\n    const showBasicConfig = ref(true);\n    const showCustomConfig = ref(false);\n    const showDataFilter = ref(false);\n    const errorMessage = ref('');\n\n    // 组件引用\n    const fieldMappingRef = ref(null);\n\n    // 图表配置\n    const chartConfig = ref({\n      fieldMappings: {},\n      basicOptions: {\n        title: '',\n        colorScheme: 'default',\n        animation: true,\n        showLegend: true,\n        showGrid: true\n      },\n      customOptions: {},\n      filterOptions: {\n        dataRange: 'all',\n        nullHandling: 'ignore',\n        limit: null\n      }\n    });\n\n    // 获取当前图表配置\n    const currentChartConfig = computed(() => {\n      return getChartConfig(props.selectedChartType);\n    });\n\n    // 检查是否有自定义配置选项\n    const hasCustomOptions = computed(() => {\n      return currentChartConfig.value.customOptions && Object.keys(currentChartConfig.value.customOptions).length > 0;\n    });\n\n    // 映射验证状态\n    const mappingValidation = computed(() => {\n      return fieldMappingRef.value?.validation || {\n        isValid: false,\n        missingFields: []\n      };\n    });\n\n    // 配置有效性验证\n    const isConfigValid = computed(() => {\n      return mappingValidation.value.isValid;\n    });\n\n    // 基础配置定义\n    const basicConfigs = {\n      title: {\n        type: 'text',\n        label: '图表标题',\n        placeholder: '请输入图表标题',\n        default: ''\n      },\n      colorScheme: {\n        type: 'select',\n        label: '配色方案',\n        options: [{\n          value: 'default',\n          label: '默认'\n        }, {\n          value: 'blue',\n          label: '蓝色'\n        }, {\n          value: 'green',\n          label: '绿色'\n        }, {\n          value: 'warm',\n          label: '暖色'\n        }, {\n          value: 'cool',\n          label: '冷色'\n        }, {\n          value: 'monochrome',\n          label: '单色'\n        }],\n        default: 'default'\n      },\n      animation: {\n        type: 'checkbox',\n        label: '启用动画',\n        default: true\n      },\n      showLegend: {\n        type: 'checkbox',\n        label: '显示图例',\n        default: true\n      },\n      showGrid: {\n        type: 'checkbox',\n        label: '显示网格',\n        default: true\n      }\n    };\n\n    // 过滤配置定义\n    const filterConfigs = {\n      dataRange: {\n        type: 'select',\n        label: '数据范围',\n        options: [{\n          value: 'all',\n          label: '全部数据'\n        }, {\n          value: 'first100',\n          label: '前100行'\n        }, {\n          value: 'last100',\n          label: '后100行'\n        }, {\n          value: 'sample',\n          label: '随机采样'\n        }],\n        default: 'all'\n      },\n      nullHandling: {\n        type: 'select',\n        label: '空值处理',\n        options: [{\n          value: 'ignore',\n          label: '忽略空值'\n        }, {\n          value: 'zero',\n          label: '替换为0'\n        }, {\n          value: 'interpolate',\n          label: '线性插值'\n        }, {\n          value: 'remove',\n          label: '删除行'\n        }],\n        default: 'ignore'\n      },\n      limit: {\n        type: 'number',\n        label: '数据限制',\n        min: 1,\n        max: 10000,\n        placeholder: '留空表示无限制',\n        help: '限制图表显示的数据点数量，防止性能问题'\n      }\n    };\n\n    // 获取图表描述\n    function getChartDescription(chartType) {\n      const descriptions = {\n        [CHART_TYPES.BAR]: '用于比较不同类别的数值大小',\n        [CHART_TYPES.LINE]: '显示数据随时间或其他连续变量的变化趋势',\n        [CHART_TYPES.PIE]: '展示各部分占整体的比例关系',\n        [CHART_TYPES.SCATTER]: '揭示两个数值变量之间的相关关系',\n        [CHART_TYPES.AREA]: '强调数量随时间的变化趋势和累积效果',\n        [CHART_TYPES.HEATMAP]: '通过颜色深浅显示数值在二维空间的分布',\n        [CHART_TYPES.RADAR]: '多维度数据的综合对比分析',\n        [CHART_TYPES.TREEMAP]: '分层数据的比例关系可视化',\n        [CHART_TYPES.SUNBURST]: '多层级分类数据的层次结构展示',\n        [CHART_TYPES.GAUGE]: '显示单一指标相对于目标的完成情况'\n      };\n      return descriptions[chartType] || '数据可视化图表';\n    }\n\n    // 切换配置区域显示状态\n    function toggleBasicConfig() {\n      showBasicConfig.value = !showBasicConfig.value;\n    }\n    function toggleCustomConfig() {\n      showCustomConfig.value = !showCustomConfig.value;\n    }\n    function toggleDataFilter() {\n      showDataFilter.value = !showDataFilter.value;\n    }\n\n    // 更新字段映射\n    function updateFieldMappings(mappings) {\n      chartConfig.value.fieldMappings = mappings;\n      emitConfigChange();\n    }\n\n    // 处理映射错误\n    function handleMappingError(error) {\n      errorMessage.value = error;\n      if (error) {\n        setTimeout(() => {\n          errorMessage.value = '';\n        }, 5000);\n      }\n    }\n\n    // 生成图表\n    function generateChart() {\n      if (!isConfigValid.value) {\n        errorMessage.value = '请完成必要的数据映射配置';\n        return;\n      }\n\n      // 构建完整的图表配置\n      const fullConfig = {\n        chartType: props.selectedChartType,\n        ...chartConfig.value,\n        // 合并自定义选项的默认值\n        customOptions: {\n          ...getDefaultCustomOptions(),\n          ...chartConfig.value.customOptions\n        }\n      };\n      emit('generate-chart', fullConfig);\n    }\n\n    // 获取自定义选项的默认值\n    function getDefaultCustomOptions() {\n      const defaults = {};\n      const customOptions = currentChartConfig.value.customOptions || {};\n      Object.keys(customOptions).forEach(key => {\n        defaults[key] = customOptions[key].default;\n      });\n      return defaults;\n    }\n\n    // 重置配置\n    function resetConfig() {\n      chartConfig.value = {\n        fieldMappings: {},\n        basicOptions: {\n          title: '',\n          colorScheme: 'default',\n          animation: true,\n          showLegend: true,\n          showGrid: true\n        },\n        customOptions: getDefaultCustomOptions(),\n        filterOptions: {\n          dataRange: 'all',\n          nullHandling: 'ignore',\n          limit: null\n        }\n      };\n      errorMessage.value = '';\n      emitConfigChange();\n    }\n\n    // 发出配置变化事件\n    function emitConfigChange() {\n      emit('config-change', {\n        chartType: props.selectedChartType,\n        ...chartConfig.value\n      });\n    }\n\n    // 监听图表类型变化\n    watch(() => props.selectedChartType, newType => {\n      // 重置字段映射（因为不同图表类型的字段要求不同）\n      chartConfig.value.fieldMappings = {};\n\n      // 重置自定义选项为新图表类型的默认值\n      chartConfig.value.customOptions = getDefaultCustomOptions();\n\n      // 自动展开自定义配置（如果有的话）\n      if (hasCustomOptions.value) {\n        showCustomConfig.value = true;\n      }\n      emitConfigChange();\n    });\n\n    // 监听基础选项变化\n    watch(() => chartConfig.value.basicOptions, () => {\n      emitConfigChange();\n    }, {\n      deep: true\n    });\n\n    // 监听自定义选项变化\n    watch(() => chartConfig.value.customOptions, () => {\n      emitConfigChange();\n    }, {\n      deep: true\n    });\n\n    // 监听过滤选项变化\n    watch(() => chartConfig.value.filterOptions, () => {\n      emitConfigChange();\n    }, {\n      deep: true\n    });\n\n    // 初始化自定义选项默认值\n    chartConfig.value.customOptions = getDefaultCustomOptions();\n    const __returned__ = {\n      props,\n      emit,\n      showBasicConfig,\n      showCustomConfig,\n      showDataFilter,\n      errorMessage,\n      fieldMappingRef,\n      chartConfig,\n      currentChartConfig,\n      hasCustomOptions,\n      mappingValidation,\n      isConfigValid,\n      basicConfigs,\n      filterConfigs,\n      getChartDescription,\n      toggleBasicConfig,\n      toggleCustomConfig,\n      toggleDataFilter,\n      updateFieldMappings,\n      handleMappingError,\n      generateChart,\n      getDefaultCustomOptions,\n      resetConfig,\n      emitConfigChange,\n      ref,\n      computed,\n      watch,\n      get getChartConfig() {\n        return getChartConfig;\n      },\n      get CHART_TYPES() {\n        return CHART_TYPES;\n      },\n      FieldMapping,\n      CustomConfigItem\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","getChartConfig","CHART_TYPES","FieldMapping","CustomConfigItem","props","__props","emit","__emit","showBasicConfig","showCustomConfig","showDataFilter","errorMessage","fieldMappingRef","chartConfig","fieldMappings","basicOptions","title","colorScheme","animation","showLegend","showGrid","customOptions","filterOptions","dataRange","nullHandling","limit","currentChartConfig","selectedChartType","hasCustomOptions","value","Object","keys","length","mappingValidation","validation","isValid","missingFields","isConfigValid","basicConfigs","type","label","placeholder","default","options","filterConfigs","min","max","help","getChartDescription","chartType","descriptions","BAR","LINE","PIE","SCATTER","AREA","HEATMAP","RADAR","TREEMAP","SUNBURST","GAUGE","toggleBasicConfig","toggleCustomConfig","toggleDataFilter","updateFieldMappings","mappings","emitConfigChange","handleMappingError","error","setTimeout","generateChart","fullConfig","getDefaultCustomOptions","defaults","forEach","key","resetConfig","newType","deep"],"sources":["D:/Learning Material/Git/Fuck-Charts/frontend/src/components/ChartConfigPanel.vue"],"sourcesContent":["<template>\n  <div class=\"chart-config-panel\">\n    <!-- 右侧边栏总标题 -->\n    <div class=\"panel-header\">\n      <h3>Chart Configuration</h3>\n    </div>\n\n    <!-- 图表类型信息 -->\n    <div class=\"chart-type-section\">\n      <div class=\"chart-type-info\">\n        <div class=\"chart-type-tag\">\n          <span class=\"chart-type-icon\">{{ currentChartConfig.icon }}</span>\n          <span class=\"chart-type-name\">{{ currentChartConfig.name }}</span>\n        </div>\n        <div class=\"chart-type-description\">\n          {{ getChartDescription(selectedChartType) }}\n        </div>\n      </div>\n    </div>\n\n    <!-- 数据映射配置区域 -->\n    <div class=\"mapping-section\">\n      <div class=\"mapping-section-header\">\n        <h4>数据映射</h4>\n        <div class=\"mapping-status\" :class=\"{ 'status-valid': mappingValidation.isValid }\">\n          {{ mappingValidation.isValid ? '✓ 完成' : `${mappingValidation.missingFields.length} 项未配置` }}\n        </div>\n      </div>\n\n      <FieldMapping\n        :chart-type=\"selectedChartType\"\n        :mappings=\"chartConfig.fieldMappings\"\n        @update:mappings=\"updateFieldMappings\"\n        @mapping-error=\"handleMappingError\"\n        ref=\"fieldMappingRef\"\n      />\n    </div>\n\n    <!-- 基础配置 -->\n    <div class=\"basic-config-section\">\n      <div class=\"section-header\" @click=\"toggleBasicConfig\">\n        <h4>基础配置</h4>\n        <div class=\"toggle-icon\" :class=\"{ 'toggle-open': showBasicConfig }\">▼</div>\n      </div>\n      <div v-if=\"showBasicConfig\" class=\"section-content\">\n        <CustomConfigItem\n          v-for=\"(config, key) in basicConfigs\"\n          :key=\"key\"\n          :config=\"config\"\n          :field-name=\"key\"\n          v-model=\"chartConfig.basicOptions[key]\"\n        />\n      </div>\n    </div>\n\n    <!-- 自定义配置 -->\n    <div class=\"custom-config-section\" v-if=\"hasCustomOptions\">\n      <div class=\"section-header\" @click=\"toggleCustomConfig\">\n        <h4>图表专属配置</h4>\n        <div class=\"toggle-icon\" :class=\"{ 'toggle-open': showCustomConfig }\">▼</div>\n      </div>\n      <div v-if=\"showCustomConfig\" class=\"section-content\">\n        <CustomConfigItem\n          v-for=\"(config, key) in currentChartConfig.customOptions\"\n          :key=\"key\"\n          :config=\"config\"\n          :field-name=\"key\"\n          v-model=\"chartConfig.customOptions[key]\"\n        />\n      </div>\n    </div>\n\n    <!-- 数据过滤 -->\n    <div class=\"filter-section\">\n      <div class=\"section-header\" @click=\"toggleDataFilter\">\n        <h4>数据过滤</h4>\n        <div class=\"toggle-icon\" :class=\"{ 'toggle-open': showDataFilter }\">▼</div>\n      </div>\n      <div v-if=\"showDataFilter\" class=\"section-content\">\n        <CustomConfigItem\n          v-for=\"(config, key) in filterConfigs\"\n          :key=\"key\"\n          :config=\"config\"\n          :field-name=\"key\"\n          v-model=\"chartConfig.filterOptions[key]\"\n        />\n      </div>\n    </div>\n\n    <!-- 操作按钮 -->\n    <div class=\"action-section\">\n      <button\n        class=\"apply-btn\"\n        :disabled=\"!isConfigValid\"\n        @click=\"generateChart\"\n      >\n        应用配置\n      </button>\n      <button class=\"reset-btn\" @click=\"resetConfig\">\n        重置配置\n      </button>\n    </div>\n\n    <!-- 错误提示 -->\n    <div v-if=\"errorMessage\" class=\"error-message\">\n      <div class=\"error-text\">{{ errorMessage }}</div>\n    </div>\n  </div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, computed, watch } from 'vue'\nimport { getChartConfig, CHART_TYPES } from '../configs/chartConfigs.js'\nimport FieldMapping from './FieldMapping.vue'\nimport CustomConfigItem from './CustomConfigItem.vue'\n\n// Props\nconst props = defineProps({\n  selectedChartType: {\n    type: String,\n    default: CHART_TYPES.BAR\n  },\n  currentFile: {\n    type: Object,\n    default: null\n  }\n})\n\n// Emits\nconst emit = defineEmits(['config-change', 'generate-chart'])\n\n// 响应式数据\nconst showBasicConfig = ref(true)\nconst showCustomConfig = ref(false)\nconst showDataFilter = ref(false)\nconst errorMessage = ref('')\n\n// 组件引用\nconst fieldMappingRef = ref(null)\n\n// 图表配置\nconst chartConfig = ref({\n  fieldMappings: {},\n  basicOptions: {\n    title: '',\n    colorScheme: 'default',\n    animation: true,\n    showLegend: true,\n    showGrid: true\n  },\n  customOptions: {},\n  filterOptions: {\n    dataRange: 'all',\n    nullHandling: 'ignore',\n    limit: null\n  }\n})\n\n// 获取当前图表配置\nconst currentChartConfig = computed(() => {\n  return getChartConfig(props.selectedChartType)\n})\n\n// 检查是否有自定义配置选项\nconst hasCustomOptions = computed(() => {\n  return currentChartConfig.value.customOptions && \n         Object.keys(currentChartConfig.value.customOptions).length > 0\n})\n\n// 映射验证状态\nconst mappingValidation = computed(() => {\n  return fieldMappingRef.value?.validation || { isValid: false, missingFields: [] }\n})\n\n// 配置有效性验证\nconst isConfigValid = computed(() => {\n  return mappingValidation.value.isValid\n})\n\n// 基础配置定义\nconst basicConfigs = {\n  title: {\n    type: 'text',\n    label: '图表标题',\n    placeholder: '请输入图表标题',\n    default: ''\n  },\n  colorScheme: {\n    type: 'select',\n    label: '配色方案',\n    options: [\n      { value: 'default', label: '默认' },\n      { value: 'blue', label: '蓝色' },\n      { value: 'green', label: '绿色' },\n      { value: 'warm', label: '暖色' },\n      { value: 'cool', label: '冷色' },\n      { value: 'monochrome', label: '单色' }\n    ],\n    default: 'default'\n  },\n  animation: {\n    type: 'checkbox',\n    label: '启用动画',\n    default: true\n  },\n  showLegend: {\n    type: 'checkbox',\n    label: '显示图例',\n    default: true\n  },\n  showGrid: {\n    type: 'checkbox',\n    label: '显示网格',\n    default: true\n  }\n}\n\n// 过滤配置定义\nconst filterConfigs = {\n  dataRange: {\n    type: 'select',\n    label: '数据范围',\n    options: [\n      { value: 'all', label: '全部数据' },\n      { value: 'first100', label: '前100行' },\n      { value: 'last100', label: '后100行' },\n      { value: 'sample', label: '随机采样' }\n    ],\n    default: 'all'\n  },\n  nullHandling: {\n    type: 'select',\n    label: '空值处理',\n    options: [\n      { value: 'ignore', label: '忽略空值' },\n      { value: 'zero', label: '替换为0' },\n      { value: 'interpolate', label: '线性插值' },\n      { value: 'remove', label: '删除行' }\n    ],\n    default: 'ignore'\n  },\n  limit: {\n    type: 'number',\n    label: '数据限制',\n    min: 1,\n    max: 10000,\n    placeholder: '留空表示无限制',\n    help: '限制图表显示的数据点数量，防止性能问题'\n  }\n}\n\n// 获取图表描述\nfunction getChartDescription(chartType) {\n  const descriptions = {\n    [CHART_TYPES.BAR]: '用于比较不同类别的数值大小',\n    [CHART_TYPES.LINE]: '显示数据随时间或其他连续变量的变化趋势',\n    [CHART_TYPES.PIE]: '展示各部分占整体的比例关系',\n    [CHART_TYPES.SCATTER]: '揭示两个数值变量之间的相关关系',\n    [CHART_TYPES.AREA]: '强调数量随时间的变化趋势和累积效果',\n    [CHART_TYPES.HEATMAP]: '通过颜色深浅显示数值在二维空间的分布',\n    [CHART_TYPES.RADAR]: '多维度数据的综合对比分析',\n    [CHART_TYPES.TREEMAP]: '分层数据的比例关系可视化',\n    [CHART_TYPES.SUNBURST]: '多层级分类数据的层次结构展示',\n    [CHART_TYPES.GAUGE]: '显示单一指标相对于目标的完成情况'\n  }\n  return descriptions[chartType] || '数据可视化图表'\n}\n\n// 切换配置区域显示状态\nfunction toggleBasicConfig() {\n  showBasicConfig.value = !showBasicConfig.value\n}\n\nfunction toggleCustomConfig() {\n  showCustomConfig.value = !showCustomConfig.value\n}\n\nfunction toggleDataFilter() {\n  showDataFilter.value = !showDataFilter.value\n}\n\n// 更新字段映射\nfunction updateFieldMappings(mappings) {\n  chartConfig.value.fieldMappings = mappings\n  emitConfigChange()\n}\n\n// 处理映射错误\nfunction handleMappingError(error) {\n  errorMessage.value = error\n  if (error) {\n    setTimeout(() => {\n      errorMessage.value = ''\n    }, 5000)\n  }\n}\n\n// 生成图表\nfunction generateChart() {\n  if (!isConfigValid.value) {\n    errorMessage.value = '请完成必要的数据映射配置'\n    return\n  }\n\n  // 构建完整的图表配置\n  const fullConfig = {\n    chartType: props.selectedChartType,\n    ...chartConfig.value,\n    // 合并自定义选项的默认值\n    customOptions: {\n      ...getDefaultCustomOptions(),\n      ...chartConfig.value.customOptions\n    }\n  }\n\n  emit('generate-chart', fullConfig)\n}\n\n// 获取自定义选项的默认值\nfunction getDefaultCustomOptions() {\n  const defaults = {}\n  const customOptions = currentChartConfig.value.customOptions || {}\n  \n  Object.keys(customOptions).forEach(key => {\n    defaults[key] = customOptions[key].default\n  })\n  \n  return defaults\n}\n\n// 重置配置\nfunction resetConfig() {\n  chartConfig.value = {\n    fieldMappings: {},\n    basicOptions: {\n      title: '',\n      colorScheme: 'default',\n      animation: true,\n      showLegend: true,\n      showGrid: true\n    },\n    customOptions: getDefaultCustomOptions(),\n    filterOptions: {\n      dataRange: 'all',\n      nullHandling: 'ignore',\n      limit: null\n    }\n  }\n\n  errorMessage.value = ''\n  emitConfigChange()\n}\n\n// 发出配置变化事件\nfunction emitConfigChange() {\n  emit('config-change', {\n    chartType: props.selectedChartType,\n    ...chartConfig.value\n  })\n}\n\n// 监听图表类型变化\nwatch(() => props.selectedChartType, (newType) => {\n  // 重置字段映射（因为不同图表类型的字段要求不同）\n  chartConfig.value.fieldMappings = {}\n  \n  // 重置自定义选项为新图表类型的默认值\n  chartConfig.value.customOptions = getDefaultCustomOptions()\n  \n  // 自动展开自定义配置（如果有的话）\n  if (hasCustomOptions.value) {\n    showCustomConfig.value = true\n  }\n  \n  emitConfigChange()\n})\n\n// 监听基础选项变化\nwatch(() => chartConfig.value.basicOptions, () => {\n  emitConfigChange()\n}, { deep: true })\n\n// 监听自定义选项变化\nwatch(() => chartConfig.value.customOptions, () => {\n  emitConfigChange()\n}, { deep: true })\n\n// 监听过滤选项变化\nwatch(() => chartConfig.value.filterOptions, () => {\n  emitConfigChange()\n}, { deep: true })\n\n// 初始化自定义选项默认值\nchartConfig.value.customOptions = getDefaultCustomOptions()\n</script>\n\n<style scoped>\n/* 白天主题CSS变量 */\n:root {\n  --bg-color: #ffffff;\n  --bg-secondary: #f8f9fa;\n  --bg-tertiary: #e9ecef;\n  --text-color: #333