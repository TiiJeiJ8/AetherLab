{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.every.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, computed, watch } from 'vue';\nexport default {\n  __name: 'DataMappingPanel',\n  props: {\n    chartType: {\n      type: String,\n      default: 'line'\n    },\n    selectedFiles: {\n      type: Array,\n      default: () => []\n    }\n  },\n  emits: ['mapping-change', 'render-mode-change', 'generate-chart', 'save-chart'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n    const emit = __emit;\n\n    // 实时渲染模式\n    const isRealTimeRender = ref(true);\n\n    // 映射数据\n    const mappingData = ref({});\n    const dragOverZone = ref(null);\n\n    // 界面控制\n    const showAdvancedConfig = ref(false);\n    const showDataFilter = ref(false);\n\n    // 数据过滤\n    const filterType = ref('all');\n    const rangeFilter = ref({\n      start: 1,\n      end: 100\n    });\n    const conditionFilter = ref({\n      column: '',\n      operator: 'eq',\n      value: ''\n    });\n\n    // 验证相关\n    const validation = ref({});\n    const validationErrors = ref({});\n\n    // 根据图表类型动态生成映射配置\n    const basicMappings = computed(() => {\n      const mappings = {\n        line: [{\n          key: 'x',\n          label: 'X轴',\n          required: true,\n          hint: '通常为时间、类别等'\n        }, {\n          key: 'y',\n          label: 'Y轴',\n          required: true,\n          hint: '数值型数据'\n        }],\n        bar: [{\n          key: 'x',\n          label: 'X轴',\n          required: true,\n          hint: '类别数据'\n        }, {\n          key: 'y',\n          label: 'Y轴',\n          required: true,\n          hint: '数值型数据'\n        }],\n        pie: [{\n          key: 'name',\n          label: '标签',\n          required: true,\n          hint: '饼图标签'\n        }, {\n          key: 'value',\n          label: '数值',\n          required: true,\n          hint: '数值型数据'\n        }],\n        scatter: [{\n          key: 'x',\n          label: 'X轴',\n          required: true,\n          hint: '数值型数据'\n        }, {\n          key: 'y',\n          label: 'Y轴',\n          required: true,\n          hint: '数值型数据'\n        }],\n        heatmap: [{\n          key: 'x',\n          label: 'X轴',\n          required: true,\n          hint: '类别数据'\n        }, {\n          key: 'y',\n          label: 'Y轴',\n          required: true,\n          hint: '类别数据'\n        }, {\n          key: 'value',\n          label: '数值',\n          required: true,\n          hint: '热力值'\n        }]\n      };\n      return mappings[props.chartType] || mappings.line;\n    });\n    const advancedMappings = computed(() => {\n      const mappings = {\n        line: [{\n          key: 'series',\n          label: '系列',\n          required: false,\n          hint: '用于分组的列'\n        }, {\n          key: 'color',\n          label: '颜色',\n          required: false,\n          hint: '颜色映射列'\n        }],\n        bar: [{\n          key: 'series',\n          label: '系列',\n          required: false,\n          hint: '用于分组的列'\n        }, {\n          key: 'color',\n          label: '颜色',\n          required: false,\n          hint: '颜色映射列'\n        }],\n        scatter: [{\n          key: 'size',\n          label: '大小',\n          required: false,\n          hint: '点大小映射'\n        }, {\n          key: 'color',\n          label: '颜色',\n          required: false,\n          hint: '颜色映射列'\n        }, {\n          key: 'series',\n          label: '系列',\n          required: false,\n          hint: '用于分组的列'\n        }],\n        pie: [{\n          key: 'color',\n          label: '颜色',\n          required: false,\n          hint: '颜色映射列'\n        }],\n        heatmap: [{\n          key: 'color',\n          label: '颜色',\n          required: false,\n          hint: '颜色映射列'\n        }]\n      };\n      return mappings[props.chartType] || [];\n    });\n\n    // 可用列（用于条件过滤）\n    const availableColumns = computed(() => {\n      const columns = [];\n      Object.values(mappingData.value).forEach(mapping => {\n        if (mapping && mapping.column) {\n          columns.push({\n            key: mapping.column.name,\n            name: `${mapping.column.name} (${mapping.file.name})`\n          });\n        }\n      });\n      return columns;\n    });\n\n    // 是否可以生成图表\n    const canGenerate = computed(() => {\n      const requiredMappings = basicMappings.value.filter(m => m.required);\n      return requiredMappings.every(m => mappingData.value[m.key]);\n    });\n\n    // 拖拽处理\n    function handleDragOver(zone) {\n      dragOverZone.value = zone;\n    }\n    function handleDragLeave() {\n      dragOverZone.value = null;\n    }\n    function handleDrop(event, zone) {\n      event.preventDefault();\n      dragOverZone.value = null;\n      try {\n        const data = JSON.parse(event.dataTransfer.getData('application/json'));\n        if (data.type === 'column') {\n          mappingData.value[zone] = {\n            column: data.column,\n            file: data.file\n          };\n          validateMapping(zone);\n          emitMappingChange();\n        }\n      } catch (error) {\n        console.error('Drop data parse error:', error);\n      }\n    }\n    function removeMapping(zone) {\n      delete mappingData.value[zone];\n      delete validation.value[zone];\n      delete validationErrors.value[zone];\n      emitMappingChange();\n    }\n    function clearAllMappings() {\n      mappingData.value = {};\n      validation.value = {};\n      validationErrors.value = {};\n      emitMappingChange();\n    }\n\n    // 验证映射\n    function validateMapping(zone) {\n      const mapping = mappingData.value[zone];\n      if (!mapping) return;\n      const zoneConfig = [...basicMappings.value, ...advancedMappings.value].find(m => m.key === zone);\n      if (!zoneConfig) return;\n\n      // 简单验证逻辑\n      let isValid = true;\n      let errorMessage = '';\n\n      // 检查数据类型\n      if (zone === 'x' || zone === 'y' || zone === 'value' || zone === 'size') {\n        if (mapping.column.type && !['number', 'integer', 'float'].includes(mapping.column.type.toLowerCase())) {\n          if (zone !== 'x') {\n            // X轴可以是类别数据\n            isValid = false;\n            errorMessage = `${zoneConfig.label}需要数值型数据`;\n          }\n        }\n      }\n      validation.value[zone] = isValid;\n      if (!isValid) {\n        validationErrors.value[zone] = errorMessage;\n      } else {\n        delete validationErrors.value[zone];\n      }\n    }\n\n    // 切换展开状态\n    function toggleAdvancedConfig() {\n      showAdvancedConfig.value = !showAdvancedConfig.value;\n    }\n    function toggleDataFilter() {\n      showDataFilter.value = !showDataFilter.value;\n    }\n\n    // 更新过滤器\n    function updateFilter() {\n      emitMappingChange();\n    }\n\n    // 生成图表\n    function generateChart() {\n      if (!canGenerate.value) return;\n      const config = {\n        mappings: mappingData.value,\n        filter: {\n          type: filterType.value,\n          range: rangeFilter.value,\n          condition: conditionFilter.value\n        }\n      };\n      emit('generate-chart', config);\n    }\n\n    // 发送映射变化事件\n    function emitMappingChange() {\n      emit('mapping-change', {\n        mappings: mappingData.value,\n        filter: {\n          type: filterType.value,\n          range: rangeFilter.value,\n          condition: conditionFilter.value\n        },\n        isValid: canGenerate.value\n      });\n    }\n\n    // 监听图表类型变化，清空映射\n    watch(() => props.chartType, () => {\n      clearAllMappings();\n    });\n    const __returned__ = {\n      props,\n      emit,\n      isRealTimeRender,\n      mappingData,\n      dragOverZone,\n      showAdvancedConfig,\n      showDataFilter,\n      filterType,\n      rangeFilter,\n      conditionFilter,\n      validation,\n      validationErrors,\n      basicMappings,\n      advancedMappings,\n      availableColumns,\n      canGenerate,\n      handleDragOver,\n      handleDragLeave,\n      handleDrop,\n      removeMapping,\n      clearAllMappings,\n      validateMapping,\n      toggleAdvancedConfig,\n      toggleDataFilter,\n      updateFilter,\n      generateChart,\n      emitMappingChange,\n      ref,\n      computed,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","props","__props","emit","__emit","isRealTimeRender","mappingData","dragOverZone","showAdvancedConfig","showDataFilter","filterType","rangeFilter","start","end","conditionFilter","column","operator","value","validation","validationErrors","basicMappings","mappings","line","key","label","required","hint","bar","pie","scatter","heatmap","chartType","advancedMappings","availableColumns","columns","Object","values","forEach","mapping","push","name","file","canGenerate","requiredMappings","filter","m","every","handleDragOver","zone","handleDragLeave","handleDrop","event","preventDefault","data","JSON","parse","dataTransfer","getData","type","validateMapping","emitMappingChange","error","console","removeMapping","clearAllMappings","zoneConfig","find","isValid","errorMessage","includes","toLowerCase","toggleAdvancedConfig","toggleDataFilter","updateFilter","generateChart","config","range","condition"],"sources":["D:/Learning Material/Git/Fuck-Charts/newArchitecture/frontend_vue_design/src/components/DataMappingPanel.vue"],"sourcesContent":["<template>\n  <div class=\"data-mapping-panel\">\n    <div class=\"panel-header\">\n      <h3>数据映射配置</h3>\n      <div class=\"render-mode-switch\">\n        <label class=\"switch\">\n          <input \n            type=\"checkbox\" \n            v-model=\"isRealTimeRender\" \n            @change=\"$emit('render-mode-change', isRealTimeRender)\"\n          >\n          <span class=\"slider\"></span>\n        </label>\n        <span class=\"switch-label\">实时渲染</span>\n      </div>\n    </div>\n\n    <div class=\"panel-content\">\n      <!-- 基本配置 -->\n      <div class=\"config-section\">\n        <h4 class=\"section-title\">基本配置</h4>\n        \n        <div class=\"mapping-group\" v-for=\"mapping in basicMappings\" :key=\"mapping.key\">\n          <div class=\"mapping-header\">\n            <label class=\"mapping-label\">{{ mapping.label }}</label>\n            <span class=\"mapping-required\" v-if=\"mapping.required\">*</span>\n          </div>\n          \n          <div \n            class=\"drop-zone\"\n            :class=\"{ \n              'has-data': mappingData[mapping.key],\n              'drag-over': dragOverZone === mapping.key,\n              'invalid': validation[mapping.key] === false\n            }\"\n            @dragover.prevent=\"handleDragOver(mapping.key)\"\n            @dragleave=\"handleDragLeave\"\n            @drop=\"handleDrop($event, mapping.key)\"\n          >\n            <div v-if=\"mappingData[mapping.key]\" class=\"mapped-column\">\n              <div class=\"column-info\">\n                <span class=\"column-name\">{{ mappingData[mapping.key].column.name }}</span>\n                <span class=\"column-source\">{{ mappingData[mapping.key].file.name }}</span>\n              </div>\n              <button class=\"remove-btn\" @click=\"removeMapping(mapping.key)\">\n                <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                  <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/>\n                  <line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n                </svg>\n              </button>\n            </div>\n            \n            <div v-else class=\"drop-placeholder\">\n              <svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                <path d=\"M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4\"/>\n                <polyline points=\"7,10 12,15 17,10\"/>\n                <line x1=\"12\" y1=\"15\" x2=\"12\" y2=\"3\"/>\n              </svg>\n              <span>拖拽列到此处</span>\n            </div>\n          </div>\n          \n          <div class=\"mapping-hint\" v-if=\"mapping.hint\">\n            {{ mapping.hint }}\n          </div>\n        </div>\n      </div>\n\n      <!-- 高级配置 -->\n      <div class=\"config-section\">\n        <div class=\"section-header\" @click=\"toggleAdvancedConfig\">\n          <h4 class=\"section-title\">高级配置</h4>\n          <svg \n            width=\"16\" \n            height=\"16\" \n            viewBox=\"0 0 24 24\" \n            fill=\"none\" \n            stroke=\"currentColor\" \n            stroke-width=\"2\"\n            :class=\"{ 'rotated': showAdvancedConfig }\"\n          >\n            <polyline points=\"6,9 12,15 18,9\"/>\n          </svg>\n        </div>\n        \n        <div class=\"advanced-config\" v-show=\"showAdvancedConfig\">\n          <div class=\"mapping-group\" v-for=\"mapping in advancedMappings\" :key=\"mapping.key\">\n            <div class=\"mapping-header\">\n              <label class=\"mapping-label\">{{ mapping.label }}</label>\n              <span class=\"mapping-optional\" v-if=\"!mapping.required\">可选</span>\n            </div>\n            \n            <div \n              class=\"drop-zone\"\n              :class=\"{ \n                'has-data': mappingData[mapping.key],\n                'drag-over': dragOverZone === mapping.key\n              }\"\n              @dragover.prevent=\"handleDragOver(mapping.key)\"\n              @dragleave=\"handleDragLeave\"\n              @drop=\"handleDrop($event, mapping.key)\"\n            >\n              <div v-if=\"mappingData[mapping.key]\" class=\"mapped-column\">\n                <div class=\"column-info\">\n                  <span class=\"column-name\">{{ mappingData[mapping.key].column.name }}</span>\n                  <span class=\"column-source\">{{ mappingData[mapping.key].file.name }}</span>\n                </div>\n                <button class=\"remove-btn\" @click=\"removeMapping(mapping.key)\">\n                  <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                    <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/>\n                    <line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n                  </svg>\n                </button>\n              </div>\n              \n              <div v-else class=\"drop-placeholder\">\n                <svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                  <path d=\"M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4\"/>\n                  <polyline points=\"7,10 12,15 17,10\"/>\n                  <line x1=\"12\" y1=\"15\" x2=\"12\" y2=\"3\"/>\n                </svg>\n                <span>拖拽列到此处</span>\n              </div>\n            </div>\n            \n            <div class=\"mapping-hint\" v-if=\"mapping.hint\">\n              {{ mapping.hint }}\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 数据过滤 -->\n      <div class=\"config-section\">\n        <div class=\"section-header\" @click=\"toggleDataFilter\">\n          <h4 class=\"section-title\">数据过滤</h4>\n          <svg \n            width=\"16\" \n            height=\"16\" \n            viewBox=\"0 0 24 24\" \n            fill=\"none\" \n            stroke=\"currentColor\" \n            stroke-width=\"2\"\n            :class=\"{ 'rotated': showDataFilter }\"\n          >\n            <polyline points=\"6,9 12,15 18,9\"/>\n          </svg>\n        </div>\n        \n        <div class=\"data-filter\" v-show=\"showDataFilter\">\n          <div class=\"filter-options\">\n            <label class=\"filter-option\">\n              <input \n                type=\"radio\" \n                v-model=\"filterType\" \n                value=\"all\"\n                @change=\"updateFilter\"\n              >\n              <span>全部行</span>\n            </label>\n            <label class=\"filter-option\">\n              <input \n                type=\"radio\" \n                v-model=\"filterType\" \n                value=\"range\"\n                @change=\"updateFilter\"\n              >\n              <span>范围选择</span>\n            </label>\n            <label class=\"filter-option\">\n              <input \n                type=\"radio\" \n                v-model=\"filterType\" \n                value=\"condition\"\n                @change=\"updateFilter\"\n              >\n              <span>条件过滤</span>\n            </label>\n          </div>\n          \n          <div class=\"filter-config\" v-if=\"filterType === 'range'\">\n            <div class=\"range-inputs\">\n              <input \n                type=\"number\" \n                v-model.number=\"rangeFilter.start\"\n                placeholder=\"起始行\"\n                min=\"1\"\n                @change=\"updateFilter\"\n              >\n              <span>到</span>\n              <input \n                type=\"number\" \n                v-model.number=\"rangeFilter.end\"\n                placeholder=\"结束行\"\n                min=\"1\"\n                @change=\"updateFilter\"\n              >\n            </div>\n          </div>\n          \n          <div class=\"filter-config\" v-if=\"filterType === 'condition'\">\n            <div class=\"condition-builder\">\n              <select v-model=\"conditionFilter.column\" @change=\"updateFilter\">\n                <option value=\"\">选择列</option>\n                <option \n                  v-for=\"column in availableColumns\" \n                  :key=\"column.key\"\n                  :value=\"column.key\"\n                >\n                  {{ column.name }}\n                </option>\n              </select>\n              <select v-model=\"conditionFilter.operator\" @change=\"updateFilter\">\n                <option value=\"eq\">等于</option>\n                <option value=\"ne\">不等于</option>\n                <option value=\"gt\">大于</option>\n                <option value=\"lt\">小于</option>\n                <option value=\"contains\">包含</option>\n              </select>\n              <input \n                type=\"text\" \n                v-model=\"conditionFilter.value\"\n                placeholder=\"值\"\n                @change=\"updateFilter\"\n              >\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 验证信息 -->\n      <div class=\"validation-section\" v-if=\"Object.keys(validationErrors).length > 0\">\n        <h4 class=\"section-title error\">验证错误</h4>\n        <div class=\"validation-errors\">\n          <div \n            v-for=\"(error, key) in validationErrors\" \n            :key=\"key\"\n            class=\"validation-error\"\n          >\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n              <line x1=\"15\" y1=\"9\" x2=\"9\" y2=\"15\"/>\n              <line x1=\"9\" y1=\"9\" x2=\"15\" y2=\"15\"/>\n            </svg>\n            <span>{{ error }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"panel-footer\">\n      <button \n        class=\"generate-btn\"\n        :disabled=\"!canGenerate\"\n        @click=\"generateChart\"\n      >\n        <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n          <polyline points=\"22,12 18,12 15,21 9,3 6,12 2,12\"/>\n        </svg>\n        生成图表\n      </button>\n      <button \n        class=\"save-btn\"\n        :disabled=\"!canGenerate\"\n        @click=\"saveChart\"\n      >\n        <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n          <path d=\"M19 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11l5 5v11a2 2 0 0 1-2 2z\"/>\n          <polyline points=\"17,21 17,13 7,13 7,21\"/>\n          <polyline points=\"7,3 7,8 15,8\"/>\n        </svg>\n        保存图表\n      </button>\n      <button \n        class=\"clear-btn\"\n        @click=\"clearAllMappings\"\n      >\n        <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n          <polyline points=\"3,6 5,6 21,6\"/>\n          <path d=\"M19,6v14a2,2,0,0,1-2,2H7a2,2,0,0,1-2-2V6m3,0V4a2,2,0,0,1,2-2h4a2,2,0,0,1,2,2v2\"/>\n        </svg>\n        清空配置\n      </button>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, watch } from 'vue'\n\nconst props = defineProps({\n  chartType: {\n    type: String,\n    default: 'line'\n  },\n  selectedFiles: {\n    type: Array,\n    default: () => []\n  }\n})\n\nconst emit = defineEmits(['mapping-change', 'render-mode-change', 'generate-chart', 'save-chart'])\n\n// 实时渲染模式\nconst isRealTimeRender = ref(true)\n\n// 映射数据\nconst mappingData = ref({})\nconst dragOverZone = ref(null)\n\n// 界面控制\nconst showAdvancedConfig = ref(false)\nconst showDataFilter = ref(false)\n\n// 数据过滤\nconst filterType = ref('all')\nconst rangeFilter = ref({ start: 1, end: 100 })\nconst conditionFilter = ref({ column: '', operator: 'eq', value: '' })\n\n// 验证相关\nconst validation = ref({})\nconst validationErrors = ref({})\n\n// 根据图表类型动态生成映射配置\nconst basicMappings = computed(() => {\n  const mappings = {\n    line: [\n      { key: 'x', label: 'X轴', required: true, hint: '通常为时间、类别等' },\n      { key: 'y', label: 'Y轴', required: true, hint: '数值型数据' }\n    ],\n    bar: [\n      { key: 'x', label: 'X轴', required: true, hint: '类别数据' },\n      { key: 'y', label: 'Y轴', required: true, hint: '数值型数据' }\n    ],\n    pie: [\n      { key: 'name', label: '标签', required: true, hint: '饼图标签' },\n      { key: 'value', label: '数值', required: true, hint: '数值型数据' }\n    ],\n    scatter: [\n      { key: 'x', label: 'X轴', required: true, hint: '数值型数据' },\n      { key: 'y', label: 'Y轴', required: true, hint: '数值型数据' }\n    ],\n    heatmap: [\n      { key: 'x', label: 'X轴', required: true, hint: '类别数据' },\n      { key: 'y', label: 'Y轴', required: true, hint: '类别数据' },\n      { key: 'value', label: '数值', required: true, hint: '热力值' }\n    ]\n  }\n  return mappings[props.chartType] || mappings.line\n})\n\nconst advancedMappings = computed(() => {\n  const mappings = {\n    line: [\n      { key: 'series', label: '系列', required: false, hint: '用于分组的列' },\n      { key: 'color', label: '颜色', required: false, hint: '颜色映射列' }\n    ],\n    bar: [\n      { key: 'series', label: '系列', required: false, hint: '用于分组的列' },\n      { key: 'color', label: '颜色', required: false, hint: '颜色映射列' }\n    ],\n    scatter: [\n      { key: 'size', label: '大小', required: false, hint: '点大小映射' },\n      { key: 'color', label: '颜色', required: false, hint: '颜色映射列' },\n      { key: 'series', label: '系列', required: false, hint: '用于分组的列' }\n    ],\n    pie: [\n      { key: 'color', label: '颜色', required: false, hint: '颜色映射列' }\n    ],\n    heatmap: [\n      { key: 'color', label: '颜色', required: false, hint: '颜色映射列' }\n    ]\n  }\n  return mappings[props.chartType] || []\n})\n\n// 可用列（用于条件过滤）\nconst availableColumns = computed(() => {\n  const columns = []\n  Object.values(mappingData.value).forEach(mapping => {\n    if (mapping && mapping.column) {\n      columns.push({\n        key: mapping.column.name,\n        name: `${mapping.column.name} (${mapping.file.name})`\n      })\n    }\n  })\n  return columns\n})\n\n// 是否可以生成图表\nconst canGenerate = computed(() => {\n  const requiredMappings = basicMappings.value.filter(m => m.required)\n  return requiredMappings.every(m => mappingData.value[m.key])\n})\n\n// 拖拽处理\nfunction handleDragOver(zone) {\n  dragOverZone.value = zone\n}\n\nfunction handleD