tyle scoped>\n/* 可拖拽浮窗样式 */\n.history-modal-draggable {\n  position: fixed;\n  z-index: 9999;\n  width: 480px;\n  min-height: 420px;\n  background: rgba(0,0,0,0.13);\n  left: 50vw;\n  top: 12vh;\n  border-radius: 16px;\n  box-shadow: 0 8px 32px rgba(0,0,0,0.18);\n  display: flex;\n  align-items: flex-start;\n  justify-content: center;\n  user-select: none;\n  transition: box-shadow 0.2s;\n}\n.history-content-rect {\n  width: 100%;\n  min-height: 420px;\n  background: var(--bg-color, #fff);\n  border-radius: 16px;\n  box-shadow: 0 2px 8px rgba(0,0,0,0.08);\n  padding: 18px 18px 12px 18px;\n  overflow-y: auto;\n  animation: pop-in 0.4s;\n}\n.history-header-rect {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-weight: bold;\n  font-size: 1.1em;\n  margin-bottom: 10px;\n  cursor: move;\n  user-select: none;\n}\n.close-btn {\n  background: none;\n  border: none;\n  font-size: 1.5em;\n  cursor: pointer;\n  color: var(--text-color, #333);\n  line-height: 1;\n}\n.history-list-rect {\n  display: flex;\n  flex-direction: column;\n  gap: 10px;\n  padding: 4px 0;\n}\n.chart-history-item-rect {\n  background: var(--bg-color);\n  border-radius: 8px;\n  padding: 8px 12px;\n  box-shadow: 0 1px 4px rgba(0,0,0,0.04);\n  display: flex;\n  align-items: center;\n  position: relative;\n  transition: transform 0.2s, box-shadow 0.2s;\n  gap: 12px;\n}\n.chart-history-item-rect:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 2px 8px rgba(58, 57, 57, 0.13);\n}\n.thumb-title-row-rect {\n  display: flex;\n  align-items: center;\n  gap: 14px;\n  width: 100%;\n}\n.chart-thumb-rect {\n  width: 60px;\n  height: 60px;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: 0 1px 4px rgba(0,0,0,0.08);\n  background: #f6f6f6;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  transition: box-shadow 0.18s;\n}\n.chart-thumb-rect:hover {\n  box-shadow: 0 2px 12px rgba(52,199,89,0.13);\n}\n.chart-history-title-rect