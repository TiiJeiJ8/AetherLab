     <label for=\"null-handling\">缺失值处理：</label>\n        <select id=\"null-handling\" v-model=\"localConfig.nullHandling\">\n        <option value=\"ignore\">忽略</option>\n        <option value=\"zero\">填充为0</option>\n        <option value=\"interpolate\">插值</option>\n        </select>\n    </div>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\n// 该组件用于所有图表的通用基础配置\nimport { computed, defineProps, defineEmits } from 'vue'\nconst props = defineProps({\n    modelValue: {\n        type: Object,\n        required: true\n    }\n})\nconst emit = defineEmits(['update:modelValue'])\n\n// 本地副本，避免直接修改父组件数据\nconst localConfig = computed({\n    get: () => props.modelValue,\n    set: (val) => emit('update:modelValue', val)\n})\n</script>\n\n<style scoped>\n.chart-basic-config {\n  border: 1px solid #e0e0e0;\n  border-radius: 6px;\n  padding: 16px 12px 8px 12px;\n  margin-bottom: 16px;\n  background: #fafbfc;\n}\n.basic-config-title {\n  font-size: 16px;\n  font-weight: bold;\n  margin-bottom: 10px;\n}\n.basic-config-item {\n  display: flex;\n  align-items: center;\n  margin-bottom: 10px;\n}\n.basic-config-item label {\n  width: 90px;\n  font-size: 14px;\n  color: #333;\n}\n.basic-config-item input[type=\"text\"],\n.basic-config-item select {\n  flex: 1;\n  padding: 4px 8px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  font-size: 14px;\n}\n.basic-config-item input[type=\"checkbox\"] {\n  margin-left: 0;\n  margin-right: 6px;\n}\n</style>\n"],"mappings":"AAmCA,SAASA,QAAkC,QAAQ,KAAI;;;;;;;;;;;;;;;;IAFvD;IACA;IAEA,MAAMC,KAAK,GAAGC,OAAA;IAMd,MAAMC,IAAI,GAAGC,MAAA;;IAEb;IACA,MAAMC,WAAW,GAAGL,QAAQ,CAAC;MACzBM,GAAG,EAAEA,CAAA,KAAML,KAAK,CAACM,UAAU;MAC3BC,GAAG,EAAGC,GAAG,IAAKN,IAAI,CAAC,mBAAmB,EAAEM,GAAG;IAC/C,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}