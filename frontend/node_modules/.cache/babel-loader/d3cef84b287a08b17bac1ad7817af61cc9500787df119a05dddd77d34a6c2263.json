{"ast":null,"code":"import ChartThumbnail from './ChartThumbnail.vue';\nimport { ref } from 'vue';\n\n// 删除确认\nimport { nextTick } from 'vue';\n// 编辑标题\n\nexport default {\n  __name: 'ChartHistoryModal',\n  props: {\n    show: Boolean,\n    chartHistory: Array\n  },\n  emits: ['close', 'preview', 'delete'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    const showDeleteConfirm = ref(false);\n    let deleteIndex = ref(null);\n    const editIndex = ref(null);\n    const editTitle = ref('');\n    let editInput = null;\n    function setEditInput(el) {\n      editInput = el;\n    }\n    function startEdit(idx) {\n      editIndex.value = idx;\n      editTitle.value = props.chartHistory[idx].option?.title?.text || props.chartHistory[idx].title || '';\n      nextTick(() => {\n        if (editInput && typeof editInput.focus === 'function') editInput.focus();\n      });\n    }\n    function finishEdit(idx) {\n      if (editIndex.value !== null) {\n        // 修改 option.title.text\n        if (!props.chartHistory[idx].option.title) props.chartHistory[idx].option.title = {};\n        props.chartHistory[idx].option.title.text = editTitle.value;\n        editIndex.value = null;\n      }\n    }\n    function cancelEdit() {\n      editIndex.value = null;\n    }\n    function confirmDelete(idx) {\n      deleteIndex.value = idx;\n      showDeleteConfirm.value = true;\n    }\n    function cancelDelete() {\n      showDeleteConfirm.value = false;\n      deleteIndex.value = null;\n    }\n    function handleDelete() {\n      if (deleteIndex.value !== null) {\n        emit('delete', deleteIndex.value);\n        showDeleteConfirm.value = false;\n        deleteIndex.value = null;\n      }\n    }\n\n    // props\n    const props = __props;\n    const emit = __emit;\n    function onClose() {\n      emit('close');\n    }\n    function onPreview(item) {\n      emit('preview', item);\n    }\n    function onDelete(idx) {\n      emit('delete', idx);\n    }\n    const __returned__ = {\n      showDeleteConfirm,\n      get deleteIndex() {\n        return deleteIndex;\n      },\n      set deleteIndex(v) {\n        deleteIndex = v;\n      },\n      editIndex,\n      editTitle,\n      get editInput() {\n        return editInput;\n      },\n      set editInput(v) {\n        editInput = v;\n      },\n      setEditInput,\n      startEdit,\n      finishEdit,\n      cancelEdit,\n      confirmDelete,\n      cancelDelete,\n      handleDelete,\n      props,\n      emit,\n      onClose,\n      onPreview,\n      onDelete,\n      ChartThumbnail,\n      ref,\n      nextTick\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ChartThumbnail","ref","nextTick","showDeleteConfirm","deleteIndex","editIndex","editTitle","editInput","setEditInput","el","startEdit","idx","value","props","chartHistory","option","title","text","focus","finishEdit","cancelEdit","confirmDelete","cancelDelete","handleDelete","emit","__props","__emit","onClose","onPreview","item","onDelete"],"sources":["D:/Learning Material/Git/Fuck-Charts/frontend/src/components/Chart/ChartHistoryModal.vue"],"sourcesContent":["<template>\n<div>\n<transition name=\"fade-slide\">\n    <div v-if=\"show\" class=\"history-modal\" @click.self=\"onClose\">\n    <div class=\"history-content\">\n        <div class=\"history-header\">\n        <span>Charts Storage</span>\n        <button class=\"close-btn\" @click=\"onClose\">×</button>\n        </div>\n        <div v-if=\"!chartHistory?.length\" class=\"empty-tip\">\n        No saved charts available.\n        </div>\n        <div class=\"history-list\">\n        <div\n            v-for=\"(item, idx) in chartHistory\"\n            :key=\"idx\"\n            class=\"chart-history-item\"\n        >\n            <div class=\"thumb-title-row\">\n            <ChartThumbnail :option=\"item.option\" @click=\"onPreview(item)\" />\n            <div v-if=\"editIndex !== idx\" class=\"chart-history-title\" @dblclick=\"startEdit(idx)\">\n                {{ item.option?.title?.text || item.title || 'Untitled' }}\n            </div>\n            <input\n                v-else\n                class=\"chart-history-title-edit\"\n                v-model=\"editTitle\"\n                @blur=\"finishEdit(idx)\"\n                @keyup.enter=\"finishEdit(idx)\"\n                @keyup.esc=\"cancelEdit\"\n                :ref=\"editIndex === idx ? setEditInput : null\"\n            />\n            <button class=\"delete-btn\" @click.stop=\"confirmDelete(idx)\" title=\"Delete\">❌</button>\n            </div>\n        </div>\n        </div>\n    </div>\n    </div>\n</transition>\n<transition name=\"fade\">\n    <div v-if=\"showDeleteConfirm\" class=\"confirm-modal\" @click.self=\"cancelDelete\">\n    <div class=\"confirm-content\">\n        <h3>Confirmation</h3>\n        <p>Are you sure you want to delete this chart? This operation is irreversible.</p>\n        <div name=\"confirm-buttons\">\n        <button class=\"cancel-btn\" @click=\"cancelDelete\">Cancel</button>\n        <button class=\"confirm-btn\" @click=\"handleDelete\">Confirm</button>\n        </div>\n    </div>\n    </div>\n</transition>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport ChartThumbnail from './ChartThumbnail.vue'\nimport { ref } from 'vue'\n\n// 删除确认\nconst showDeleteConfirm = ref(false)\nlet deleteIndex = ref(null)\n\nimport { nextTick } from 'vue'\n// 编辑标题\nconst editIndex = ref(null)\nconst editTitle = ref('')\nlet editInput = null\nfunction setEditInput(el) {\n    editInput = el\n}\n\nfunction startEdit(idx) {\n    editIndex.value = idx\n    editTitle.value = props.chartHistory[idx].option?.title?.text || props.chartHistory[idx].title || ''\n    nextTick(() => {\n        if (editInput && typeof editInput.focus === 'function') editInput.focus()\n    })\n}\nfunction finishEdit(idx) {\n    if (editIndex.value !== null) {\n        // 修改 option.title.text\n        if (!props.chartHistory[idx].option.title) props.chartHistory[idx].option.title = {}\n        props.chartHistory[idx].option.title.text = editTitle.value\n        editIndex.value = null\n    }\n}\nfunction cancelEdit() {\n    editIndex.value = null\n}\n\nfunction confirmDelete(idx) {\n    deleteIndex.value = idx\n    showDeleteConfirm.value = true\n}\n\nfunction cancelDelete() {\n    showDeleteConfirm.value = false\n    deleteIndex.value = null\n}\n\nfunction handleDelete() {\n    if (deleteIndex.value !== null) {\n        emit('delete', deleteIndex.value)\n        showDeleteConfirm.value = false\n        deleteIndex.value = null\n    }\n}\n\n// props\nconst props = defineProps({\n    show: Boolean,\n    chartHistory: Array\n})\nconst emit = defineEmits(['close', 'preview', 'delete'])\nfunction onClose() { emit('close') }\nfunction onPreview(item) { emit('preview', item) }\nfunction onDelete(idx) { emit('delete', idx) }\n</script>\n\n<style scoped>\n.history-modal {\n    position: fixed;\n    top: 0; left: 0; right: 0; bottom: 0;\n    background: rgba(0,0,0,0.18);\n    z-index: 9999;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n.history-content {\n    width: min(90vw, 480px);\n    max-height: min(90vh, 600px);\n    background: var(--bg-color, #fff);\n    border-radius: 12px;\n    box-shadow: 0 8px 32px rgba(0,0,0,0.18);\n    padding: clamp(12px, 2vw, 18px);\n    overflow-y: auto;\n    animation: pop-in 0.4s;\n}\n.history-header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    font-weight: bold;\n    font-size: 1.1em;\n    margin-bottom: 10px;\n}\n.close-btn {\n    background: none;\n    border: none;\n    font-size: 1.5em;\n    cursor: pointer;\n    color: var(--text-color, #333);\n    line-height: 1;\n}\n.history-list {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));\n    gap: clamp(8px, 1.5vw, 16px);\n    padding: 4px;\n}\n.chart-history-item {\n    background: var(--bg-color);\n    border-radius: 8px;\n    padding: clamp(6px, 1vw, 10px);\n    box-shadow: 0 1px 4px rgba(0,0,0,0.04);\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    position: relative;\n    transition: transform 0.2s, box-shadow 0.2s;\n}\n.chart-history-item:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 1px 1px rgba(58, 57, 57, 0.88);\n}\n.thumb-title-row {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    gap: 8px;\n    width: 100%;\n}\n.chart-history-title {\n    font-size: clamp(0.8em, 1.5vw, 1em);\n    font-weight: 500;\n    color: var(--text-color);\n    text-align: center;\n    width: 100%;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    cursor: pointer;\n    transition: background 0.2s;\n}\n.chart-history-title:hover {\n    background: #f0f0f0;\n}\n.chart-history-title-edit {\n    width: 90%;\n    font-size: inherit;\n    padding: 2px 6px;\n    border-radius: 4px;\n    border: 1px solid #bbb;\n    outline: none;\n    text-align: center;\n    margin: 0 auto;\n    display: block;\n}\n.delete-btn {\n    position: absolute;\n    top: 4px;\n    right: 4px;\n    background: var(--text-color);\n    border: none;\n    border-radius: 50%;\n    width: 24px;\n    height: 24px;\n    font-size: 12px;\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    opacity: 0;\n    transition: opacity 0.2s;\n}\n.chart-history-item:hover .delete-btn {\n    opacity: 1;\n}\n@media (max-width: 480px) {\n    .history-header {\n        font-size: 1em;\n        padding: 8px 0;\n    }\n\n    .delete-btn {\n        opacity: 1; /* 在移动端始终显示删除按钮 */\n    }\n}\n\n.fade-slide-enter-active, .fade-slide-leave-active {\n    transition: opacity 0.3s, transform 0.3s;\n}\n.fade-slide-enter-from, .fade-slide-leave-to {\n    opacity: 0;\n    transform: translateY(-30px);\n}\n@keyframes pop-in {\n    from { opacity: 0; transform: scale(0.95);}\n    to { opacity: 1; transform: scale(1);}\n}\n\n/* 确认弹窗样式 */\n.confirm-modal {\n    position: fixed;\n    top: 0; left: 0; right: 0; bottom: 0;\n    background: rgba(0,0,0,0.5);\n    z-index: 10000;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.confirm-content {\n    background: var(--bg-color, #fff);\n    border-radius: 8px;\n    padding: 20px;\n    width: min(90vw, 300px);\n    text-align: center;\n    box-shadow: 0 4px 16px rgba(0,0,0,0.2);\n}\n\n.confirm-content h3 {\n    margin: 0 0 12px;\n    color: #333;\n    font-size: 1.2em;\n}\n\n.confirm-content p {\n    margin: 0 0 20px;\n    color: #666;\n    font-size: 0.95em;\n}\n\n.confirm-buttons {\n    display: flex;\n    gap: 12px;\n    justify-content: center;\n}\n\n.confirm-btn, .cancel-btn {\n    padding: 8px 16px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    font-size: 0.9em;\n    transition: opacity 0.2s;\n}\n\n.cancel-btn {\n    background: #f0f0f0;\n    color: #666;\n}\n\n.confirm-btn {\n    background: #dc3545;\n    color: white;\n}\n\n.confirm-btn:hover, .cancel-btn:hover {\n    opacity: 0.8;\n}\n\n/* 淡入淡出动画 */\n.fade-enter-active, .fade-leave-active {\n    transition: opacity 0.2s;\n}\n\n.fade-enter-from, .fade-leave-to {\n    opacity: 0;\n}\n\n.empty-tip {\n    text-align: center;\n    color: #999;\n    padding: 40px 20px;\n    font-size: 1.1em;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    min-height: 200px;\n    background: #f8f9fa;\n    border-radius: 8px;\n    margin: 10px 0;\n    user-select: none;\n}\n\n@media (max-width: 480px) {\n    .empty-tip {\n        min-height: 150px;\n        padding: 20px;\n        font-size: 1em;\n    }\n}\n</style>"],"mappings":"AAwDA,OAAOA,cAAc,MAAM,sBAAqB;AAChD,SAASC,GAAG,QAAQ,KAAI;;AAExB;AAIA,SAASC,QAAQ,QAAQ,KAAI;AAC7B;;;;;;;;;;;;;;;IATA;IAKA,MAAMC,iBAAi