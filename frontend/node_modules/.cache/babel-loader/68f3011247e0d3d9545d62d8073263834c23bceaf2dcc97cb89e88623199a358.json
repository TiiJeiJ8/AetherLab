 });\n    const canRender = computed(() => {\n      return Object.values(mappedData.value).some(arr => arr.length > 0);\n    });\n\n    // 拖拽处理\n    function handleDrop(event, configKey) {\n      event.preventDefault();\n      const columnData = JSON.parse(event.dataTransfer.getData('text/plain'));\n      addMapping(configKey, columnData);\n      dragOverZone.value = null;\n      if (realTimeRender.value) {\n        renderChart();\n      }\n    }\n    function handleDragOver(configKey) {\n      dragOverZone.value = configKey;\n    }\n    function handleDragLeave() {\n      dragOverZone.value = null;\n    }\n\n    // 渲染控制\n    function toggleRealTimeRender() {\n      realTimeRender.value = !realTimeRender.value;\n    }\n    function renderChart() {\n      if (!canRender.value) return;\n      const option = generateChartOption(filteredData.value);\n      emit('chart-updated', option);\n    }\n    function togglePreview() {\n      showPreview.value = !showPreview.value;\n    }\n\n    // 监听变化\n    watch(() => [mappedData.value, filteredData.value], () => {\n      if (realTimeRender.value) {\n        renderChart();\n      }\n      emit('mapping-changed', {\n        mappedData: mappedData.value,\n        filteredData: filteredData.value\n      });\n    }, {\n      deep: true\n    });\n    watch(() => props.selectedChartType, newType => {\n      // 重置映射数据\n      Object.keys(mappedData.value).forEach(key => {\n        mappedData.value[key] = [];\n      });\n    });\n    const __returned__ = {\n      props,\n      emit,\n      chartConfigs,\n      mappedData,\n      generateChartOption,\n      removeMapping,\n      addMapping,\n      filterType,\n      rangeStart,\n      rangeEnd,\n      conditionColumn,\n      conditionOperator,\n      conditionValue,\n      filteredData,\n      updateFilter,\n      realTimeRender,\n      dragOverZone,\n      showPreview,\n      availableColumns,\n      previewData,\n      canRender,\n      handleDrop,\n      handleDragOver,\n      handleDr