{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { ref, computed, watch, onMounted, onUnmounted } from 'vue';\nimport { getThemeIcon } from '@/assets/JS/icons.js';\n\n// Props\n\nexport default {\n  __name: 'FileStructurePanel',\n  props: {\n    files: {\n      type: Array,\n      default: () => []\n    },\n    showPanel: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['close', 'column-drag'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n\n    // Emits\n    const emit = __emit;\n\n    // 状态管理\n    const selectedFileId = ref('');\n    const fileStructure = ref(null);\n    const isDragging = ref(false);\n    const isFocused = ref(false);\n    const dragOffset = ref({\n      x: 0,\n      y: 0\n    });\n    const draggingColumn = ref('');\n    const panelRef = ref(null);\n\n    // 面板位置\n    const panelPosition = ref({\n      position: 'fixed',\n      top: '100px',\n      right: '450px',\n      zIndex: 10001\n    });\n\n    // 当前选中的文件\n    const selectedFile = computed(() => {\n      return props.files.find(file => file.id === selectedFileId.value) || null;\n    });\n\n    // 监听文件变化\n    watch(selectedFileId, async newFileId => {\n      if (newFileId) {\n        await loadFileStructure(newFileId);\n      } else {\n        fileStructure.value = null;\n      }\n    });\n\n    // 加载文件结构\n    async function loadFileStructure(fileId) {\n      const file = props.files.find(f => f.id === fileId);\n      if (!file) return;\n      try {\n        // 模拟从后端获取文件结构数据\n        // 在实际应用中，这里应该调用API\n        const structure = await simulateFileStructureAnalysis(file);\n        fileStructure.value = structure;\n      } catch (error) {\n        console.error('加载文件结构失败:', error);\n      }\n    }\n\n    // 模拟文件结构分析\n    async function simulateFileStructureAnalysis(file) {\n      // 模拟异步分析\n      await new Promise(resolve => setTimeout(resolve, 500));\n\n      // 基于文件类型返回模拟数据\n      if (file.name.endsWith('.csv') || file.name.endsWith('.xlsx')) {\n        return {\n          rowCount: Math.floor(Math.random() * 10000) + 100,\n          columns: [{\n            name: 'ID',\n            type: 'integer',\n            stats: {\n              nonNull: 1250,\n              unique: 1250\n            },\n            samples: ['1', '2', '3', '4', '5']\n          }, {\n            name: 'Name',\n            type: 'string',\n            stats: {\n              nonNull: 1248,\n              unique: 1200\n            },\n            samples: ['张三', '李四', '王五', '赵六', '钱七']\n          }, {\n            name: 'Age',\n            type: 'integer',\n            stats: {\n              nonNull: 1245,\n              unique: 60\n            },\n            samples: ['25', '30', '35', '28', '42']\n          }, {\n            name: 'Salary',\n            type: 'float',\n            stats: {\n              nonNull: 1240,\n              unique: 800\n            },\n            samples: ['5000.50', '7500.00', '9200.75', '6800.25', '12000.00']\n          }, {\n            name: 'Department',\n            type: 'string',\n            stats: {\n              nonNull: 1250,\n              unique: 8\n            },\n            samples: ['技术部', '销售部', '人事部', '财务部', '市场部']\n          }, {\n            name: 'JoinDate',\n            type: 'date',\n            stats: {\n              nonNull: 1250,\n              unique: 500\n            },\n            samples: ['2020-01-15', '2019-03-22', '2021-07-10', '2020-11-05', '2022-02-28']\n          }],\n          preview: [{\n            ID: '1',\n            Name: '张三',\n            Age: '25',\n            Salary: '5000.50',\n            Department: '技术部',\n            JoinDate: '2020-01-15'\n          }, {\n            ID: '2',\n            Name: '李四',\n            Age: '30',\n            Salary: '7500.00',\n            Department: '销售部',\n            JoinDate: '2019-03-22'\n          }, {\n            ID: '3',\n            Name: '王五',\n            Age: '35',\n            Salary: '9200.75',\n            Department: '人事部',\n            JoinDate: '2021-07-10'\n          }, {\n            ID: '4',\n            Name: '赵六',\n            Age: '28',\n            Salary: '6800.25',\n            Department: '财务部',\n            JoinDate: '2020-11-05'\n          }, {\n            ID: '5',\n            Name: '钱七',\n            Age: '42',\n            Salary: '12000.00',\n            Department: '市场部',\n            JoinDate: '2022-02-28'\n          }]\n        };\n      }\n      return null;\n    }\n\n    // 获取列类型图标\n    function getColumnIcon(type) {\n      const iconMap = {\n        'integer': 'hash',\n        'float': 'hash',\n        'string': 'type',\n        'date': 'calendar',\n        'boolean': 'check'\n      };\n      return getThemeIcon(iconMap[type] || 'circle');\n    }\n\n    // 文件选择处理\n    function handleFileChange() {\n      // 文件变化时自动加载结构\n    }\n\n    // 拖拽处理\n    function handleDragStart(column, event) {\n      draggingColumn.value = column.name;\n\n      // 设置拖拽数据\n      event.dataTransfer.setData('application/json', JSON.stringify({\n        type: 'column',\n        fileId: selectedFileId.value,\n        fileName: selectedFile.value?.name,\n        columnName: column.name,\n        columnType: column.type,\n        columnStats: column.stats\n      }));\n\n      // 设置拖拽效果\n      event.dataTransfer.effectAllowed = 'copy';\n\n      // 通知父组件开始拖拽\n      emit('column-drag', {\n        action: 'start',\n        column\n      });\n    }\n    function handleDragEnd() {\n      draggingColumn.value = '';\n      emit('column-drag', {\n        action: 'end'\n      });\n    }\n\n    // 面板拖拽功能\n    function handleMouseDown() {\n      isFocused.value = true;\n    }\n    function handleMouseEnter() {\n      isFocused.value = true;\n    }\n    function handleMouseLeave() {\n      if (!isDragging.value) {\n        isFocused.value = false;\n      }\n    }\n    function startDrag(event) {\n      isDragging.value = true;\n      isFocused.value = true;\n      const rect = panelRef.value.getBoundingClientRect();\n      dragOffset.value = {\n        x: event.clientX - rect.left,\n        y: event.clientY - rect.top\n      };\n      document.addEventListener('mousemove', handlePanelDrag, {\n        passive: false\n      });\n      document.addEventListener('mouseup', stopDrag, {\n        passive: false\n      });\n      event.preventDefault();\n      event.stopPropagation();\n    }\n    function handlePanelDrag(event) {\n      if (!isDragging.value) return;\n      requestAnimationFrame(() => {\n        const newX = event.clientX - dragOffset.value.x;\n        const newY = event.clientY - dragOffset.value.y;\n        const maxX = window.innerWidth - 320;\n        const maxY = window.innerHeight - 400;\n        const clampedX = Math.max(0, Math.min(newX, maxX));\n        const clampedY = Math.max(0, Math.min(newY, maxY));\n        panelPosition.value = {\n          position: 'fixed',\n          top: `${clampedY}px`,\n          right: 'auto',\n          left: `${clampedX}px`,\n          zIndex: 10001\n        };\n      });\n      event.preventDefault();\n    }\n    function stopDrag() {\n      isDragging.value = false;\n      document.removeEventListener('mousemove', handlePanelDrag);\n      document.removeEventListener('mouseup', stopDrag);\n      setTimeout(() => {\n        if (!isDragging.value) {\n          isFocused.value = false;\n        }\n      }, 150);\n    }\n\n    // 关闭面板\n    function closePanel() {\n      emit('close');\n    }\n\n    // 格式化文件大小\n    function formatFileSize(bytes) {\n      if (bytes === 0) return '0 Bytes';\n      const k = 1024;\n      const sizes = ['Bytes', 'KB', 'MB', 'GB'];\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\n    }\n\n    // 生命周期\n    onMounted(() => {\n      document.addEventListener('click', handleGlobalClick);\n    });\n    onUnmounted(() => {\n      document.removeEventListener('click', handleGlobalClick);\n      document.removeEventListener('mousemove', handlePanelDrag);\n      document.removeEventListener('mouseup', stopDrag);\n    });\n    function handleGlobalClick(event) {\n      if (panelRef.value && !panelRef.value.contains(event.target)) {\n        isFocused.value = false;\n      }\n    }\n    const __returned__ = {\n      props,\n      emit,\n      selectedFileId,\n      fileStructure,\n      isDragging,\n      isFocused,\n      dragOffset,\n      draggingColumn,\n      panelRef,\n      panelPosition,\n      selectedFile,\n      loadFileStructure,\n      simulateFileStructureAnalysis,\n      getColumnIcon,\n      handleFileChange,\n      handleDragStart,\n      handleDragEnd,\n      handleMouseDown,\n      handleMouseEnter,\n      handleMouseLeave,\n      startDrag,\n      handlePanelDrag,\n      stopDrag,\n      closePanel,\n      formatFileSize,\n      handleGlobalClick,\n      ref,\n      computed,\n      watch,\n      onMounted,\n      onUnmounted,\n      get getThemeIcon() {\n        return getThemeIcon;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","onMounted","onUnmounted","getThemeIcon","props","__props","emit","__emit","selectedFileId","fileStructure","isDragging","isFocused","dragOffset","x","y","draggingColumn","panelRef","panelPosition","position","top","right","zIndex","selectedFile","files","find","file","id","value","newFileId","loadFileStructure","fileId","f","structure","simulateFileStructureAnalysis","error","console","Promise","resolve","setTimeout","name","endsWith","rowCount","Math","floor","random","columns","type","stats","nonNull","unique","samples","preview","ID","Name","Age","Salary","Department","JoinDate","getColumnIcon","iconMap","handleFileChange","handleDragStart","column","event","dataTransfer","setData","JSON","stringify","fileName","columnName","columnType","columnStats","effectAllowed","action","handleDragEnd","handleMouseDown","handleMouseEnter","handleMouseLeave","startDrag","rect","getBoundingClientRect","clientX","left","clientY","document","addEventListener","handlePanelDrag","passive","stopDrag","preventDefault","stopPropagation","requestAnimationFrame","newX","newY","maxX","window","innerWidth","maxY","innerHeight","clampedX","max","min","clampedY","removeEventListener","closePanel","formatFileSize","bytes","k","sizes","i","log","parseFloat","pow","toFixed","handleGlobalClick","contains","target"],"sources":["D:/Learning Material/Git/Fuck-Charts/newArchitecture/frontend_vue_design/src/components/FileStructurePanel.vue"],"sourcesContent":["<template>\n  <div \n    v-if=\"showPanel\"\n    class=\"file-structure-panel\"\n    :class=\"{ 'is-focused': isFocused, 'dragging': isDragging }\"\n    :style=\"panelPosition\"\n    @mousedown=\"handleMouseDown\"\n    @mouseenter=\"handleMouseEnter\"\n    @mouseleave=\"handleMouseLeave\"\n    ref=\"panelRef\">\n    \n    <!-- 拖拽头部 -->\n    <div \n      class=\"panel-header\"\n      @mousedown.prevent=\"startDrag\">\n      <div class=\"panel-title\">\n        <span class=\"panel-icon\" v-html=\"getThemeIcon('database')\"></span>\n        <span>文件结构</span>\n        <span class=\"file-indicator\">{{ selectedFile?.name || '未选择文件' }}</span>\n      </div>\n      <button class=\"close-btn\" @click=\"closePanel\">\n        <span v-html=\"getThemeIcon('close')\"></span>\n      </button>\n    </div>\n\n    <!-- 文件选择器 -->\n    <div class=\"file-selector\" v-if=\"files.length > 0\">\n      <select \n        v-model=\"selectedFileId\" \n        @change=\"handleFileChange\"\n        class=\"file-select\">\n        <option value=\"\">选择文件</option>\n        <option \n          v-for=\"file in files\" \n          :key=\"file.id\" \n          :value=\"file.id\">\n          {{ file.name }}\n        </option>\n      </select>\n    </div>\n\n    <!-- 文件结构内容 -->\n    <div class=\"panel-content\" v-if=\"selectedFile\">\n      <div class=\"file-info\">\n        <div class=\"info-row\">\n          <span class=\"info-label\">文件名:</span>\n          <span class=\"info-value\">{{ selectedFile.name }}</span>\n        </div>\n        <div class=\"info-row\">\n          <span class=\"info-label\">大小:</span>\n          <span class=\"info-value\">{{ formatFileSize(selectedFile.size) }}</span>\n        </div>\n        <div class=\"info-row\">\n          <span class=\"info-label\">类型:</span>\n          <span class=\"info-value\">{{ selectedFile.type }}</span>\n        </div>\n        <div class=\"info-row\" v-if=\"fileStructure\">\n          <span class=\"info-label\">行数:</span>\n          <span class=\"info-value\">{{ fileStructure.rowCount }}</span>\n        </div>\n      </div>\n\n      <!-- 列结构 -->\n      <div class=\"columns-section\" v-if=\"fileStructure && fileStructure.columns\">\n        <div class=\"section-header\">\n          <span class=\"section-title\">列结构</span>\n          <span class=\"columns-count\">{{ fileStructure.columns.length }} 列</span>\n        </div>\n        \n        <div class=\"columns-list\">\n          <div \n            v-for=\"(column, index) in fileStructure.columns\"\n            :key=\"index\"\n            class=\"column-item\"\n            :class=\"{ 'is-dragging': draggingColumn === column.name }\"\n            draggable=\"true\"\n            @dragstart=\"handleDragStart(column, $event)\"\n            @dragend=\"handleDragEnd\">\n            \n            <div class=\"column-header\">\n              <span class=\"column-icon\" v-html=\"getColumnIcon(column.type)\"></span>\n              <span class=\"column-name\">{{ column.name }}</span>\n              <span class=\"column-type\">{{ column.type }}</span>\n            </div>\n            \n            <div class=\"column-stats\" v-if=\"column.stats\">\n              <span class=\"stat-item\">\n                <span class=\"stat-label\">非空:</span>\n                <span class=\"stat-value\">{{ column.stats.nonNull }}</span>\n              </span>\n              <span class=\"stat-item\" v-if=\"column.stats.unique\">\n                <span class=\"stat-label\">唯一:</span>\n                <span class=\"stat-value\">{{ column.stats.unique }}</span>\n              </span>\n            </div>\n            \n            <!-- 样本数据 -->\n            <div class=\"column-samples\" v-if=\"column.samples\">\n              <div class=\"samples-header\">样本数据:</div>\n              <div class=\"samples-content\">\n                <span \n                  v-for=\"(sample, idx) in column.samples.slice(0, 3)\"\n                  :key=\"idx\"\n                  class=\"sample-item\">\n                  {{ sample }}\n                </span>\n                <span v-if=\"column.samples.length > 3\" class=\"sample-more\">\n                  +{{ column.samples.length - 3 }}\n                </span>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 数据预览 -->\n      <div class=\"data-preview\" v-if=\"fileStructure && fileStructure.preview\">\n        <div class=\"section-header\">\n          <span class=\"section-title\">数据预览</span>\n        </div>\n        <div class=\"preview-table\">\n          <table>\n            <thead>\n              <tr>\n                <th v-for=\"column in fileStructure.columns\" :key=\"column.name\">\n                  {{ column.name }}\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"(row, index) in fileStructure.preview.slice(0, 5)\" :key=\"index\">\n                <td v-for=\"column in fileStructure.columns\" :key=\"column.name\">\n                  {{ row[column.name] }}\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n\n    <!-- 空状态 -->\n    <div class=\"empty-state\" v-else>\n      <div class=\"empty-icon\" v-html=\"getThemeIcon('fileText')\"></div>\n      <div class=\"empty-text\">请先选择一个文件查看其结构</div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, watch, onMounted, onUnmounted } from 'vue'\nimport { getThemeIcon } from '@/assets/JS/icons.js'\n\n// Props\nconst props = defineProps({\n  files: {\n    type: Array,\n    default: () => []\n  },\n  showPanel: {\n    type: Boolean,\n    default: false\n  }\n})\n\n// Emits\nconst emit = defineEmits(['close', 'column-drag'])\n\n// 状态管理\nconst selectedFileId = ref('')\nconst fileStructure = ref(null)\nconst isDragging = ref(false)\nconst isFocused = ref(false)\nconst dragOffset = ref({ x: 0, y: 0 })\nconst draggingColumn = ref('')\nconst panelRef = ref(null)\n\n// 面板位置\nconst panelPosition = ref({\n  position: 'fixed',\n  top: '100px',\n  right: '450px',\n  zIndex: 10001\n})\n\n// 当前选中的文件\nconst selectedFile = computed(() => {\n  return props.files.find(file => file.id === selectedFileId.value) || null\n})\n\n// 监听文件变化\nwatch(selectedFileId, async (newFileId) => {\n  if (newFileId) {\n    await loadFileStructure(newFileId)\n  } else {\n    fileStructure.value = null\n  }\n})\n\n// 加载文件结构\nasync function loadFileStructure(fileId) {\n  const file = props.files.find(f => f.id === fileId)\n  if (!file) return\n\n  try {\n    // 模拟从后端获取文件结构数据\n    // 在实际应用中，这里应该调用API\n    const structure = await simulateFileStructureAnalysis(file)\n    fileStructure.value = structure\n  } catch (error) {\n    console.error('加载文件结构失败:', error)\n  }\n}\n\n// 模拟文件结构分析\nasync function simulateFileStructureAnalysis(file) {\n  // 模拟异步分析\n  await new Promise(resolve => setTimeout(resolve, 500))\n  \n  // 基于文件类型返回模拟数据\n  if (file.name.endsWith('.csv') || file.name.endsWith('.xlsx')) {\n    return {\n      rowCount: Math.floor(Math.random() * 10000) + 100,\n      columns: [\n        {\n          name: 'ID',\n          type: 'integer',\n          stats: { nonNull: 1250, unique: 1250 },\n          samples: ['1', '2', '3', '4', '5']\n        },\n        {\n          name: 'Name',\n          type: 'string',\n          stats: { nonNull: 1248, unique: 1200 },\n          samples: ['张三', '李四', '王五', '赵六', '钱七']\n        },\n        {\n          name: 'Age',\n          type: 'integer',\n          stats: { nonNull: 1245, unique: 60 },\n          samples: ['25', '30', '35', '28', '42']\n        },\n        {\n          name: 'Salary',\n          type: 'float',\n          stats: { nonNull: 1240, unique: 800 },\n          samples: ['5000.50', '7500.00', '9200.75', '6800.25', '12000.00']\n        },\n        {\n          name: 'Department',\n          type: 'string',\n          stats: { nonNull: 1250, unique: 8 },\n          samples: ['技术部', '销售部', '人事部', '财务部', '市场部']\n        },\n        {\n          name: 'JoinDate',\n          type: 'date',\n          stats: { nonNull: 1250, unique: 500 },\n          samples: ['2020-01-15', '2019-03-22', '2021-07-10', '2020-11-05', '2022-02-28']\n        }\n      ],\n      preview: [\n        { ID: '1', Name: '张三', Age: '25', Salary: '5000.50', Department: '技术部', JoinDate: '2020-01-15' },\n        { ID: '2', Name: '李四', Age: '30', Salary: '7500.00', Department: '销售部', JoinDate: '2019-03-22' },\n        { ID: '3', Name: '王五', Age: '35', Salary: '9200.75', Department: '人事部', JoinDate: '2021-07-10' },\n        { ID: '4', Name: '赵六', Age: '28', Salary: '6800.25', Department: '财务部', JoinDate: '2020-11-05' },\n        { ID: '5', Name: '钱七', Age: '42', Salary: '12000.00', Department: '市场部', JoinDate: '2022-02-28' }\n      ]\n    }\n  }\n  \n  return null\n}\n\n// 获取列类型图标\nfunction getColumnIcon(type) {\n  const iconMap = {\n    'integer': 'hash',\n    'float': 'hash',\n    'string': 'type',\n    'date': 'calendar',\n    'boolean': 'check'\n  }\n  return getThemeIcon(iconMap[type] || 'circle')\n}\n\n// 文件选择处理\nfunction handleFileChange() {\n  // 文件变化时自动加载结构\n}\n\n// 拖拽处理\nfunction handleDragStart(column, event) {\n  draggingColumn.value = column.name\n  \n  // 设置拖拽数据\n  event.dataTransfer.setData('application/json', JSON.stringify({\n    type: 'column',\n    fileId: selectedFileId.value,\n    fileName: selectedFile.value?.name,\n    columnName: column.name,\n    columnType: column.type,\n    columnStats: column.stats\n  }))\n  \n  // 设置拖拽效果\n  event.dataTransfer.effectAllowed = 'copy'\n  \n  // 通知父组件开始拖拽\n  emit('column-drag', { action: 'start', column })\n}\n\nfunction handleDragEnd() {\n  draggingColumn.value = ''\n  emit('column-drag', { action: 'end' })\n}\n\n// 面板拖拽功能\nfunction handleMouseDown() {\n  isFocused.value = true\n}\n\nfunction handleMouseEnter() {\n  isFocused.value = true\n}\n\nfunction handleMouseLeave() {\n  if (!isDragging.value) {\n    isFocused.value = false\n  }\n}\n\nfunction startDrag(event) {\n  isDragging.value = true\n  isFocused.value = true\n  \n  const rect = panelRef.value.getBoundingClientRect()\n  dragOffset.value = {\n    x: event.clientX - rect.left,\n    y: event.clientY - rect.top\n  }\n  \n  document.addEventListener('mousemove', handlePanelDrag, { passive: false })\n  document.addEventListener('mouseup', stopDrag, { passive: false })\n  \n  event.preventDefault()\n  event.stopPropagation()\n}\n\nfunction handlePanelDrag(event) {\n  if (!isDragging.value) return\n  \n  requestAnimationFrame(() => {\n    const newX = event.clientX - dragOffset.value.x\n    const newY = event.clientY - dragOffset.value.y\n    \n    const maxX = window.innerWidth - 320\n    const maxY = window.innerHeight - 400\n    \n    const clampedX = Math.max(0, Math.min(newX, maxX))\n    const clampedY = Math.max(0, Math.min(newY, maxY))\n    \n    panelPosition.value = {\n      position: 'fixed',\n      top: `${clampedY}px`,\n      right: 'auto',\n      left: `${clampedX}px`,\n      zIndex: 10001\n    }\n  })\n  \n  event.preventDefault()\n}\n\nfunction stopDrag() {\n  isDragging.value = false\n  \n  document.removeEventListener('mousemove', handlePanelDrag)\n  document.removeEventListener('mouseup', stopDrag)\n  \n  setTimeout(() => {\n    if (!isDragging.value) {\n      isFocused.value = false\n    }\n  }, 150)\n}\n\n// 关闭面板\nfunction closePanel() {\n  emit('close')\n}\n\n// 格式化文件大小\nfunction formatFileSize(bytes) {\n  if (bytes === 0) return '0 Bytes'\n  const k = 1024\n  const sizes = ['Bytes', 'KB', 'MB', 'GB']\n  const i = Math.floor(Math.log(bytes) / Math.log(k))\n  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\n}\n\n// 生命周期\nonMounted(() => {\n  document.addEventListener('click', handleGlobalClick)\n})\n\nonUnmounted(() => {\n  document.removeEventListener('click', handleGlobalClick)\n  document.removeEventListener('mousemove', handlePanelDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n\nfunction handleGlobalClick(event) {\n  if (panelRef.value && !panelRef.value.contains(event.target)) {\n    isFocused.value = false\n  }\n}\n</script>\n\n<style scoped>\n.file-structure-panel {\n  position: fixed;\n  width: 320px;\n  max-height: 80vh;\n  background: rgba(255, 255, 255, 0.95);\n  backdrop-filter: blur(20px);\n  border: 1px solid rgba(0, 0, 0, 0.1);\n  border-radius: 12px;\n  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.15);\n  z-index: 10001;\n  overflow: hidden;\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n  user-select: none;\n}\n\n.file-structure-panel.is-focused {\n  transform: translateY(-2px);\n  box-shadow: 0 12px 40px rgba(0, 0, 0, 0.2);\n}\n\n.file-structure-panel.dragging {\n  transform: scale(1.02);\n  box-shadow: 0 16px 48px rgba(0, 0, 0, 0.3);\n  transition: none;\n}\n\n[data-theme=\"dark\"] .file-structure-panel {\n  background: rgba(17, 24, 39, 0.95);\n  border: 1px solid rgba(55, 65, 81, 0.3);\n}\n\n.panel-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 12px 16px;\n  background: rgba(59, 130, 246, 0.05);\n  border-bottom: 1px solid rgba(0, 0, 0, 0.08);\n  cursor: