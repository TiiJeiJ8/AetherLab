   <thead>\n                                <tr>\n                                    <th class=\"row-number\">#</th>\n                                    <th v-for=\"header in tableHeaders\"\n                                        :key=\"header\">{{ header }}</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                <tr v-for=\"(row, idx) in previewData\"\n                                    :key=\"idx\">\n                                    <td class=\"row-number\">{{ idx + 1 }}</td>\n                                    <td v-for=\"header in tableHeaders\"\n                                        :key=\"header\">{{ row[header] }}</td>\n                                </tr>\n                            </tbody>\n                        </table>\n                    </div>\n                    <div v-else class=\"empty-tip\">\n                        <div class=\"empty-icon\">üìä</div>\n                        <p>ÊöÇÊó†Êï∞ÊçÆÂèØÈ¢ÑËßà</p>\n                    </div>\n                </div>\n            </div>\n        </transition>\n    </div>\n</transition>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, computed } from 'vue'\n\nconst props = defineProps({\n    show: Boolean,\n    currentFile: Object,\n    previewData: {\n        type: Array,\n        default: () => []\n    },\n    isSidePreview: {\n        type: Boolean,\n        default: false\n    }\n})\n\nconst emit = defineEmits(['close'])\n\n// Ë°®Â§¥ËÆ°ÁÆó\nconst tableHeaders = computed(() => {\n    if (!props.previewData.length) return []\n    return Object.keys(props.previewData[0])\n})\n\n// Ê†ºÂºèÂåñÊñá‰ª∂Â§ßÂ∞è\nfunction formatFileSize(bytes) {\n    if (!bytes) return '0 Bytes'\n    const k = 1024\n    const sizes = ['Bytes', 'KB', 'MB', 'GB']\n    const i = Math.floor(Math.log(bytes) / Math.log(k))\n    return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\n}\n\nfunction onClose() {\n    emit('close')\n}\n</script>\n\n<style scoped>\n/* Ê®°ÊÄÅÊ°ÜÂü∫Á°ÄÊ†∑Âºè */\n.preview-modal {\n    position: fixed;\n    top: 0; left: 0; right: 0; bottom: 0;\n    background: rgba(0, 0, 0, 0.6);\n    z-index: 10000;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    backdrop-filter: blur(4px);\n}\n\n.preview-content {\n    width: min(96vw, 1400px);\n    height: min(92vh, 900px);\n    background: var(--bg-color, #fff);\n    border-radius: 20px;\n    box-shadow: 0 25px 80px rgba(0, 0, 0, 0.35);\n    display: flex;\n    flex-direction: column;\n    overflow: hidden;\n    border: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n/* ‰æßËæπÈ¢ÑËßàÊ®°Âºè */\n.side-preview {\n    justify-content: flex-end;\n    align-items: flex-start;\n    padding: 15px;\n}\n\n.side-preview .preview-content {\n    width: min(65vw, 1000px);\n    height: min(88vh, 800px);\n    margin-top: 6vh;\n}\n\n/* Â§¥ÈÉ®Ê†∑Âºè */\n.preview-header {\n    padding: 28px 32px;\n    border-bottom: 1px solid #e5e7eb;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background: linear-gradient(135deg, #f8f9fa 0%, #f1f5f9 100%);\n    flex-shrink: 0;\n    position: relative;\n}\n\n.preview-header::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    height: 4px;\n    background: linear-gradient(90deg, #3b82f6, #8b5cf6, #06b6d4);\n    border-radius: 20px 20px 0 0;\n}\n\n.header-info h3 {\n    margin: 0 0 6px 0;\n    font-size: 20px;\n    font-weight: 700;\n    color: #1f2937;\n    letter-spacing: -0.025em;\n}\n\n.file-info {\n    font-size: 14px;\n    color: #6b7280;\n    font-weight: 500;\n}\n\n.close-btn {\n    background: none;\n    border: none;\n    font-size: 30px;\n    cursor: pointer;\n    color: #6b7280;\n    padding: 10px;\n    border-radius: 12px;\n    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n    line-height: 1;\n}\n\n.close-btn:hover {\n    background: rgba(239, 68, 68, 0.1);\n    color: #ef4444;\n    transform: scale(1.1);\n}\n\n/* Ë°®Ê†ºÂå∫Âüü */\n.preview-table-wrapper {\n    flex: 1;\n    overflow: hidden;\n    display: flex;\n    flex-direction: column;\n}\n\n.table-container {\n    flex: 1;\n    overflow: auto;\n    padding: 0;\n}\n\n.preview-table {\n    width: 100%;\n    border-collapse: collapse;\n    font-size: 14px;\n}\n\n.preview-table th {\n    background: linear-gradient(135deg, #f9fafb 0%, #f3f4f6 100%);\n    font-weight: 700;\n    color: #374151;\n    padding: 16px 20px;\n    text-align: left;\n    border-bottom: 2px solid #e5e7eb;\n    position: sticky;\n    top: 0;\n    z-index: 10;\n    font-size: 14px;\n    letter-spacing: 0.025em;\n    text-transform: uppercase;\n    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);\n}\n\n.preview-table td {\n    padding: 14px 20px;\n    border-bottom: 1px solid #f3f4f6;\n    color: #1f2937;\n    max-width: 220px;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    font-size: 14px;\n    transition: all 0.2s ease;\n}\n\n.preview-table tr:hover {\n    background: linear-gradient(135deg, #f9fafb 0%, #f0f9ff 100%);\n    transform: translateX(2px);\n    box-shadow: 0 2px 8px rgba(59, 130, 246, 0.1);\n}\n\n.row-number {\n    background: linear-gradient(135deg, #f3f4f6 0%, #e5e7eb 100%) !important;\n    font-weight: 600;\n    color: #6b7280 !important;\n    text-align: center !important;\n    width: 70px;\n    min-width: 70px;\n    max-width: 70px !important;\n    font-family: 'Consolas', 'Monaco', monospace;\n}\n\n.preview-table th.row-number {\n    background: linear-gradient(135deg, #e5e7eb 0%, #d1d5db 100%) !important;\n    font-weight: 800;\n}\n\n/* Á©∫Áä∂ÊÄÅ */\n.empty-tip {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    height: 100%;\n    color: #9ca3af;\n    padding: 60px 20px;\n}\n\n.empty-icon {\n    font-size: 64px;\n    margin-bottom: 16px;\n    opacity: 0.5;\n}\n\n.empty-tip p {\n    margin: 0;\n    font-size: 16px;\n}\n\n/* Âä®ÁîªÊïàÊûú */\n.modal-fade-enter-active,\n.modal-fade-leave-active {\n    transition: all 0.4s cubic-bezier(0.25, 0.8, 0.25, 1);\n}\n\n.modal-fade-enter-from,\n.modal-fade-leave-to {\n    opacity: 0;\n    backdrop-filter: blur(0px);\n}\n\n.modal-slide-enter-active {\n    transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n}\n\n.modal-slide-leave-active {\n    transition: all 0.3s cubic-bezier(0.55, 0.055, 0.675, 0.19);\n}\n\n.modal-slide-enter-from {\n    opacity: 0;\n    transform: scale(0.7) translateY(-60px) rotateX(15deg);\n}\n\n.modal-slide-leave-to {\n    opacity: 0;\n    transform: scale(0.95) translateY(30px);\n}\n\n/* Ë°®Ê†ºË°åÂä®Áîª */\n.preview-table tbody tr {\n    animation: fadeInRow 0.6s ease-out backwards;\n}\n\n.preview-table tbody tr:nth-child(1) { animation-delay: 0.1s; }\n.preview-table tbody tr:nth-child(2) { animation-delay: 0.15s; }\n.preview-table tbody tr:nth-child(3) { animation-delay: 0.2s; }\n.preview-table tbody tr:nth-child(4) { animation-delay: 0.25s; }\n.preview-table tbody tr:nth-child(5) { animation-delay: 0.3s; }\n.preview-table tbody tr:nth-child(n+6) { animation-delay: 0.35s; }\n\n@keyframes fadeInRow {\n    from {\n        opacity: 0;\n        transform: translateX(-20px);\n    }\n    to {\n        opacity: 1;\n        transform: translateX(0);\n    }\n}\n\n/* ÂìçÂ∫îÂºèËÆæËÆ° */\n@media (max-width: 768px) {\n    .preview-content {\n        width: 95vw;\n        height: 90vh;\n        margin: 20px;\n    }\n    \n    .side-preview .preview-content {\n        width: 95vw;\n        height: 85vh;\n        margin: 10px;\n    }\n    \n    .preview-header {\n        padding: 16px 20px;\n        flex-direction: column;\n        align-items: flex-start;\n        gap: 8px;\n    }\n    \n    .close-btn {\n        position: absolute;\n        top: 16px;\n        right: 20px;\n    }\n    \n    .preview-table th,\n    .preview-table td {\n        padding: 8px 12px;\n        font-size: 13px;\n    }\n    \n    .row-number {\n        width: 40px;\n        min-width: 40px;\n        max-width: 40px !important;\n    }\n}\n\n/* ÊªöÂä®Êù°Ê†∑Âºè */\n.table-container::-webkit-scrollbar {\n    width: 8px;\n    height: 8px;\n}\n\n.table-container::-webkit-scrollbar-track {\n    background: #f1f5f9;\n}\n\n.table-container::-webkit-scrollbar-thumb {\n    background: #cbd5e1;\n    border-radius: 4px;\n}\n\n.table-container::-webkit-scrollbar-thumb:hover {\n    background: #94a3b8;\n}\n</style>"],"mappings":";;EAAAA,GAAA;EAOiBC,KAAK,EAAC;;;EAEFA,KAAK,EAAC;AAAgB;;EAClBA,KAAK,EAAC;AAAa;;EAV5CD,GAAA;EAY8BC,KAAK,EAAC;;;EASfA,KAAK,EAAC;AAAuB;;EArBlDD,GAAA;EAsBmDC,KAAK,EAAC;;;EAC1BA,KAAK,EAAC;AAAe;;EAWZA,KAAK,EAAC;AAAY;;EAlC1DD,GAAA;EAyCgCC,KAAK,EAAC;;;uBAxCtCC,YAAA,CAgDaC,WAAA;IAhDDC,IAAI,EAAC;EAAY;IAD7BC,OAAA,EAAAC,QAAA,CAEI,MA8CM,CA9CKC,MAAA,CAAAC,IAAI,I,cAAfC,mBAAA,CA8CM;MAhDVT,GAAA;MAGQC,KAAK,EAHbS,eAAA,EAGc,eAAe;QAAA,gBACKH,MAAA,CAAAI;MAAa;MACtCC,OAAK,EALdC,cAAA,CAKqBC,MAAA,CAAAC,OAAO;QACpBC,YAAA,CAyCab,WAAA;MAzCDC,IAAI,EAAC,aAAa;MAACa,MAAM,EAAN;;MANvCZ,OAAA,EAAAC,QAAA,CAOY,MAuCM,CAvC6BC,MAAA,CAAAC,IAAI,I,cAAvCC,mBAAA,CAuCM,OAvCNS,UAuCM,GAtCFC,mBAAA,QAAW,EACXC,mBAAA,CASM,OATNC,UASM,GARFD,mBAAA,CAMM,OANNE,UAMM,GALFF,mBAAA,CAAkD,YAAAG,gBAAA,CAA3ChB,MAAA,CAAAiB,WAAW,EAAEpB,IAAI,oCACMG,MAAA,CAAAiB,WAAW,I,cAAzCf,mBAAA,CAGO,QAHPgB,UAGO,EAAAF,gBAAA,CAFAT,MAAA,CAAAY,cAAc,CAACnB,MAAA,CAAAiB,WAAW,CAACG,IAAI,KAAI,KACtC,GAAAJ,gBAAA,CAAGhB,MAAA,CAAAiB,WAAW,CAACI,IAAI,SAAQ,OAAK,GAAAL,gBAAA,CAAGhB,MAAA,CAAAiB,WAAW,CAACK,OAAO,SAAQ,KAClE,mBAfxBV,mBAAA,e,GAiBoBC,mBAAA,CAAqD;QAA7CnB,KAAK,EAAC,WAAW;QAAEW,OAAK,EAAEE,MAAA,CAAAC;SAAS,GAAC,E,GAGhDI,mBAAA,YAAe,EACfC,mBAAA,CAwBM,OAxBNU,UAwBM,GAvBSvB,MAAA,CAAAwB,WAAW,CAACC,MAAM,I,cAA7BvB,mBAAA,CAkBM,OAlBNwB,UAkBM,GAjBFb,mBAAA,CAgBQ,SAhBRc,UAgBQ,GAfJd,mBAAA,CAMQ,gBALJA,mBAAA,CAIK,a,0BAHDA,mBAAA,CAA6B;QAAzBnB,KAAK,EAAC;MAAY,GAAC,GAAC,uB,kBACxBQ,mBAAA,CACmC0B,SAAA,QA5BvEC,WAAA,CA2ByDtB,MAAA,CAAAuB,YAAY,EAAtBC,MAAM;6BAAjB7B,mBAAA,CACmC;UAA9BT,G