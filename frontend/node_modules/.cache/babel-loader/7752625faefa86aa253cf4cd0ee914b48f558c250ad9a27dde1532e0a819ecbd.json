{"ast":null,"code":"import { ref, onMounted, onUnmounted } from 'vue';\nimport { RouterLink } from 'vue-router';\nexport default {\n  __name: 'TopBar',\n  props: {\n    actions: {\n      type: Array,\n      default: () => [{\n        type: 'button',\n        label: 'Home',\n        to: '/',\n        external: false\n      }]\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    const props = __props;\n\n    // 滚动隐藏逻辑\n    const isHidden = ref(false);\n    let lastScrollY = window.scrollY;\n    let ticking = false;\n    function handleScroll() {\n      if (!ticking) {\n        window.requestAnimationFrame(() => {\n          const currentY = window.scrollY;\n          isHidden.value = currentY > lastScrollY && currentY > 30;\n          lastScrollY = currentY;\n          ticking = false;\n        });\n        ticking = true;\n      }\n    }\n\n    // 汉堡菜单状态切换\n    const isMenuOpen = ref(false);\n    function toggleMenu() {\n      isMenuOpen.value = !isMenuOpen.value;\n    }\n\n    // 点击空白区域关闭菜单\n    function handleClickOutside(event) {\n      const nav = document.querySelector('.top-bar');\n      if (nav && !nav.contains(event.target)) {\n        isMenuOpen.value = false;\n      }\n    }\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n      document.addEventListener('click', handleClickOutside);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n      document.removeEventListener('click', handleClickOutside);\n    });\n    const __returned__ = {\n      props,\n      isHidden,\n      get lastScrollY() {\n        return lastScrollY;\n      },\n      set lastScrollY(v) {\n        lastScrollY = v;\n      },\n      get ticking() {\n        return ticking;\n      },\n      set ticking(v) {\n        ticking = v;\n      },\n      handleScroll,\n      isMenuOpen,\n      toggleMenu,\n      handleClickOutside,\n      ref,\n      onMounted,\n      onUnmounted,\n      get RouterLink() {\n        return RouterLink;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","RouterLink","props","__props","isHidden","lastScrollY","window","scrollY","ticking","handleScroll","requestAnimationFrame","currentY","value","isMenuOpen","toggleMenu","handleClickOutside","event","nav","document","querySelector","contains","target","addEventListener","removeEventListener"],"sources":["D:/Learning Material/Git/Fuck-Charts/newArchitecture/frontend_vue_design/src/components/TopBar.vue"],"sourcesContent":["<template>\n    <header class=\"top-bar\" :class=\"{ 'top-bar--hidden': isHidden }\">\n        <nav class=\"nav-container\">\n        <!-- LOGO -->\n        <div class=\"logo dynamic-gradient-text\">Fuck Charts</div>\n\n        <!-- 汉堡图标：小屏显示 -->\n        <div class=\"burger-container\" @click=\"toggleMenu\">\n            <span class=\"burger-icon\">☰</span>\n        </div>\n\n        <!-- 菜单项 -->\n        <div class=\"nav-actions\" :class=\"{ 'show-menu': isMenuOpen }\">\n            <template v-for=\"(item, idx) in actions\" :key=\"idx\">\n            <!-- 普通按钮 -->\n            <button\n                v-if=\"item.type === 'button' && !item.to\"\n                class=\"nav-btn\"\n                @click=\"item.onClick\"\n            >\n                {{ item.label }}\n            </button>\n\n            <!-- 内部路由跳转 -->\n            <RouterLink\n                v-else-if=\"item.type === 'button' && item.to && !item.external\"\n                :to=\"item.to\"\n                class=\"nav-btn router-link\"\n            >\n                {{ item.label }}\n            </RouterLink>\n\n            <!-- 外部链接跳转 -->\n            <a\n                v-else-if=\"item.type === 'button' && item.to && item.external\"\n                :href=\"item.to\"\n                class=\"nav-btn external-link\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n            >\n                {{ item.label }}\n            </a>\n            </template>\n        </div>\n        </nav>\n    </header>\n</template>\n\n<script setup>\n/* eslint-disable */\n    import { ref, onMounted, onUnmounted } from 'vue'\n    import { RouterLink } from 'vue-router'\n\n    const props = defineProps({\n    actions: {\n        type: Array,\n        default: () => [\n        { type: 'button', label: 'Home', to: '/', external: false }\n        ],\n    },\n    })\n\n    // 滚动隐藏逻辑\n    const isHidden = ref(false)\n    let lastScrollY = window.scrollY\n    let ticking = false\n\n    function handleScroll() {\n    if (!ticking) {\n        window.requestAnimationFrame(() => {\n        const currentY = window.scrollY\n        isHidden.value = currentY > lastScrollY && currentY > 30\n        lastScrollY = currentY\n        ticking = false\n        })\n        ticking = true\n    }\n    }\n\n    // 汉堡菜单状态切换\n    const isMenuOpen = ref(false)\n    function toggleMenu() {\n    isMenuOpen.value = !isMenuOpen.value\n    }\n\n    // 点击空白区域关闭菜单\n    function handleClickOutside(event) {\n        const nav = document.querySelector('.top-bar')\n        if (nav && !nav.contains(event.target)) {\n            isMenuOpen.value = false\n    }\n    }\n\n    onMounted(() => {\n        window.addEventListener('scroll', handleScroll)\n        document.addEventListener('click', handleClickOutside)\n    })\n    onUnmounted(() => {\n        window.removeEventListener('scroll', handleScroll)\n        document.removeEventListener('click', handleClickOutside)\n    })\n</script>\n\n<style scoped>\n/* 顶部导航栏样式 */\n.top-bar {\n    position: fixed;\n    top: 2%;\n    left: 50%;\n    transform: translateX(-50%);\n    background-color: var(--bg-color);\n    backdrop-filter: saturate(180%) blur(10px);\n    border-radius: 12px;\n    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.15);\n    padding: 0 20px;\n    z-index: 9999;\n    width: 90%;\n    max-width: 1100px;\n    height: 70px;\n    display: flex;\n    align-items: center;\n    transition: top 0.35s cubic-bezier(0.4, 0, 0.2, 1), opacity 0.25s;\n}\n\n.top-bar--hidden {\n    top: -100px;\n    opacity: 0;\n    pointer-events: none;\n}\n\n/* 导航布局容器 */\n.nav-container {\n    width: 100%;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n\n/* LOGO 动态渐变文字 */\n.logo {\n    flex-shrink: 0;\n}\n.dynamic-gradient-text {\n    font-weight: 900;\n    font-size: 2.3rem;\n    background: linear-gradient(270deg, #ff416c, #fa83bf, #1f97ff, #12d8fa, #50f6a0, #ff416c);\n    background-size: 1200% 1200%;\n    -webkit-background-clip: text;\n    background-clip: text;\n    -webkit-text-fill-color: transparent;\n    animation: gradientMove 20s ease infinite;\n    user-select: none;\n}\n\n@keyframes gradientMove {\n    0% {\n        background-position: 0% 50%;\n    }\n    50% {\n        background-position: 100% 50%;\n    }\n    100% {\n        background-position: 0% 50%;\n}\n}\n\n/* 按钮样式 */\n.nav-btn {\n    background: transparent;\n    border: none;\n    font-size: 1.01rem;\n    font-weight: 600;\n    cursor: pointer;\n    color: var(--text-color);\n    padding: 6px 14px;\n    border-radius: 8px;\n    transition: background-color 0.3s ease;\n    text-decoration: none;\n    display: inline-block;\n    text-align: center;\n}\n\n.nav-btn:hover {\n    background-color: rgba(136, 133, 133, 0.1);\n}\n\n/* 汉堡菜单图标 */\n.burger-container {\n    display: none;\n    cursor: pointer;\n    font-size: 1.7rem;\n    user-select: none;\n    color: var(--text-color);\n}\n\n/* 按钮容器 */\n.nav-actions {\n    display: flex;\n    gap: 8px;\n    align-items: center;\n    justify-content: flex-end;\n    transition: max-height 0.3s ease;\n}\n\n/* 响应式设计：小屏模式 */\n@media (max-width: 990px) {\n    .top-bar {\n        right: 3%;\n        width: auto;\n        height: auto;\n        padding: 12px;\n        flex-direction: column;\n        align-items: flex-start;\n    }\n\n    .nav-container {\n        flex-direction: column;\n        align-items: stretch;\n        width: 100%;\n    }\n\n    .dynamic-gradient-text,\n    .logo {\n        font-size: 1.3rem;\n    }\n\n    .burger-container {\n        display: block;\n        /* align-self: flex-end; */\n    }\n\n    .nav-actions {\n        display: none;\n        flex-direction: column;\n        align-items: flex-start;\n        width: 100%;\n        gap: 10px;\n        margin-top: 12px;\n    }\n\n    .nav-actions.show-menu {\n        display: flex;\n    }\n\n    .nav-btn {\n        width: 100%;\n        text-align: left;\n        font-size: 1rem;\n        padding: 10px 12px;\n    }\n}\n</style>\n"],"mappings":"AAkDI,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAW,QAAQ,KAAI;AAChD,SAASC,UAAU,QAAQ,YAAW;;;;;;;;;;;;;;;;;;;IAF1C;IAII,MAAMC,KAAK,GAAGC,OAAA;;IASd;IACA,MAAMC,QAAQ,GAAGN,GAAG,CAAC,KAAK;IAC1B,IAAIO,WAAW,GAAGC,MAAM,CAACC,OAAM;IAC/B,IAAIC,OAAO,GAAG,KAAI;IAElB,SAASC,YAAYA,CAAA,EAAG;MACxB,IAAI,CAACD,OAAO,EAAE;QACVF,MAAM,CAACI,qBAAqB,CAAC,MAAM;UACnC,MAAMC,QAAQ,GAAGL,MAAM,CAACC,OAAM;UAC9BH,QAAQ,CAACQ,KAAK,GAAGD,QAAQ,GAAGN,WAAW,IAAIM,QAAQ,GAAG,EAAC;UACvDN,WAAW,GAAGM,QAAO;UACrBH,OAAO,GAAG,KAAI;QACd,CAAC;QACDA,OAAO,GAAG,IAAG;MACjB;IACA;;IAEA;IACA,MAAMK,UAAU,GAAGf,GAAG,CAAC,KAAK;IAC5B,SAASgB,UAAUA,CAAA,EAAG;MACtBD,UAAU,CAACD,KAAK,GAAG,CAACC,UAAU,CAACD,KAAI;IACnC;;IAEA;IACA,SAASG,kBAAkBA,CAACC,KAAK,EAAE;MAC/B,MAAMC,GAAG,GAAGC,QAAQ,CAACC,aAAa,CAAC,UAAU;MAC7C,IAAIF,GAAG,IAAI,CAACA,GAAG,CAACG,QAAQ,CAACJ,KAAK,CAACK,MAAM,CAAC,EAAE;QACpCR,UAAU,CAACD,KAAK,GAAG,KAAI;MAC/B;IACA;IAEAb,SAAS,CAAC,MAAM;MACZO,MAAM,CAACgB,gBAAgB,CAAC,QAAQ,EAAEb,YAAY;MAC9CS,QAAQ,CAACI,gBAAgB,CAAC,OAAO,EAAEP,kBAAkB;IACzD,CAAC;IACDf,WAAW,CAAC,MAAM;MACdM,MAAM,CAACiB,mBAAmB,CAAC,QAAQ,EAAEd,YAAY;MACjDS,QAAQ,CAACK,mBAAmB,CAAC,OAAO,EAAER,kBAAkB;IAC5D,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}