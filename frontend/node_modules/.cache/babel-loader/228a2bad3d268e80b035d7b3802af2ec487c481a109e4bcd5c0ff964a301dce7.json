  }, null, 8 /* PROPS */, [\"files\"]), _createCommentVNode(\" Êñá‰ª∂ÁªìÊûÑÈù¢Êùø \"), _createVNode($setup[\"FileStructurePanel\"], {\n    show: $setup.showFileStructure,\n    files: $setup.workspaceFiles,\n    \"initial-position\": {\n      x: 300,\n      y: 200\n    },\n    onClose: _cache[3] || (_cache[3] = $event => $setup.showFileStructure = false),\n    onFileChange: $setup.handleFileStructureChange\n  }, null, 8 /* PROPS */, [\"show\", \"files\"]), _createCommentVNode(\" Â∑•‰ΩúÂå∫Êñá‰ª∂‰ø°ÊÅØÊåâÈíÆ \"), $setup.workspaceFiles.length > 0 ? (_openBlock(), _createElementBlock(\"div\", _hoisted_4, [_createElementVNode(\"button\", {\n    class: \"info-btn\",\n    onClick: _cache[4] || (_cache[4] = $event => $setup.showFileStructure = true),\n    title: \"Show file structure\"\n  }, [_cache[5] || (_cache[5] = _createElementVNode(\"span\", {\n    class: \"info-icon\"\n  }, \"üìä\", -1 /* HOISTED */)), _createElementVNode(\"span\", null, _toDisplayString($setup.workspaceFiles.length), 1 /* TEXT */)])])) : _createCommentVNode(\"v-if\", true)], 64 /* STABLE_FRAGMENT */);\n}","map":{"version":3,"names":["class","key","_createElementBlock","_Fragment","_createElementVNode","_hoisted_1","_createVNode","$setup","actions","UploadFiles","DataPreview","openHistory","_createCommentVNode","position","collapsedWidth","expandedWidth","default","_withCtx","onSelect","onChartTypeSelect","_","selectedChartType","onChartGenerated","handleChartGenerated","onPreviewChart","handlePreviewChart","_hoisted_2","_hoisted_3","chartOption","_createBlock","option","show","showHistory","chartHistory","onClose","_cache","$event","onPreview","previewChart","onDelete","deleteChart","showFileUpload","workspaceFiles","onWorkspaceUpdated","handleWorkspaceUpdate","showDataPreview","currentDataFile","previewData","files","onRemove","handleWorkspaceRemove","handleWorkspacePreview","onClear","handleWorkspaceClear","showFileStructure","x","y","onFileChange","handleFileStructureChange","length","_hoisted_4","onClick","title","_toDisplayString"],"sources":["D:\\Learning Material\\Git\\Fuck-Charts\\newArchitecture\\frontend_vue_design\\src\\views\\ChartVisualizations.vue"],"sourcesContent":["<template>\n<div class=\"py-5\">\n  <TopBar :actions=\"[\n    { type: 'button', label: 'Files', onClick: UploadFiles },\n    { type: 'button', label: 'Data Preview', onClick: DataPreview},\n    { type: 'button', label: 'Chart History', onClick: openHistory},\n    { type: 'button', label: 'Dashboard', },\n    { type: 'button', label: 'Instruction', },\n    { type: 'button', label: 'Back2Home', to: '/'},\n  ]\"/>\n</div>\n\n<!-- Â∑¶‰æßËæπÊ†è -->\n<SideBar\nposition=\"left\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"180\">\n  <ChartTypeSelector @select=\"onChartTypeSelect\" />\n</SideBar>\n\n<!-- Âè≥‰æßËæπÊ†è -->\n<SideBar\nposition=\"right\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"400\">\n  <DataMappingPanel\n    :selected-chart-type=\"selectedChartType\"\n    @chart-generated=\"handleChartGenerated\"\n    @preview-chart=\"handlePreviewChart\"\n  />\n</SideBar>\n\n<!-- ÁªòÂà∂Âå∫ -->\n<div class=\"main-content\">\n  <section class=\"chart-workspace\">\n    <ChartDisplay v-if=\"chartOption\" :option=\"chartOption\" />\n  </section>\n</div>\n\n<!-- ÂõæË°®Â≠òÂÇ®Âå∫ -->\n<ChartHistoryModal\n  :show=\"showHistory\"\n  :chartHistory=\"chartHistory\"\n  @close=\"showHistory = false\"\n  @preview=\"previewChart\"\n  @delete=\"deleteChart\"\n/>\n\n<!-- Êñá‰ª∂‰∏ä‰º†ÂºπÁ™ó -->\n<FileUploadModal\n  v-if=\"showFileUpload\"\n  :show=\"showFileUpload\"\n  :workspaceFiles=\"workspaceFiles\"\n  @close=\"showFileUpload = false\"\n  @workspace-updated=\"handleWorkspaceUpdate\"\n/>\n\n<!-- Êï∞ÊçÆÈ¢ÑËßàÂºπÁ™ó -->\n<DataPreviewModal\n  v-if=\"showDataPreview\"\n  :show=\"showDataPreview\"\n  :current-file=\"currentDataFile\"\n  :preview-data=\"previewData\"\n  @close=\"showDataPreview = false\"\n/>\n\n<!-- Êñá‰ª∂Â∑•‰ΩúÂå∫ -->\n<FileWorkspace\n  :files=\"workspaceFiles\"\n  @remove=\"handleWorkspaceRemove\"\n  @preview=\"handleWorkspacePreview\"\n  @clear=\"handleWorkspaceClear\"\n/>\n\n<!-- Êñá‰ª∂ÁªìÊûÑÈù¢Êùø -->\n<FileStructurePanel\n  :show=\"showFileStructure\"\n  :files=\"workspaceFiles\"\n  :initial-position=\"{ x: 300, y: 200 }\"\n  @close=\"showFileStructure = false\"\n  @file-change=\"handleFileStructureChange\"\n/>\n\n<!-- Â∑•‰ΩúÂå∫Êñá‰ª∂‰ø°ÊÅØÊåâÈíÆ -->\n<div v-if=\"workspaceFiles.length > 0\" class=\"workspace-info-btn\">\n  <button\n    class=\"info-btn\"\n    @click=\"showFileStructure = true\"\n    title=\"Show file structure\">\n    <span class=\"info-icon\">üìä</span>\n    <span>{{ workspaceFiles.length }}</span>\n  </button>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport TopBar from '../components/TopBar.vue'\nimport SideBar from '../components/SideBar.vue'\nimport ChartTypeSelector from '../components/chartIcons.vue'\nimport ChartDisplay from '../components/ChartDisplay.vue'\nimport ChartHistoryModal from '../components/ChartHistoryModal.vue'\nimport FileUploadModal from '../components/FileUploadModal.vue'\nimport DataPreviewModal from '../components/DataPreviewModal.vue'\nimport FileWorkspace from '../components/FileWorkspace.vue'\nimport FileStructurePanel from '../components/FileStructurePanel.vue'\nimport DataMappingPanel from '../components/DataMappingPanel.vue'\nimport { getFilePreview } from '../services/FileServices.js'\nimport { globalDataMapper } from '../utils/dataMappingUtils.js'\nimport {ref, nextTick} from 'vue'\n\n// Êñá‰ª∂‰∏ä‰º†Áõ∏ÂÖ≥\nconst showFileUpload = ref(false)\nconst showFileStructure = ref(false)\nconst selectedChartType = ref('bar')\n\nfunction UploadFiles() {\n  showFileUpload.value = true\n}\n\n// Êï∞ÊçÆÈ¢ÑËßàÁõ∏ÂÖ≥\nconst showDataPreview = ref(false)\nconst currentDataFile = ref(null)\nconst previewData = ref([])\n\n// Â∑•‰ΩúÂå∫Áõ∏ÂÖ≥\nconst workspaceFiles = ref([])\n\nfunction DataPreview() {\n  // ËøôÈáåÂèØ‰ª•Ê†πÊçÆÂΩìÂâçÂõæË°®Ëé∑ÂèñÂØπÂ∫îÁöÑÊï∞ÊçÆÊñá‰ª∂‰ø°ÊÅØ\n  currentDataFile.value = {\n    name: 'currentFile.csv'\n  }\n  // ËøôÈáåÂèØ‰ª•Ëé∑ÂèñÂΩìÂâçÂõæË°®ÁöÑÊï∞ÊçÆÁî®‰∫éÈ¢ÑËßà\n  previewData.value = [\n    { column1: 'A', column2: 100 },\n    { column1: 'B', column2: 200 },\n    { column1: 'C', column2: 300 }\n  ]\n  showDataPreview.value = true\n}\n\n// ÊµãËØïÂõæË°®ÁªòÂà∂\nconst chartOption = ref({\n  title: { text: 'Á§∫‰æãÂõæË°®' },\n  tooltip: {},\n  xAxis: { data: ['A', 'B', 'C', 'D'] },\n  yAxis: {},\n  series: [{ type: 'line', data: [23, 45, 12, 37] }]\n})\n\n// ÂõæË°®Â≠òÂÇ®Âå∫ÔºàÂèØ‰ªéÂêéÁ´ØËé∑ÂèñÔºâ\nconst chartHistory = ref([\n  {\n    title: 'ÈîÄÂîÆÊü±Áä∂Âõæ',\n    option: {\n      title: { text: 'ÈîÄÂîÆÊü±Áä∂Âõæ' },\n      tooltip: {},\n      xAxis: { data: ['‰∏ÄÂ≠£Â∫¶', '‰∫åÂ≠£Â∫¶', '‰∏âÂ≠£Â∫¶', 'ÂõõÂ≠£Â∫¶'] },\n      yAxis: {},\n      series: [{ type: 'bar', data: [120, 200, 150, 80] }]\n    }\n  },\n  {\n    title: 'ÊäòÁ∫øË∂ãÂäø',\n    option: {\n      title: { text: 'ÊäòÁ∫øË∂ãÂäø' },\n      tooltip: {},\n      xAxis: { data: ['1Êúà', '2Êúà', '3Êúà', '4Êúà'] },\n      yAxis: {},\n      series: [{ type: 'line', data: [30, 50, 80, 65] }]\n    }\n  },\n  {\n    title: 'È•ºÂõæÁ§∫‰æã',\n    option: {\n      title: { text: 'È•ºÂõæÁ§∫‰æã' },\n      tooltip: {},\n      series: [\n        {\n          type: 'pie',\n          radius: '60%',\n          data: [\n            { value: 40, name: 'AÁ±ª' },\n            { value: 32, name: 'BÁ±ª' },\n            { value: 28, name: 'CÁ±ª' }\n          ]\n        }\n      ]\n    }\n  }\n])\n\nconst showHistory = ref(false)\nfunction openHistory() {\n  showHistory.value = true\n}\nfunction previewChart(item) {\n  chartOption.value = null\n  nextTick(() => {\n    chartOption.value = JSON.parse(JSON.stringify(item.option))\n    showHistory.value = false\n  })\n}\nfunction deleteChart(idx) {\n  chartHistory.value.splice(idx, 1)\n}\n\n// ÂõæË°®ÈÄâÊã©\nfunction onChartTypeSelect(type) {\n    console.log(`Selected chart type: ${type}`)\n    selectedChartType.value = type\n    // ÂèØÊ∑ªÂä†Â§ÑÁêÜ‰∏∫Âä†ËΩΩÂõæË°®Á±ªÂûã„ÄÅÂàáÊç¢ÁªÑ‰ª∂Á≠âÂäüËÉΩ\n}\n\n// Â∑•‰ΩúÂå∫Áõ∏ÂÖ≥ÊñπÊ≥ï\nfunction handleWorkspaceUpdate(files) {\n    workspaceFiles.value = [...files]\n    console.log('Workspace updated:', workspaceFiles.value.length, 'files')\n}\n\nfunction handleWorkspaceRemove(file) {\n    console.log('File removed from workspace:', file.name)\n    // ‰ªéÂ∑•‰ΩúÂå∫Êï∞ÁªÑ‰∏≠ÁßªÈô§Êñá‰ª∂\n    const index = workspaceFiles.value.findIndex(f => f.id === file.id)\n    if (index !== -1) {\n        workspaceFiles.value.splice(index, 1)\n    }\n}\n\nfunction handleWorkspacePreview(file) {\n    // È¢ÑËßàÂ∑•‰ΩúÂå∫‰∏≠ÁöÑÊñá‰ª∂\n    console.log('Preview workspace file:', file.name)\n    currentDataFile.value = file\n    \n    // ‰ΩøÁî®‰∏é FileUploadModal Áõ∏ÂêåÁöÑÈ¢ÑËßàÈÄªËæë\n    if (file.previewData) {\n        // Â¶ÇÊûúÊñá‰ª∂Â∑≤ÁªèÊúâÈ¢ÑËßàÊï∞ÊçÆÔºåÁõ¥Êé•‰ΩøÁî®\n        previewData.value = file.previewData\n        showDataPreview.value = true\n    } else {\n        // Âê¶ÂàôËØªÂèñÊñá‰ª∂ÂÜÖÂÆπ\n        loadFilePreview(file)\n    }\n}\n\n// Âä†ËΩΩÊñá‰ª∂È¢ÑËßàÊï∞ÊçÆ\nasync function loadFilePreview(file) {\n    try {\n        const previewResult = await getFilePreview(file.id)\n        previewData.value = previewResult.data\n        showDataPreview.value = true\n    } catch (error) {\n        console.error('Preview failed:', error)\n        alert('Preview failed: ' + error.message)\n    }\n}\n\nfunction handleWorkspaceClear() {\n    console.log('Workspace cleared')\n    workspaceFiles.value = []\n}\n\n// Êñá‰ª∂ÁªìÊûÑÈù¢ÊùøÁõ∏ÂÖ≥\nfunction handleFileStructureChange(file) {\n    console.log('File structure changed:', file.name)\n    // Â§ÑÁêÜÊñá‰ª∂ÁªìÊûÑÂèòÂåñ\n}\n\n// ÂõæË°®ÁîüÊàêÁõ∏ÂÖ≥\nfunction handleChartGenerated(chartData) {\n    console.log('Chart generated:', chartData)\n    \n    // Êõ¥Êñ∞ÂõæË°®ÊòæÁ§∫\n    chartOption.value = null\n    nextTick(() => {\n        chartOption.value = chartData.option\n    })\n    \n    // ‰øùÂ≠òÂà∞ÂéÜÂè≤ËÆ∞ÂΩï\n    const historyItem = {\n        title: `${selectedChartType.value} Chart`,\n        option: chartData.option,\n        config: chartData.config,\n        timestamp: chartData.timestamp\n    }\n    chartHistory.value.unshift(historyItem)\n    \n    // ÈôêÂà∂ÂéÜÂè≤ËÆ∞ÂΩïÊï∞Èáè\n    if (chartHistory.value.length > 50) {\n        chartHistory.value = chartHistory.value.slice(0, 50)\n    }\n}\n\nfunction handlePreviewChart(previewChartOption) {\n    console.log('Preview chart:', previewChartOption)\n    \n    // Êõ¥Êñ∞ÂõæË°®ÊòæÁ§∫\n    chartOption.value = null\n    nextTick(() => {\n        chartOption.value = previewChartOption\n    })\n}\n</script>\n\n<style scoped>\n.sideBar-btn {\nbackground: transparent;\nborder: none;\nfont-size: 1.1rem;\nfont-weight: 600;\ncursor: pointer;\ncolor: var(--text-color);\npadding: 6px 14px;\nborder-radius: 8px;\ntransition:\n    background-color 0.3s cubic-bezier(.4,2,.6,1),\n    transform 0.2s cubic-bezier(.4,2,.6,1),\n    box-shadow 0.2s;\ntext-align: center;\ntext-decoration: none;\n}\n.sideBar-btn:hover {\nbackground-color: rgba(136, 133, 133, 0.1);\ntransform: translateY(-2px) scale(1.02);\nbox-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n/* ÂõæË°®ÁªòÂà∂Âå∫ */\n.main-content {\ndisplay: flex;\ngap: 32px;\nmargin: 40px 60px 0 220px;\nmin-height: 500px;\n}\n@media (max-width: 767px) {\n  .main-content {\n    margin: 20px 30px 0 80px;\n    gap: 16px;\n    min-height: auto;\n  }\n}\n.chart-workspace {\nflex: 1;\nmin-width: 0;\ndisplay: flex;\nalign-items: center;\njustify-content: center;\n}\n\n/* ÂõæË°®ÁºìÂ≠òÂå∫ */\n.chart-history {\nwidth: 220px;\nbackground: var(--bg-color, #fff);\nborder-radius: 10px;\nbox-shadow: 0 0px 1px var(--text-color);\npadding: 16px;\nflex-shrink: 0;\n}\n.chart-history-item {\npadding: 8px 0;\ncursor: pointer;\nborder-bottom: 1px solid #747272;\ntransition: background 0.2s;\n}\n.chart-history-item:hover {\nbackground: rgba(136, 133, 133, 0.1);\n}\n\n/* ÊãñÊãΩÁõ∏ÂÖ≥Ê†∑Âºè */\n.is-dragging {\ncursor: grabbing !important;\n}\n\n.is-dragging * {\ncursor: grabbing !important;\n}\n\n.drop-zone-active {\nborder-color: var(--primary-color, #5470c6) !important;\nbackground: var(--primary-bg, rgba(84, 112, 198, 0.05)) !important;\n}\n\n.drop-zone-hover {\nborder-color: var(--primary-color, #5470c6) !important;\nbackground: var(--primary-bg, rgba(84, 112, 198, 0.1)) !important;\n}\n\n/* ÊöóÈªëÊ®°ÂºèÂèòÈáè */\n@media (prefers-color-scheme: dark) {\n:root {\n  --bg-color: #1a1a1a;\n  --header-bg: #2d2d2d;\n  --border-color: #404040;\n  --text-color: #e0e0e0;\n  --text-secondary: #b0b0b0;\n  --hover-bg: rgba(255, 255, 255, 0.05);\n  --primary-color: #6366f1;\n  --primary-hover: #7c3aed;\n  --primary-bg: rgba(99, 102, 241, 0.1);\n}\n}\n</style>\n"],"mappings":";;EACKA,KAAK,EAAC;AAAM;;EAgCZA,KAAK,EAAC;AAAc;;EACdA,KAAK,EAAC;AAAiB;;EAlClCC,GAAA;EAoFsCD,KAAK,EAAC;;;uBApF5CE,mBAAA,CAAAC,SAAA,SACAC,mBAAA,CASM,OATNC,UASM,GARJC,YAAA,CAOIC,MAAA;IAPKC,OAAO,G;;;eAAmDD,MAAA,CAAAE;IAAW,G;;;eAA0DF,MAAA,CAAAG;IAAW,G;;;eAA0DH,MAAA,CAAAI;IAAW,G;;;;;;;;;;;0CAU1NC,mBAAA,UAAa,EACbN,YAAA,CAKUC,MAAA;IAJVM,QAAQ,EAAC,MAAM;IACdC,cAAc,EAAE,EAAE;IAClBC,aAAa,EAAE;;IAhBhBC,OAAA,EAAAC,QAAA,CAiBE,MAAiD,CAAjDX,YAAA,CAAiDC,MAAA;MAA7BW,QAAM,EAAEX,MAAA,CAAAY;IAAiB,G;IAjB/CC,CAAA;MAoBAR,mBAAA,UAAa,EACbN,YAAA,CASUC,MAAA;IARVM,QAAQ,EAAC,OAAO;IACfC,cAAc,EAAE,EAAE;IAClBC,aAAa,EAAE;;IAxBhBC,OAAA,EAAAC,QAAA,CAyBE,MAIE,CAJFX,YAAA,CAIEC,MAAA;MAHC,qBAAmB,EAAEA,MAAA,CAAAc,iBAAiB;MACtCC,gBAAe,EAAEf,MAAA,CAAAgB,oBAAoB;MACrCC,cAAa,EAAEjB,MAAA,CAAAkB;;IA5BpBL,CAAA;MAgCAR,mBAAA,SAAY,EACZR,mBAAA,CAIM,OAJNsB,UAIM,GAHJtB,mBAAA,CAEU,WAFVuB,UAEU,GADYpB,MAAA,CAAAqB,WAAW,I,cAA/BC,YAAA,CAAyDtB,MAAA;IAnC7DN,GAAA;IAmCsC6B,MAAM,EAAEvB,MAAA,CAAAqB;yCAnC9ChB,mBAAA,e,KAuCAA,mBAAA,WAAc,EACdN,YAAA,CAMEC,MAAA;IALCwB,IAAI,EAAExB,MAAA,CAAAyB,WAAW;IACjBC,YAAY,EAAE1B,MAAA,CAAA0B,YAAY;IAC1BC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAE7B,MAAA,CAAAyB,WAAW;IAClBK,SAAO,EAAE9B,MAAA,CAAA+B,YAAY;IACrBC,QAAM,EAAEhC,MAAA,CAAAiC;qDAGX5B,mBAAA,YAAe,EAEPL,MAAA,CAAAkC,cAAc,I,cADtBZ,YAAA,CAMEtB,MAAA;IAvDFN,GAAA;IAmDG8B,IAAI,