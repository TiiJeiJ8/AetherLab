frontend_vue_design\\src\\components\\TopBar.vue"],"sourcesContent":["<template>\n    <header class=\"top-bar\" :class=\"{ 'top-bar--hidden': isHidden }\">\n        <nav class=\"nav-container\">\n        <!-- LOGO -->\n        <div class=\"logo dynamic-gradient-text\">Fuck Charts</div>\n\n        <!-- 汉堡菜单图标（小屏可见） -->\n        <div class=\"burger-container\" @click=\"toggleMenu\">\n            <span class=\"burger-icon\">☰</span>\n        </div>\n\n        <!-- 菜单项（动画展开） -->\n        <transition name=\"menu-fade\">\n            <div class=\"nav-actions\" v-show=\"isMenuOpen || isDesktop\">\n            <template v-for=\"(item, idx) in actions\" :key=\"idx\">\n                <!-- 普通按钮 -->\n                <button\n                v-if=\"item.type === 'button' && !item.to\"\n                class=\"nav-btn\"\n                @click=\"item.onClick\"\n                >\n                {{ item.label }}\n                </button>\n\n                <!-- 内部路由跳转 -->\n                <RouterLink\n                v-else-if=\"item.type === 'button' && item.to && !item.external\"\n                :to=\"item.to\"\n                class=\"nav-btn\"\n                @click=\"isMenuOpen = false\"\n                >\n                {{ item.label }}\n                </RouterLink>\n\n                <!-- 外部链接 -->\n                <a\n                v-else-if=\"item.type === 'button' && item.to && item.external\"\n                :href=\"item.to\"\n                class=\"nav-btn\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n                >\n                {{ item.label }}\n                </a>\n            </template>\n            </div>\n        </transition>\n        </nav>\n    </header>\n</template>\n\n<script setup>\n/**/ */\n    import { ref, onMounted, onUnmounted, computed } from 'vue'\n    import { RouterLink } from 'vue-router'\n\n    const props = defineProps({\n    actions: {\n        type: Array,\n        default: () => [\n        { type: 'button', label: 'Home', to: '/', external: false }\n        ]\n    }\n    })\n\n    // 滚动隐藏逻辑\n    const isHidden = ref(false)\n    let lastScrollY = window.scrollY\n    let ticking = false\n    function handleScroll() {\n    if (!ticking) {\n        window.requestAnimationFrame(() => {\n        const currentY = window.scrollY\n        isHidden.value = currentY > lastScrollY && currentY > 30\n        lastScrollY = currentY\n        ticking = false\n        })\n        ticking = true\n    }\n    }\n\n    // 菜单状态\n    const isMenuOpen = ref(false)\n    const toggleMenu = () => {\n    isMenuOpen.value = !isMenuOpen.value\n    }\n\n    const isDesktop = computed(() => window.innerWidth >= 990)\n\n    // 点击空白处关闭菜单\n    function handleClickOutside(event) {\n    const nav = document.querySelector('.top-bar')\n    if (nav && !nav.contains(event.target)) {\n        isMenuOpen.value = false\n    }\n    }\n\n    onMounted(() => {\n    window.addEventListener('scroll', handleScroll)\n    document.addEventListener('click', handleClickOutside)\n    })\n    onUnmounted(() => {\n    window.removeEventListener('scroll', handleScroll)\n    document.removeEventListener('click', handleClickOutside)\n    })\n</script>\n\n<style scoped>\n/* 顶部导航栏样式 */\n.top-bar {\n    position: fixed;\n    top: 2%;\n    left: 50%;\n    transform: translateX(-50%);\n    background-color: var(--bg-color);\n    backdrop-filter: saturate(180%) blur(10px);\n    border-radius: 12px;\n    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.15);\n    padding: 0 20px;\n    z-index: 9999;\n    width: 90%;\n    max-width: 1100px;\n    height: 70px;\n    display: flex;\n    align-items: center;\n    transition: top 0.35s cubic-bezier(0.4, 0, 0.2, 1), opacity 0.25s;\n}\n\n.top-bar--hidden {\n    top: -100px;\n    opacity: 0;\n    pointer-events: none;\n}\n\n/* 导航布局容器 */\n.nav-container {\n    width: 100%;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n\n/* LOGO 动态渐变文字 */\n.logo {\n    flex-shrink: 0;\n}\n.dynamic-gradient-text {\n    font-weight: 900;\n    font-size: 2.3rem;\n    background: linear-gradient(270deg, #ff416c, #fa83bf, #1f97ff, #12d8fa, #50f6a0, #ff416c);\n    background-size: 1200% 1200%;\n    -webkit-background-clip: text;\n    background-clip: text;\n    -webkit-text-fill-color: transparent;\n    animation: gradientMove 20s ease infinite;\n    user-select: none;\n}\n\n@keyframes gradientMove {\n    0% {\n        background-position: 0% 50%;\n    }\n    50% {\n        background-position: 100% 50%;\n    }\n    100% {\n        background-position: 0% 50%;\n}\n}\n\n/* 按钮样式 */\n.nav-btn {\n    background: transparent;\n    border: none;\n    font-size: 1.01rem;\n    font-weight: 600;\n    cursor: pointer;\n    color: var(--text-color);\n    padding: 6px 14px;\n    border-radius: 8px;\n    transition: background-color 0.3s ease;\n    text-decoration: none;\n    display: inline-block;\n    text-align: center;\n}\n\n.nav-btn:hover {\n    background-color: rgba(136, 133, 133, 0.1);\n}\n\n/* 汉堡菜单图标 */\n.burger-container {\n    display: none;\n    cursor: pointer;\n    font-size: 1.7rem;\n    user-select: none;\n    color: var(--text-color);\n}\n\n/* 按钮容器 */\n.nav-actions {\n    display: flex;\n    gap: 8px;\n    align-items: center;\n    justify-content: flex-end;\n    transition: max-height 0.3s ease;\n}\n\n/* 响应式设计：小屏模式 */\n@media (max-width: 990px) {\n    .top-bar {\n        right: 5%;\n        width: auto;\n        height: auto;\n        padding: 12px;\n        flex-direction: column;\n        align-items: flex-start;\n    }\n\n    .nav-container {\n        flex-direction: column;\n        align-items: stretch;\n        width: 100%;\n    }\n\n    .dynamic-gradient-text,\n    .logo {\n        font-size: 1.3rem;\n        display: block;\n    }\n\n    .burger-container {\n        display: block;\n        align-self: flex-end;\n    }\n\n    .nav-actions {\n        display: none;\n        flex-direction: column;\n        align-items: flex-start;\n        width: 100%;\n        gap: 10px;\n        margin-top: 12px;\n    }\n\n    .nav-actions.show-menu {\n        display: flex;\n    }\n\n    .nav-btn {\n        width: 100%;\n        text-align: left;\n        font-size: 1rem;\n        padding: 10px 12px;\n    }\n}\n</style>\n"],"mappings":";;EAEaA,KAAK,EAAC;AAAe;;EAWjBA,KAAK,EAAC;AAAa;mBAbpC;mBAAA;;;uBACIC,mBAAA,CA+CS;IA/CDD,KAAK,EADjBE,eAAA,EACkB,SAAS;MAAA,mBAA8BC,IAAA,CAAAC;IAAQ;MACzDC,mBAAA,CA6CM,OA7CNC,UA6CM,GA5CNC,mBAAA,UAAa,E,0BACbF,mBAAA,CAAyD;IAApDL,KAAK,EAAC;EAA4B,GAAC,aAAW,sBAEnDO,mBAAA,kBAAqB,EACrBF,mBAAA,CAEM;IAFDL,KAAK,EAAC,kBAAkB;IAAEQ,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEP,IAAA,CAAAQ,UAAA,IAAAR,IAAA,CAAAQ,UAAA,IAAAD,IAAA,CAAU;gCAC5CL,mBAAA,CAAkC;IAA5BL,KAAK,EAAC;EAAa,GAAC,GAAC,oB,IAG/BO,mBAAA,eAAkB,EAClBK,YAAA,CAkCaC,WAAA;IAlCDC,IAAI,EAAC,WAAW;IAA5BC,SAkCa,EAlCb;;IAZRC,OAAA,EAAAC,QAAA,CAaY,MAgCM,C,gBAhCNZ,mBAAA,CAgCM,OAhCNa,UAgCM,I,kBA/BNjB,mBAAA,CA8BWkB,SAAA,QA5CvBC,WAAA,CAc4CjB,IAAA,CAAAkB,OAAO,EAdnD,CAc8BC,IAAI,EAAEC,GAAG;2BAdvCtB,mBAAA,CAAAkB,SAAA;QAAAK,GAAA,EAc2DD;MAAG,IAC9ChB,mBAAA,UAAa,EAEPe,IAAI,CAACG,IAAI,kBAAkBH,IAAI,CAACI,EAAE,I,cADxCzB,mBAAA,CAMS;QAtBzBuB,GAAA;QAkBgBxB,KAAK,EAAC,SAAS;QACdQ,OAAK,EAAEc,IAAI,CAACd;0BAEVc,IAAI,CAACK,KAAK,wBArB7BC,UAAA,KA0B2BN,IAAI,CAACG,IAAI,iBAAiBH,IAAI,CAACI,EAAE,KAAKJ,IAAI,CAACO,QAAQ,I,cAD9D5B,mBAAA,CAOakB,SAAA;QAhC7BK,GAAA;MAAA,IAwBgBjB,mBAAA,YAAe,EACfK,YAAA,CAOakB,qBAAA;QALZJ,EAAE,EAAEJ,IAAI,CAACI,EAAE;QACZ1B,KAAK,EAAC,SAAS;QACdQ,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAsB,MAAA,IAAE5B,IAAA,CAAA6B,UAAU;;QA7BlChB,OAAA,EAAAC,QAAA,CA+BgB,MAAgB,CA/BhCgB,gBAAA,CAAAC,gBAAA,CA+BmBZ,IAAI,CAACK,KAAK,iB;QA/B7BQ,CAAA;sGAoC2Bb,IAAI,CAACG,IAAI,iBAAiBH,IAAI,CAACI,EAAE,IAAIJ,IAAI,CAACO,QAAQ,I,cAD7D5B,mBAAA,CAQIkB,SAAA;QA3CpBK,GAAA;MAAA,IAkCgBjB,mBAAA,UAAa,EACbF,mBAAA,CAQI;QANH+B,IAAI,EAAEd,IAAI,CAACI,EAAE;QACd1B,KAAK,EAAC,SAAS;QACfqC,MAAM,EAAC,QAAQ;QACfC,GAAG,EAAC;0BAEDhB,IAAI,CAACK,KAAK,wBA1C7BY,UAAA,E,oDAAAhC,mBAAA,e;sEAa6CJ,IAAA,CAAA6B,UAAU,IAAI7B,IAAA,