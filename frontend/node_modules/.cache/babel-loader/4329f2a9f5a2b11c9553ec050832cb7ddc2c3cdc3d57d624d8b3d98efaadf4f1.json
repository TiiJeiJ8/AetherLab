]),\n      draggable: \"true\",\n      onDragstart: $event => $setup.startColumnDrag($event, column),\n      onDragend: $setup.endColumnDrag\n    }, [_createElementVNode(\"div\", _hoisted_18, [_createElementVNode(\"div\", _hoisted_19, [_cache[9] || (_cache[9] = _createElementVNode(\"span\", {\n      class: \"drag-handle\"\n    }, \"⋮⋮\", -1 /* HOISTED */)), _createTextVNode(\" \" + _toDisplayString(column.name), 1 /* TEXT */)]), _createElementVNode(\"div\", {\n      class: _normalizeClass([\"column-type\", column.type])\n    }, _toDisplayString(column.type), 3 /* TEXT, CLASS */)]), _createElementVNode(\"div\", _hoisted_20, [_createElementVNode(\"div\", _hoisted_21, [_cache[10] || (_cache[10] = _createElementVNode(\"span\", {\n      class: \"stat-label\"\n    }, \"非空:\", -1 /* HOISTED */)), _createElementVNode(\"span\", _hoisted_22, _toDisplayString(column.nonNull) + \"/\" + _toDisplayString($props.previewData.length), 1 /* TEXT */)]), _createElementVNode(\"div\", _hoisted_23, [_cache[11] || (_cache[11] = _createElementVNode(\"span\", {\n      class: \"stat-label\"\n    }, \"唯一:\", -1 /* HOISTED */)), _createElementVNode(\"span\", _hoisted_24, _toDisplayString(column.unique), 1 /* TEXT */)])]), _createElementVNode(\"div\", _hoisted_25, [_cache[12] || (_cache[12] = _createElementVNode(\"div\", {\n      class: \"sample-header\"\n    }, \"样本数据:\", -1 /* HOISTED */)), _createElementVNode(\"div\", _hoisted_26, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList(column.samples, (sample, i) => {\n      return _openBlock(), _createElementBlock(\"span\", {\n        key: i,\n        class: \"sample-value\"\n      }, _toDisplayString(sample), 1 /* TEXT */);\n    }), 128 /* KEYED_FRAGMENT */))])])], 42 /* CLASS, PROPS, NEED_HYDRATION */, _hoisted_17);\n  }), 128 /* KEYED_FRAGMENT */))], 2 /* CLASS */)])])], 36 /* STYLE, NEED_HYDRATION */)) : _createCommentVNode(\"v-if\", true);\n}","map":{"version":3,"names":["class","$props","show","_createElementBlock","key","style","_normalizeStyle","$setup","modalPosition","onMousedown","startDrag","ref","_createElementVNode","_hoisted_1","_hoisted_2","innerHTML","getFileIcon","fileInfo","name","_hoisted_3","_hoisted_4","_toDisplayString","_hoisted_5","columns","length","onClick","_cache","$event","_ctx","$emit","_hoisted_6","_hoisted_7","_hoisted_8","_hoisted_9","formatFileSize","size","_hoisted_10","_hoisted_11","previewData","_hoisted_12","_hoisted_13","split","pop","toUpperCase","_hoisted_14","_hoisted_15","_hoisted_16","_normalizeClass","active","viewMode","title","_createStaticVNode","_Fragment","_renderList","column","index","draggedColumn","draggable","onDragstart","startColumnDrag","onDragend","endColumnDrag","_hoisted_18","_hoisted_19","_createTextVNode","type","_hoisted_20","_hoisted_21","_hoisted_22","nonNull","_hoisted_23","_hoisted_24","unique","_hoisted_25","_hoisted_26","samples","sample","i","_hoisted_17","_createCommentVNode"],"sources":["D:\\Learning Material\\Git\\Fuck-Charts\\newArchitecture\\frontend_vue_design\\src\\components\\FileStructureModal.vue"],"sourcesContent":["<template>\n<div \n  v-if=\"show\"\n  class=\"file-structure-modal\"\n  :style=\"modalPosition\"\n  @mousedown=\"startDrag\"\n  ref=\"modalRef\"\n>\n  <div class=\"modal-header\">\n    <div class=\"modal-title\">\n      <span class=\"file-icon\" v-html=\"getFileIcon(fileInfo.name)\"></span>\n      <span class=\"file-name\">{{ fileInfo.name }}</span>\n      <span class=\"column-count\">{{ columns.length }} 列</span>\n    </div>\n    <button class=\"close-btn\" @click=\"$emit('close')\">×</button>\n  </div>\n  \n  <div class=\"modal-content\">\n    <div class=\"structure-info\">\n      <div class=\"info-row\">\n        <span class=\"info-label\">文件大小:</span>\n        <span class=\"info-value\">{{ formatFileSize(fileInfo.size) }}</span>\n      </div>\n      <div class=\"info-row\">\n        <span class=\"info-label\">数据行数:</span>\n        <span class=\"info-value\">{{ previewData.length }}</span>\n      </div>\n      <div class=\"info-row\">\n        <span class=\"info-label\">文件类型:</span>\n        <span class=\"info-value\">{{ fileInfo.name.split('.').pop().toUpperCase() }}</span>\n      </div>\n    </div>\n    \n    <div class=\"columns-section\">\n      <div class=\"section-header\">\n        <h4>列结构</h4>\n        <div class=\"view-toggle\">\n          <button \n            :class=\"{ active: viewMode === 'grid' }\"\n            @click=\"viewMode = 'grid'\"\n            title=\"网格视图\"\n          >\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <rect x=\"3\" y=\"3\" width=\"7\" height=\"7\"/>\n              <rect x=\"14\" y=\"3\" width=\"7\" height=\"7\"/>\n              <rect x=\"3\" y=\"14\" width=\"7\" height=\"7\"/>\n              <rect x=\"14\" y=\"14\" width=\"7\" height=\"7\"/>\n            </svg>\n          </button>\n          <button \n            :class=\"{ active: viewMode === 'list' }\"\n            @click=\"viewMode = 'list'\"\n            title=\"列表视图\"\n          >\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <line x1=\"8\" y1=\"6\" x2=\"21\" y2=\"6\"/>\n              <line x1=\"8\" y1=\"12\" x2=\"21\" y2=\"12\"/>\n              <line x1=\"8\" y1=\"18\" x2=\"21\" y2=\"18\"/>\n              <line x1=\"3\" y1=\"6\" x2=\"3.01\" y2=\"6\"/>\n              <line x1=\"3\" y1=\"12\" x2=\"3.01\" y2=\"12\"/>\n              <line x1=\"3\" y1=\"18\" x2=\"3.01\" y2=\"18\"/>\n            </svg>\n          </button>\n        </div>\n      </div>\n      \n      <div class=\"columns-container\" :class=\"viewMode\">\n        <div \n          v-for=\"(column, index) in columns\" \n          :key=\"index\"\n          class=\"column-item\"\n          :class=\"{ 'dragging': draggedColumn === column.name }\"\n          draggable=\"true\"\n          @dragstart=\"startColumnDrag($event, column)\"\n          @dragend=\"endColumnDrag\"\n        >\n          <div class=\"column-header\">\n            <div class=\"column-name\">\n              <span class=\"drag-handle\">⋮⋮</span>\n              {{ column.name }}\n            </div>\n            <div class=\"column-type\" :class=\"column.type\">\n              {{ column.type }}\n            </div>\n          </div>\n          <div class=\"column-stats\">\n            <div class=\"stat-item\">\n              <span class=\"stat-label\">非空:</span>\n              <span class=\"stat-value\">{{ column.nonNull }}/{{ previewData.length }}</span>\n            </div>\n            <div class=\"stat-item\">\n              <span class=\"stat-label\">唯一:</span>\n              <span class=\"stat-value\">{{ column.unique }}</span>\n            </div>\n          </div>\n          <div class=\"column-samples\">\n            <div class=\"sample-header\">样本数据:</div>\n            <div class=\"sample-values\">\n              <span \n                v-for=\"(sample, i) in column.samples\" \n                :key=\"i\"\n                class=\"sample-value\"\n              >\n                {{ sample }}\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport { ref, computed, onMounted, onUnmounted } from 'vue'\n\nconst props = defineProps({\n  show: {\n    type: Boolean,\n    default: false\n  },\n  fileInfo: {\n    type: Object,\n    required: true\n  },\n  previewData: {\n    type: Array,\n    default: () => []\n  }\n})\n\nconst emit = defineEmits(['close', 'column-drag'])\n\n// 视图模式\nconst viewMode = ref('grid')\n\n// 拖拽相关\nconst isDragging = ref(false)\nconst draggedColumn = ref(null)\nconst dragOffset = ref({ x: 0, y: 0 })\nconst modalPosition = ref({\n  position: 'fixed',\n  top: '50px',\n  left: '300px',\n  zIndex: 10000\n})\n\nconst modalRef = ref(null)\n\n// 计算列结构信息\nconst columns = computed(() => {\n  if (!props.previewData || props.previewData.length === 0) return []\n  \n  const columnNames = Object.keys(props.previewData[0] || {})\n  \n  return columnNames.map(name => {\n    const values = props.previewData.map(row => row[name]).filter(val => val !== null && val !== undefined && val !== '')\n    const uniqueValues = [...new Set(values)]\n    \n    // 检测数据类型\n    const type = detectColumnType(values)\n    \n    // 获取样本数据\n    const samples = uniqueValues.slice(0, 3).map(val => \n      String(val).length > 20 ? String(val).substring(0, 20) + '...' : String(val)\n    )\n    \n    return {\n      name,\n      type,\n      nonNull: values.length,\n      unique: uniqueValues.length,\n      samples\n    }\n  })\n})\n\n// 检测列数据类型\nfunction detectColumnType(values) {\n  if (values.length === 0) return 'unknown'\n  \n  const sample = values.slice(0, 10)\n  let numericCount = 0\n  let dateCount = 0\n  \n  for (const value of sample) {\n    if (!isNaN(Number(value)) && !isNaN(parseFloat(value))) {\n      numericCount++\n    }\n    if (isValidDate(value)) {\n      dateCount++\n    }\n  }\n  \n  if (numericCount / sample.length > 0.8) return 'numeric'\n  if (dateCount / sample.length > 0.6) return 'date'\n  return 'text'\n}\n\n// 检测是否为有效日期\nfunction isValidDate(value) {\n  const date = new Date(value)\n  return !isNaN(date.getTime()) && String(value).length > 6\n}\n\n// 获取文件图标\nfunction getFileIcon(fileName) {\n  const ext = fileName.split('.').pop().toLowerCase()\n  const iconMap = {\n    'xlsx': '📊',\n    'csv': '📋',\n    'json': '📄',\n    'txt': '📝'\n  }\n  return iconMap[ext] || '📁'\n}\n\n// 格式化文件大小\nfunction formatFileSize(bytes) {\n  if (bytes === 0) return '0 Bytes'\n  const k = 1024\n  const sizes = ['Bytes', 'KB', 'MB', 'GB']\n  const i = Math.floor(Math.log(bytes) / Math.log(k))\n  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\n}\n\n// 模态框拖拽\nfunction startDrag(event) {\n  if (event.target.closest('.close-btn') || event.target.closest('.column-item')) return\n  \n  isDragging.value = true\n  const rect = modalRef.value.getBoundingClientRect()\n  dragOffset.value = {\n    x: event.clientX - rect.left,\n    y: event.clientY - rect.top\n  }\n  \n  document.addEventListener('mousemove', handleDrag)\n  document.addEventListener('mouseup', stopDrag)\n  event.preventDefault()\n}\n\nfunction handleDrag(event) {\n  if (!isDragging.value) return\n  \n  const newX = event.clientX - dragOffset.value.x\n  const newY = event.clientY - dragOffset.value.y\n  \n  const maxX = window.innerWidth - 400\n  const maxY = window.innerHeight - 300\n  \n  modalPosition.value = {\n    position: 'fixed',\n    top: `${Math.max(0, Math.min(newY, maxY))}px`,\n    left: `${Math.max(0, Math.min(newX, maxX))}px`,\n    zIndex: 10000\n  }\n}\n\nfunction stopDrag() {\n  isDragging.value = false\n  document.removeEventListener('mousemove', handleDrag)\n  document.removeEventListener('mouseup', stopDrag)\n}\n\n// 列拖拽\nfunction startColumnDrag(event, column) {\n  draggedColumn.value = column.name\n  \n  // 设置拖拽数据\n  event.dataTransfer.setData('text/plain', JSON.stringify({\n    type: 'column',\n    fileName: props.fileInfo.name,\n    fileId: props.fileInfo.id,\n    columnName: column.name,\n    columnType: column.type,\n    columnData: column\n  }))\n  \n  event.dataTransfer.effectAllowed = 'copy'\n  \n  emit('column-drag', {\n    column,\n    fileInfo: props.fileInfo\n  })\n}\n\nfunction endColumnDrag() {\n  draggedColumn.value = null\n}\n\n// 清理事件监听\nonUnmounted(() => {\n  document.removeEventListener('mousemove', handleDrag)\n  document.removeEventListener('mouseup', stopDrag)\n})\n</script>\n\n<style scoped>\n.file-structure-modal {\n  position: fixed;\n  top: 50px;\n  left: 300px;\n  width: 400px;\n  max-height: 600px;\n  background: rgba(255, 255, 255, 0.95);\n  backdrop-filter: blur(20px);\n  border-radius: 12px;\n  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.15);\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  z-index: 10000;\n  overflow: hidden;\n  cursor: move;\n}\n\n[data-theme=\"dark\"] .file-structure-modal {\n  background: rgba(17, 24, 39, 0.95);\n  border: 1px solid rgba(55, 65, 81, 0.4);\n}\n\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 12px 16px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.08);\n  background: rgba(255, 255, 255, 0.5);\n}\n\n[data-theme=\"dark\"] .modal-header {\n  border-bottom: 1px solid rgba(255, 255, 255, 0.08);\n  background: rgba(17, 24, 39, 0.5);\n}\n\n.modal-title {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--text-color);\n}\n\n.file-icon {\n  font-size: 16px;\n}\n\n.file-name {\n  max-width: 200px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.column-count {\n  font-size: 12px;\n  color: var(--text-secondary);\n  background: rgba(59, 130, 246, 0.1);\n  padding: 2px 6px;\n  border-radius: 4px;\n}\n\n.close-btn {\n  background: none;\n  border: none;\n  font-size: 20px;\n  cursor: pointer;\n  color: var(--text-secondary);\n  padding: 4px;\n  border-radius: 4px;\n  transition: all 0.2s;\n}\n\n.close-btn:hover {\n  background: rgba(239, 68, 68, 0.1);\n  color: #ef4444;\n}\n\n.modal-content {\n  padding: 16px;\n  overflow-y: auto;\n  max-height: 520px;\n  cursor: default;\n}\n\n.structure-info {\n  margin-bottom: 16px;\n  padding: 12px;\n  background: rgba(0, 0, 0, 0.02);\n  border-radius: 8px;\n}\n\n[data-theme=\"dark\"] .structure-info {\n  background: rgba(255, 255, 255, 0.02);\n}\n\n.info-row {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 4px;\n  font-size: 12px;\n}\n\n.info-label {\n  color: var(--text-secondary);\n}\n\n.info-value {\n  color: var(--text-color);\n  font-weight: 500;\n}\n\n.section-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 12px;\n}\n\n.section-header h4 {\n  margin: 0;\n  font-size: 14px;\n  color: var(--text-color);\n}\n\n.view-toggle {\n  display: flex;\n  gap: 4px;\n}\n\n.view-toggle button {\n  background: none;\n  border: 1px solid rgba(0, 0, 0, 0.1);\n  padding: 4px;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-secondary);\n  transition: all 0.2s;\n}\n\n.view-toggle button:hover,\n.view-toggle button.active {\n  background: rgba(59, 130, 246, 0.1);\n  border-color: #3b82f6;\n  color: #3b82f6;\n}\n\n[data-theme=\"dark\"] .view-toggle button {\n  border: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n.columns-container {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.columns-container.grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));\n  gap: 8px;\n}\n\n.column-item {\n  background: rgba(255, 255, 255, 0.7);\n  border: 1px solid rgba(0, 0, 0, 0.08);\n  border-radius: 8px;\n  padding: 12px;\n  cursor: grab;\n  transition: all 0.2s;\n}\n\n.column-item:active {\n  cursor: grabbing;\n}\n\n.column-item.dragging {\n  opacity: 0.5;\n  transform: scale(0.95);\n}\n\n.column-item:hover {\n  background: rgba(255, 255, 255, 0.9);\n  border-color: #3b82f6;\n  box-shadow: 0 2px 8px rgba(59, 130, 246, 0.1);\n}\n\n[data-theme=\"dark\"] .column-item {\n  background: rgba(17, 24, 39, 0.7);\n  border: 1px solid rgba(255, 255, 255, 0.08);\n}\n\n[data-theme=\"dark\"] .column-item:hover {\n  background: rgba(17, 24, 39, 0.9);\n  border-color: #60a5fa;\n  box-shadow: 0 2px 8px rgba(96, 165, 250, 0.1);\n}\n\n.column-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 8px;\n}\n\n.column-name {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-color);\n}\n\n.drag-handle {\n  font-size: 10px;\n  color: var(--text-secondary);\n  transform: rotate(90deg);\n  cursor: grab;\n}\n\n.column-type {\n  font-size: 11px;\n  padding: 2px 6px;\n  border-radius: 4px;\n  font-weight: 500;\n}\n\n.co