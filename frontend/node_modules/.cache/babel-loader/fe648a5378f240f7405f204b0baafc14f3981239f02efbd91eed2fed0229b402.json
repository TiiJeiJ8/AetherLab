ck","title","_toDisplayString"],"sources":["D:\\Learning Material\\Git\\Fuck-Charts\\newArchitecture\\frontend_vue_design\\src\\views\\ChartVisualizations.vue"],"sourcesContent":["<template>\n<div class=\"py-5\">\n  <TopBar :actions=\"[\n    { type: 'button', label: 'Files', onClick: UploadFiles },\n    { type: 'button', label: 'Data Preview', onClick: DataPreview},\n    { type: 'button', label: 'Chart History', onClick: openHistory},\n    { type: 'button', label: 'Dashboard', },\n    { type: 'button', label: 'Instruction', },\n    { type: 'button', label: 'Back2Home', to: '/'},\n  ]\"/>\n</div>\n\n<!-- å·¦ä¾§è¾¹æ  -->\n<SideBar\nposition=\"left\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"180\">\n  <ChartTypeSelector @select=\"onChartTypeSelect\" />\n</SideBar>\n\n<!-- å³ä¾§è¾¹æ  -->\n<SideBar\nposition=\"right\"\n:collapsedWidth=\"40\"\n:expandedWidth=\"400\">\n  <DataMappingPanel \n    :selected-chart-type=\"selectedChartType\"\n    @chart-generated=\"handleChartGenerated\"\n    @preview-chart=\"handlePreviewChart\"\n  />\n</SideBar>\n\n<!-- ç»˜åˆ¶åŒº -->\n<div class=\"main-content\">\n  <section class=\"chart-workspace\">\n    <ChartDisplay v-if=\"chartOption\" :option=\"chartOption\" />\n  </section>\n</div>\n\n<!-- å›¾è¡¨å­˜å‚¨åŒº -->\n<ChartHistoryModal\n  :show=\"showHistory\"\n  :chartHistory=\"chartHistory\"\n  @close=\"showHistory = false\"\n  @preview=\"previewChart\"\n  @delete=\"deleteChart\"\n/>\n\n<!-- æ–‡ä»¶ä¸Šä¼ å¼¹çª— -->\n<FileUploadModal\n  v-if=\"showFileUpload\"\n  :show=\"showFileUpload\"\n  :workspaceFiles=\"workspaceFiles\"\n  @close=\"showFileUpload = false\"\n  @workspace-updated=\"handleWorkspaceUpdate\"\n/>\n\n<!-- æ•°æ®é¢„è§ˆå¼¹çª— -->\n<DataPreviewModal\n  v-if=\"showDataPreview\"\n  :show=\"showDataPreview\"\n  :current-file=\"currentDataFile\"\n  :preview-data=\"previewData\"\n  @close=\"showDataPreview = false\"\n/>\n\n<!-- æ–‡ä»¶å·¥ä½œåŒº -->\n<FileWorkspace\n  :files=\"workspaceFiles\"\n  @remove=\"handleWorkspaceRemove\"\n  @preview=\"handleWorkspacePreview\"\n  @clear=\"handleWorkspaceClear\"\n/>\n\n<!-- æ–‡ä»¶ç»“æž„é¢æ¿ -->\n<FileStructurePanel\n  :show=\"showFileStructure\"\n  :files=\"workspaceFiles\"\n  :initial-position=\"{ x: 300, y: 200 }\"\n  @close=\"showFileStructure = false\"\n  @file-change=\"handleFileStructureChange\"\n/>\n\n<!-- å·¥ä½œåŒºæ–‡ä»¶ä¿¡æ¯æŒ‰é’® -->\n<div v-if=\"workspaceFiles.length > 0\" class=\"workspace-info-btn\">\n  <button \n    class=\"info-btn\"\n    @click=\"showFileStructure = true\"\n    title=\"Show file structure\">\n    <span class=\"info-icon\">ðŸ“Š</span>\n    <span>{{ workspaceFiles.length }}</span>\n  </button>\n</div>\n</template>\n\n<script setup>\n/* eslint-disable */\nimport TopBar from '../components/TopBar.vue'\nimport SideBar from '../components/SideBar.vue'\nimport ChartTypeSelector from '../components/chartIcons.vue'\nimport ChartDisplay from '../components/ChartDisplay.vue'\nimport ChartHistoryModal from '../components/ChartHistoryModal.vue'\nimport FileUploadModal from '../components/FileUploadModal.vue'\nimport DataPreviewModal from '../components/DataPreviewModal.vue'\nimport FileWorkspace from '../components/FileWorkspace.vue'\nimport FileStructurePanel from '../components/FileStructurePanel.vue'\nimport DataMappingPanel from '../components/DataMappingPanel.vue'\nimport { getFilePreview } from '../services/FileServices.js'\nimport { globalDataMapper } from '../utils/dataMappingUtils.js'\nimport {ref, nextTick} from 'vue'\n\n// æ–‡ä»¶ä¸Šä¼ ç›¸å…³\nconst showFileUpload = ref(false)\nconst showFileStructure = ref(false)\nconst selectedChartType = ref('bar')\n\nfunction UploadFiles() {\n  showFileUpload.value = true\n}\n\n// æ•°æ®é¢„è§ˆç›¸å…³\nconst showDataPreview = ref(false)\nconst currentDataFile = ref(null)\nconst previewData = ref([])\n\n// å·¥ä½œåŒºç›¸å…³\nconst workspaceFiles = ref([])\n\nfunction DataPreview() {\n  // è¿™é‡Œå¯ä»¥æ ¹æ®å½“å‰å›¾è¡¨èŽ·å–å¯¹åº”çš„æ•°æ®æ–‡ä»¶ä¿¡æ¯\n  currentDataFile.value = {\n    name: 'currentFile.csv'\n  }\n  // è¿™é‡Œå¯ä»¥èŽ·å–å½“å‰å›¾è¡¨çš„æ•°æ®ç”¨äºŽé¢„è§ˆ\n  previewData.value = [\n    { column1: 'A', column2: 100 },\n    { column1: 'B', column2: 200 },\n    { column1: 'C', column2: 300 }\n  ]\n  showDataPreview.value = true\n}\n\n// æµ‹è¯•å›¾è¡¨ç»˜åˆ¶\nconst chartOption = ref({\n  title: { text: 'ç¤ºä¾‹å›¾è¡¨' },\n  tooltip: {},\n  xAxis: { data: ['A', 'B', 'C', 'D'] },\n  yAxis: {},\n  series: [{ type: 'line', data: [23, 45, 12, 37] }]\n})\n\n// å›¾è¡¨å­˜å‚¨åŒºï¼ˆå¯ä»ŽåŽç«¯èŽ·å–ï¼‰\nconst chartHistory = ref([\n  {\n    title: 'é”€å”®æŸ±çŠ¶å›¾',\n    option: {\n      title: { text: 'é”€å”®æŸ±çŠ¶å›¾' },\n      tooltip: {},\n      xAxis: { data: ['ä¸€å­£åº¦', 'äºŒå­£åº¦', 'ä¸‰å­£åº¦', 'å››å­£åº¦'] },\n      yAxis: {},\n      series: [{ type: 'bar', data: [120, 200, 150, 80] }]\n    }\n  },\n  {\n    title: 'æŠ˜çº¿è¶‹åŠ¿',\n    option: {\n      title: { text: 'æŠ˜çº¿è¶‹åŠ¿' },\n      tooltip: {},\n      xAxis: { data: ['1æœˆ', '2æœˆ', '3æœˆ', '4æœˆ'] },\n      yAxis: {},\n      series: [{ type: 'line', data: [30, 50, 80, 65] }]\n    }\n  },\n  {\n    title: 'é¥¼å›¾ç¤ºä¾‹',\n    option: {\n      title: { text: 'é¥¼å›¾ç¤ºä¾‹' },\n      tooltip: {},\n      series: [\n        {\n          type: 'pie',\n          radius: '60%',\n          data: [\n            { value: 40, name: 'Aç±»' },\n            { value: 32, name: 'Bç±»' },\n            { value: 28, name: 'Cç±»' }\n          ]\n        }\n      ]\n    }\n  }\n])\n\nconst showHistory = ref(false)\nfunction openHistory() {\n  showHistory.value = true\n}\nfunction previewChart(item) {\n  chartOption.value = null\n  nextTick(() => {\n    chartOption.value = JSON.parse(JSON.stringify(item.option))\n    showHistory.value = false\n  })\n}\nfunction deleteChart(idx) {\n  chartHistory.value.splice(idx, 1)\n}\n\n// å›¾è¡¨é€‰æ‹©\nfunction onChartTypeSelect(type) {\n    console.log(`Selected chart type: ${type}`)\n    selectedChartType.value = type\n    // å¯æ·»åŠ å¤„ç†ä¸ºåŠ è½½å›¾è¡¨ç±»åž‹ã€åˆ‡æ¢ç»„ä»¶ç­‰åŠŸèƒ½\n}\n\n// å·¥ä½œåŒºç›¸å…³æ–¹æ³•\nfunction handleWorkspaceUpdate(files) {\n    workspaceFiles.value = [...files]\n    console.log('Workspace updated:', workspaceFiles.value.length, 'files')\n}\n\nfunction handleWorkspaceRemove(file) {\n    console.log('File removed from workspace:', file.name)\n    // ä»Žå·¥ä½œåŒºæ•°ç»„ä¸­ç§»é™¤æ–‡ä»¶\n    const index = workspaceFiles.value.findIndex(f => f.id === file.id)\n    if (index !== -1) {\n        workspaceFiles.value.splice(index, 1)\n    }\n}\n\nfunction handleWorkspacePreview(file) {\n    // é¢„è§ˆå·¥ä½œåŒºä¸­çš„æ–‡ä»¶\n    console.log('Preview workspace file:', file.name)\n    currentDataFile.value = file\n    \n    // ä½¿ç”¨ä¸Ž FileUploadModal ç›¸åŒçš„é¢„è§ˆé€»è¾‘\n    if (file.previewData) {\n        // å¦‚æžœæ–‡ä»¶å·²ç»æœ‰é¢„è§ˆæ•°æ®ï¼Œç›´æŽ¥ä½¿ç”¨\n        previewData.value = file.previewData\n        showDataPreview.value = true\n    } else {\n        // å¦åˆ™è¯»å–æ–‡ä»¶å†…å®¹\n        loadFilePreview(file)\n    }\n}\n\n// åŠ è½½æ–‡ä»¶é¢„è§ˆæ•°æ®\nasync function loadFilePreview(file) {\n    try {\n        const previewResult = await getFilePreview(file.id)\n        previewData.value = previewResult.data\n        showDataPreview.value = true\n    } catch (error) {\n        console.error('Preview failed:', error)\n        alert('Preview failed: ' + error.message)\n    }\n}\n\nfunction handleWorkspaceClear() {\n    console.log('Workspace cleared')\n    workspaceFiles.value = []\n}\n\n// æ–‡ä»¶ç»“æž„é¢æ¿ç›¸å…³\nfunction handleFileStructureChange(file) {\n    console.log('File structure changed:', file.name)\n    // å¤„ç†æ–‡ä»¶ç»“æž„å˜åŒ–\n}\n\n// å›¾è¡¨ç”Ÿæˆç›¸å…³\nfunction handleChartGenerated(chartData) {\n    console.log('Chart generated:', chartData)\n    \n    // æ›´æ–°å›¾è¡¨æ˜¾ç¤º\n    chartOption.value = null\n    nextTick(() => {\n        chartOption.value = chartData.option\n    })\n    \n    // ä¿å­˜åˆ°åŽ†å²è®°å½•\n    const historyItem = {\n        title: `${selectedChartType.value} Chart`,\n        option: chartData.option,\n        config: chartData.config,\n        timestamp: chartData.timestamp\n    }\n    chartHistory.value.unshift(historyItem)\n    \n    // é™åˆ¶åŽ†å²è®°å½•æ•°é‡\n    if (chartHistory.value.length > 50) {\n        chartHistory.value = chartHistory.value.slice(0, 50)\n    }\n}\n\nfunction handlePreviewChart(chartOption) {\n    console.log('Preview chart:', chartOption)\n    \n    // æ›´æ–°å›¾è¡¨æ˜¾ç¤º\n    chartOption.value = null\n    nextTick(() => {\n        chartOption.value = chartOption\n    })\n}\n</script>\n\n<style scoped>\n.sideBar-btn {\nbackground: transparent;\nborder: none;\nfont-size: 1.1rem;\nfont-weight: 600;\ncursor: pointer;\ncolor: var(--text-color);\npadding: 6px 14px;\nborder-radius: 8px;\ntransition:\n    background-color 0.3s cubic-bezier(.4,2,.6,1),\n    transform 0.2s cubic-bezier(.4,2,.6,1),\n    box-shadow 0.2s;\ntext-align: center;\ntext-decoration: none;\n}\n.sideBar-btn:hover {\nbackground-color: rgba(136, 133, 133, 0.1);\ntransform: translateY(-2px) scale(1.02);\nbox-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n/* å›¾è¡¨ç»˜åˆ¶åŒº */\n.main-content {\ndisplay: flex;\ngap: 32px;\nmargin: 40px 60px 0 220px;\nmin-height: 500px;\n}\n@media (max-width: 767px) {\n  .main-content {\n    margin: 20px 30px 0 80px;\n    gap: 16px;\n    min-height: auto;\n  }\n}\n.chart-workspace {\nflex: 1;\nmin-width: 0;\ndisplay: flex;\nalign-items: center;\njustify-content: center;\n}\n\n/* å›¾è¡¨ç¼“å­˜åŒº */\n.chart-history {\nwidth: 220px;\nbackground: var(--bg-color, #fff);\nborder-radius: 10px;\nbox-shadow: 0 0px 1px var(--text-color);\npadding: 16px;\nflex-shrink: 0;\n}\n.chart-history-item {\npadding: 8px 0;\ncursor: pointer;\nborder-bottom: 1px solid #747272;\ntransition: background 0.2s;\n}\n.chart-history-item:hover {\nbackground: rgba(136, 133, 133, 0.1);\n}\n</style>\n"],"mappings":";;EACKA,KAAK,EAAC;AAAM;;EAgCZA,KAAK,EAAC;AAAc;;EACdA,KAAK,EAAC;AAAiB;;EAlClCC,GAAA;EAoFsCD,KAAK,EAAC;;;uBApF5CE,mBAAA,CAAAC,SAAA,SACAC,mBAAA,CASM,OATNC,UASM,GA