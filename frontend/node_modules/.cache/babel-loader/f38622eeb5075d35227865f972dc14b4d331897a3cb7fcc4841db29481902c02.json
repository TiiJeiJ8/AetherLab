{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted, computed } from 'vue';\nimport { watchEffect, onBeforeUnmount, nextTick } from 'vue';\nexport default {\n  __name: 'ChartBasicConfig',\n  props: {\n    modelValue: {\n      type: Object,\n      required: true\n    },\n    showNullHandling: {\n      type: Boolean,\n      default: true\n    },\n    // ‰º†ÂÖ•ÂΩìÂâçÁî®‰∫éÁªòÂõæÁöÑÊï∞ÊçÆ\n    chartData: {\n      type: Array,\n      default: () => []\n    }\n  },\n  emits: ['update:modelValue'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n\n    /* eslint-disable */\n    // This component is for basic configuration shared by all chart types\n    // ËØ•ÁªÑ‰ª∂Áî®‰∫éÊâÄÊúâÂõæË°®ÁöÑÈÄöÁî®Âü∫Á°ÄÈÖçÁΩÆ\n    const props = __props;\n    const emit = __emit;\n\n    // Use a computed property for two-way binding with parent\n    // Êú¨Âú∞ÂâØÊú¨ÔºåÈÅøÂÖçÁõ¥Êé•‰øÆÊîπÁà∂ÁªÑ‰ª∂Êï∞ÊçÆ\n    const localConfig = computed({\n      get: () => props.modelValue,\n      set: val => emit('update:modelValue', val)\n    });\n\n    // ‰∏ªÈ¢òÂàóË°®ÔºàÂèØÊâãÂä®Áª¥Êä§ÊàñÈÄöËøáÊé•Âè£/fetchËé∑ÂèñÔºâ\n    const themeList = ref(['default']);\n\n    // ÁõëÂê¨htmlÁöÑdata-themeÂèòÂåñÔºåËá™Âä®ÂêåÊ≠•colorScheme\n    function syncThemeSelectWithHtml() {\n      const htmlTheme = document.documentElement.getAttribute('data-theme');\n      if (htmlTheme === 'dark' && localConfig.value.colorScheme?.toLowerCase() !== 'dark') {\n        if (themeList.value.map(t => t.toLowerCase()).includes('dark')) {\n          localConfig.value.colorScheme = 'dark';\n        }\n      } else if (htmlTheme === 'light' && localConfig.value.colorScheme?.toLowerCase() === 'dark') {\n        localConfig.value.colorScheme = 'default';\n      }\n    }\n    const themeObserver = new MutationObserver(() => {\n      syncThemeSelectWithHtml();\n    });\n    onMounted(() => {\n      syncThemeSelectWithHtml();\n      themeObserver.observe(document.documentElement, {\n        attributes: true,\n        attributeFilter: ['data-theme']\n      });\n    });\n    onBeforeUnmount(() => {\n      themeObserver.disconnect();\n    });\n    watchEffect(() => {\n      syncThemeSelectWithHtml();\n    });\n    onMounted(async () => {\n      // ÂÖàÂ∞ùËØïÂêéÁ´ØÊé•Âè£\n      try {\n        const res = await fetch('/api/themes');\n        if (res.ok) {\n          const arr = await res.json();\n          themeList.value = arr;\n          return;\n        }\n      } catch (e) {}\n      // ÂêéÁ´ØÊé•Âè£‰∏çÂèØÁî®Êó∂ÔºåÈôçÁ∫ß‰∏∫ÈùôÊÄÅ themes.json\n      try {\n        const res = await fetch('/themes/themes.json');\n        if (res.ok) {\n          const arr = await res.json();\n          themeList.value = arr;\n        }\n      } catch (e) {}\n    });\n\n    // ÊéßÂà∂legend ‰ΩçÁΩÆ\n    const legendPositions = [{\n      value: 'bottom',\n      label: 'Bottom'\n    }, {\n      value: 'top',\n      label: 'Top'\n    }, {\n      value: 'left',\n      label: 'Left'\n    }, {\n      value: 'right',\n      label: 'Right'\n    }];\n\n    // legendPosition ÈªòËÆ§ÂÄº bottom\n    onMounted(() => {\n      if (localConfig.value) {\n        if (localConfig.value.legendVisible === undefined) {\n          localConfig.value.legendVisible = true;\n        }\n        if (localConfig.value.legendVisible === null) {\n          localConfig.value.legendVisible = true;\n        }\n        if (!localConfig.value.legendPosition) {\n          localConfig.value.legendPosition = 'bottom';\n        }\n      }\n    });\n    const showThemeTip = ref(false);\n    const __returned__ = {\n      props,\n      emit,\n      localConfig,\n      themeList,\n      syncThemeSelectWithHtml,\n      themeObserver,\n      legendPositions,\n      showThemeTip,\n      ref,\n      onMounted,\n      computed,\n      watchEffect,\n      onBeforeUnmount,\n      nextTick\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n   